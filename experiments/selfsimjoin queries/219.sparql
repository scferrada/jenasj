PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?u1 ?f1 ?n1 ?j1 ?e1 WHERE {
	?id1 wdt:P2146 ?x1 ;
		wdt:P1457 ?u1 ;
		wdt:P2213 ?f1 ;
		wdt:P2045 ?n1 ;
		wdt:P1096 ?j1 ;
		wdt:P2325 ?e1 .
}
SIMILARITY JOIN ON (?x1 ?u1 ?f1 ?n1 ?j1 ?e1) (?x2 ?u2 ?f2 ?n2 ?j2 ?e2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?u2 ?f2 ?n2 ?j2 ?e2 WHERE {
	?id2 wdt:P2146 ?x2 ;
		wdt:P1457 ?u2 ;
		wdt:P2213 ?f2 ;
		wdt:P2045 ?n2 ;
		wdt:P1096 ?j2 ;
		wdt:P2325 ?e2 .
}
