PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?a1 ?o1 ?j1 ?m1 ?t1 WHERE {
	?id1 wdt:P4501 ?a1 ;
		wdt:P1096 ?o1 ;
		wdt:P2248 ?j1 ;
		wdt:P2243 ?m1 ;
		wdt:P2325 ?t1 .
}
SIMILARITY JOIN ON (?a1 ?o1 ?j1 ?m1 ?t1) (?a2 ?o2 ?j2 ?m2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?a2 ?o2 ?j2 ?m2 ?t2 WHERE {
	?id2 wdt:P4501 ?a2 ;
		wdt:P1096 ?o2 ;
		wdt:P2248 ?j2 ;
		wdt:P2243 ?m2 ;
		wdt:P2325 ?t2 .
}
