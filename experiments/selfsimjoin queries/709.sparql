PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?b1 ?y1 ?o1 ?h1 ?n1 ?p1 WHERE {
	?id1 wdt:P2146 ?b1 ;
		wdt:P4501 ?y1 ;
		wdt:P2243 ?o1 ;
		wdt:P2213 ?h1 ;
		wdt:P1096 ?n1 ;
		wdt:P2248 ?p1 .
}
SIMILARITY JOIN ON (?b1 ?y1 ?o1 ?h1 ?n1 ?p1) (?b2 ?y2 ?o2 ?h2 ?n2 ?p2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?b2 ?y2 ?o2 ?h2 ?n2 ?p2 WHERE {
	?id2 wdt:P2146 ?b2 ;
		wdt:P4501 ?y2 ;
		wdt:P2243 ?o2 ;
		wdt:P2213 ?h2 ;
		wdt:P1096 ?n2 ;
		wdt:P2248 ?p2 .
}
