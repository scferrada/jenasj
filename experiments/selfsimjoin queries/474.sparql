PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?k1 ?s1 ?f1 ?a1 ?z1 WHERE {
	?id1 wdt:P2146 ?l1 ;
		wdt:P2248 ?k1 ;
		wdt:P2244 ?s1 ;
		wdt:P2213 ?f1 ;
		wdt:P1096 ?a1 ;
		wdt:P2233 ?z1 .
}
SIMILARITY JOIN ON (?l1 ?k1 ?s1 ?f1 ?a1 ?z1) (?l2 ?k2 ?s2 ?f2 ?a2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?k2 ?s2 ?f2 ?a2 ?z2 WHERE {
	?id2 wdt:P2146 ?l2 ;
		wdt:P2248 ?k2 ;
		wdt:P2244 ?s2 ;
		wdt:P2213 ?f2 ;
		wdt:P1096 ?a2 ;
		wdt:P2233 ?z2 .
}
