PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?z1 ?c1 ?p1 ?y1 WHERE {
	?id1 wdt:P2146 ?m1 ;
		wdt:P2244 ?z1 ;
		wdt:P2045 ?c1 ;
		wdt:P2248 ?p1 ;
		wdt:P2243 ?y1 .
}
SIMILARITY JOIN ON (?m1 ?z1 ?c1 ?p1 ?y1) (?m2 ?z2 ?c2 ?p2 ?y2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?z2 ?c2 ?p2 ?y2 WHERE {
	?id2 wdt:P2146 ?m2 ;
		wdt:P2244 ?z2 ;
		wdt:P2045 ?c2 ;
		wdt:P2248 ?p2 ;
		wdt:P2243 ?y2 .
}
