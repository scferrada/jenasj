PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?m1 ?i1 ?d1 WHERE {
	?id1 wdt:P2045 ?s1 ;
		wdt:P1096 ?m1 ;
		wdt:P4501 ?i1 ;
		wdt:P2243 ?d1 .
}
SIMILARITY JOIN ON (?s1 ?m1 ?i1 ?d1) (?s2 ?m2 ?i2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?m2 ?i2 ?d2 WHERE {
	?id2 wdt:P2045 ?s2 ;
		wdt:P1096 ?m2 ;
		wdt:P4501 ?i2 ;
		wdt:P2243 ?d2 .
}
