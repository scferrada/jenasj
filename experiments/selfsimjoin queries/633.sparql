PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?a1 ?p1 ?q1 ?y1 WHERE {
	?id1 wdt:P1086 ?a1 ;
		wdt:P2154 ?p1 ;
		wdt:P1148 ?q1 ;
		wdt:P2114 ?y1 .
}
SIMILARITY JOIN ON (?a1 ?p1 ?q1 ?y1) (?a2 ?p2 ?q2 ?y2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?a2 ?p2 ?q2 ?y2 WHERE {
	?id2 wdt:P1086 ?a2 ;
		wdt:P2154 ?p2 ;
		wdt:P1148 ?q2 ;
		wdt:P2114 ?y2 .
}
