PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?e1 ?b1 ?r1 ?v1 WHERE {
	?id1 wdt:P4501 ?i1 ;
		wdt:P1096 ?e1 ;
		wdt:P2233 ?b1 ;
		wdt:P2248 ?r1 ;
		wdt:P2325 ?v1 .
}
SIMILARITY JOIN ON (?i1 ?e1 ?b1 ?r1 ?v1) (?i2 ?e2 ?b2 ?r2 ?v2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?e2 ?b2 ?r2 ?v2 WHERE {
	?id2 wdt:P4501 ?i2 ;
		wdt:P1096 ?e2 ;
		wdt:P2233 ?b2 ;
		wdt:P2248 ?r2 ;
		wdt:P2325 ?v2 .
}
