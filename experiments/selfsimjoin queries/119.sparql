PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?s1 ?z1 ?v1 ?x1 WHERE {
	?id1 wdt:P2244 ?i1 ;
		wdt:P2045 ?s1 ;
		wdt:P2248 ?z1 ;
		wdt:P2243 ?v1 ;
		wdt:P4501 ?x1 .
}
SIMILARITY JOIN ON (?i1 ?s1 ?z1 ?v1 ?x1) (?i2 ?s2 ?z2 ?v2 ?x2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?s2 ?z2 ?v2 ?x2 WHERE {
	?id2 wdt:P2244 ?i2 ;
		wdt:P2045 ?s2 ;
		wdt:P2248 ?z2 ;
		wdt:P2243 ?v2 ;
		wdt:P4501 ?x2 .
}
