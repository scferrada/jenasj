PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?k1 ?p1 ?o1 ?l1 WHERE {
	?id1 wdt:P2213 ?k1 ;
		wdt:P1457 ?p1 ;
		wdt:P4501 ?o1 ;
		wdt:P2248 ?l1 .
}
SIMILARITY JOIN ON (?k1 ?p1 ?o1 ?l1) (?k2 ?p2 ?o2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?k2 ?p2 ?o2 ?l2 WHERE {
	?id2 wdt:P2213 ?k2 ;
		wdt:P1457 ?p2 ;
		wdt:P4501 ?o2 ;
		wdt:P2248 ?l2 .
}
