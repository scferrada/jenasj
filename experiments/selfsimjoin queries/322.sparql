PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?p1 ?g1 ?d1 ?n1 WHERE {
	?id1 wdt:P2244 ?h1 ;
		wdt:P2045 ?p1 ;
		wdt:P1096 ?g1 ;
		wdt:P4501 ?d1 ;
		wdt:P2325 ?n1 .
}
SIMILARITY JOIN ON (?h1 ?p1 ?g1 ?d1 ?n1) (?h2 ?p2 ?g2 ?d2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?p2 ?g2 ?d2 ?n2 WHERE {
	?id2 wdt:P2244 ?h2 ;
		wdt:P2045 ?p2 ;
		wdt:P1096 ?g2 ;
		wdt:P4501 ?d2 ;
		wdt:P2325 ?n2 .
}
