PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?u1 ?k1 ?s1 WHERE {
	?id1 wdt:P2146 ?m1 ;
		wdt:P2213 ?u1 ;
		wdt:P1457 ?k1 ;
		wdt:P2243 ?s1 .
}
SIMILARITY JOIN ON (?m1 ?u1 ?k1 ?s1) (?m2 ?u2 ?k2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?u2 ?k2 ?s2 WHERE {
	?id2 wdt:P2146 ?m2 ;
		wdt:P2213 ?u2 ;
		wdt:P1457 ?k2 ;
		wdt:P2243 ?s2 .
}
