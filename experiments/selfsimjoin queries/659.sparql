PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?c1 ?t1 WHERE {
	?id1 wdt:P2146 ?y1 ;
		wdt:P2213 ?c1 ;
		wdt:P2045 ?t1 .
}
SIMILARITY JOIN ON (?y1 ?c1 ?t1) (?y2 ?c2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?c2 ?t2 WHERE {
	?id2 wdt:P2146 ?y2 ;
		wdt:P2213 ?c2 ;
		wdt:P2045 ?t2 .
}
