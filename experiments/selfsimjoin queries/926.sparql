PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?v1 ?b1 ?u1 ?e1 WHERE {
	?id1 wdt:P2146 ?i1 ;
		wdt:P2244 ?v1 ;
		wdt:P1457 ?b1 ;
		wdt:P1096 ?u1 ;
		wdt:P2213 ?e1 .
}
SIMILARITY JOIN ON (?i1 ?v1 ?b1 ?u1 ?e1) (?i2 ?v2 ?b2 ?u2 ?e2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?v2 ?b2 ?u2 ?e2 WHERE {
	?id2 wdt:P2146 ?i2 ;
		wdt:P2244 ?v2 ;
		wdt:P1457 ?b2 ;
		wdt:P1096 ?u2 ;
		wdt:P2213 ?e2 .
}
