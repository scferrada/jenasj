PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?o1 ?g1 ?l1 ?m1 WHERE {
	?id1 wdt:P2244 ?o1 ;
		wdt:P2045 ?g1 ;
		wdt:P2248 ?l1 ;
		wdt:P2325 ?m1 .
}
SIMILARITY JOIN ON (?o1 ?g1 ?l1 ?m1) (?o2 ?g2 ?l2 ?m2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?o2 ?g2 ?l2 ?m2 WHERE {
	?id2 wdt:P2244 ?o2 ;
		wdt:P2045 ?g2 ;
		wdt:P2248 ?l2 ;
		wdt:P2325 ?m2 .
}
