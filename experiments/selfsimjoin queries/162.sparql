PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?q1 ?e1 ?r1 ?b1 WHERE {
	?id1 wdt:P2146 ?p1 ;
		wdt:P2244 ?q1 ;
		wdt:P2213 ?e1 ;
		wdt:P2248 ?r1 ;
		wdt:P2325 ?b1 .
}
SIMILARITY JOIN ON (?p1 ?q1 ?e1 ?r1 ?b1) (?p2 ?q2 ?e2 ?r2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?q2 ?e2 ?r2 ?b2 WHERE {
	?id2 wdt:P2146 ?p2 ;
		wdt:P2244 ?q2 ;
		wdt:P2213 ?e2 ;
		wdt:P2248 ?r2 ;
		wdt:P2325 ?b2 .
}
