PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?h1 ?j1 ?k1 ?x1 ?b1 WHERE {
	?id1 wdt:P1457 ?v1 ;
		wdt:P2248 ?h1 ;
		wdt:P2244 ?j1 ;
		wdt:P2243 ?k1 ;
		wdt:P2045 ?x1 ;
		wdt:P4501 ?b1 .
}
SIMILARITY JOIN ON (?v1 ?h1 ?j1 ?k1 ?x1 ?b1) (?v2 ?h2 ?j2 ?k2 ?x2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?h2 ?j2 ?k2 ?x2 ?b2 WHERE {
	?id2 wdt:P1457 ?v2 ;
		wdt:P2248 ?h2 ;
		wdt:P2244 ?j2 ;
		wdt:P2243 ?k2 ;
		wdt:P2045 ?x2 ;
		wdt:P4501 ?b2 .
}
