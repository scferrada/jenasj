PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?s1 ?e1 ?w1 ?j1 ?t1 WHERE {
	?id1 wdt:P2146 ?q1 ;
		wdt:P2248 ?s1 ;
		wdt:P2213 ?e1 ;
		wdt:P2045 ?w1 ;
		wdt:P2325 ?j1 ;
		wdt:P2233 ?t1 .
}
SIMILARITY JOIN ON (?q1 ?s1 ?e1 ?w1 ?j1 ?t1) (?q2 ?s2 ?e2 ?w2 ?j2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?s2 ?e2 ?w2 ?j2 ?t2 WHERE {
	?id2 wdt:P2146 ?q2 ;
		wdt:P2248 ?s2 ;
		wdt:P2213 ?e2 ;
		wdt:P2045 ?w2 ;
		wdt:P2325 ?j2 ;
		wdt:P2233 ?t2 .
}
