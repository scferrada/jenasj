PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?o1 ?i1 ?p1 ?n1 WHERE {
	?id1 wdt:P6343 ?o1 ;
		wdt:P6344 ?i1 ;
		wdt:P1538 ?p1 ;
		wdt:P1539 ?n1 .
}
SIMILARITY JOIN ON (?o1 ?i1 ?p1 ?n1) (?o2 ?i2 ?p2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?o2 ?i2 ?p2 ?n2 WHERE {
	?id2 wdt:P6343 ?o2 ;
		wdt:P6344 ?i2 ;
		wdt:P1538 ?p2 ;
		wdt:P1539 ?n2 .
}
