PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?z1 ?d1 ?p1 ?b1 ?w1 WHERE {
	?id1 wdt:P2146 ?z1 ;
		wdt:P2213 ?d1 ;
		wdt:P1096 ?p1 ;
		wdt:P2233 ?b1 ;
		wdt:P2243 ?w1 .
}
SIMILARITY JOIN ON (?z1 ?d1 ?p1 ?b1 ?w1) (?z2 ?d2 ?p2 ?b2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?z2 ?d2 ?p2 ?b2 ?w2 WHERE {
	?id2 wdt:P2146 ?z2 ;
		wdt:P2213 ?d2 ;
		wdt:P1096 ?p2 ;
		wdt:P2233 ?b2 ;
		wdt:P2243 ?w2 .
}
