PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?g1 ?i1 ?h1 ?d1 ?r1 ?b1 WHERE {
	?id1 wdt:P2248 ?g1 ;
		wdt:P2244 ?i1 ;
		wdt:P2243 ?h1 ;
		wdt:P2213 ?d1 ;
		wdt:P2325 ?r1 ;
		wdt:P2233 ?b1 .
}
SIMILARITY JOIN ON (?g1 ?i1 ?h1 ?d1 ?r1 ?b1) (?g2 ?i2 ?h2 ?d2 ?r2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?g2 ?i2 ?h2 ?d2 ?r2 ?b2 WHERE {
	?id2 wdt:P2248 ?g2 ;
		wdt:P2244 ?i2 ;
		wdt:P2243 ?h2 ;
		wdt:P2213 ?d2 ;
		wdt:P2325 ?r2 ;
		wdt:P2233 ?b2 .
}
