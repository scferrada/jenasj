PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?z1 ?g1 ?e1 WHERE {
	?id1 wdt:P2146 ?p1 ;
		wdt:P2233 ?z1 ;
		wdt:P2243 ?g1 ;
		wdt:P2325 ?e1 .
}
SIMILARITY JOIN ON (?p1 ?z1 ?g1 ?e1) (?p2 ?z2 ?g2 ?e2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?z2 ?g2 ?e2 WHERE {
	?id2 wdt:P2146 ?p2 ;
		wdt:P2233 ?z2 ;
		wdt:P2243 ?g2 ;
		wdt:P2325 ?e2 .
}
