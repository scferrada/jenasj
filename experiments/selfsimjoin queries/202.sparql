PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?a1 ?d1 ?u1 ?w1 WHERE {
	?id1 wdt:P2244 ?l1 ;
		wdt:P2045 ?a1 ;
		wdt:P2233 ?d1 ;
		wdt:P2248 ?u1 ;
		wdt:P2325 ?w1 .
}
SIMILARITY JOIN ON (?l1 ?a1 ?d1 ?u1 ?w1) (?l2 ?a2 ?d2 ?u2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?a2 ?d2 ?u2 ?w2 WHERE {
	?id2 wdt:P2244 ?l2 ;
		wdt:P2045 ?a2 ;
		wdt:P2233 ?d2 ;
		wdt:P2248 ?u2 ;
		wdt:P2325 ?w2 .
}
