PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?q1 ?l1 ?b1 ?g1 WHERE {
	?id1 wdt:P2213 ?s1 ;
		wdt:P1457 ?q1 ;
		wdt:P2233 ?l1 ;
		wdt:P4501 ?b1 ;
		wdt:P2248 ?g1 .
}
SIMILARITY JOIN ON (?s1 ?q1 ?l1 ?b1 ?g1) (?s2 ?q2 ?l2 ?b2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?q2 ?l2 ?b2 ?g2 WHERE {
	?id2 wdt:P2213 ?s2 ;
		wdt:P1457 ?q2 ;
		wdt:P2233 ?l2 ;
		wdt:P4501 ?b2 ;
		wdt:P2248 ?g2 .
}
