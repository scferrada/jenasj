PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?g1 ?j1 ?r1 ?x1 WHERE {
	?id1 wdt:P2146 ?q1 ;
		wdt:P2244 ?g1 ;
		wdt:P1457 ?j1 ;
		wdt:P2045 ?r1 ;
		wdt:P2243 ?x1 .
}
SIMILARITY JOIN ON (?q1 ?g1 ?j1 ?r1 ?x1) (?q2 ?g2 ?j2 ?r2 ?x2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?g2 ?j2 ?r2 ?x2 WHERE {
	?id2 wdt:P2146 ?q2 ;
		wdt:P2244 ?g2 ;
		wdt:P1457 ?j2 ;
		wdt:P2045 ?r2 ;
		wdt:P2243 ?x2 .
}
