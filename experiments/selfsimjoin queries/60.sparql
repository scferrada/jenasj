PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?f1 ?t1 WHERE {
	?id1 wdt:P2146 ?e1 ;
		wdt:P1457 ?f1 ;
		wdt:P2325 ?t1 .
}
SIMILARITY JOIN ON (?e1 ?f1 ?t1) (?e2 ?f2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?f2 ?t2 WHERE {
	?id2 wdt:P2146 ?e2 ;
		wdt:P1457 ?f2 ;
		wdt:P2325 ?t2 .
}
