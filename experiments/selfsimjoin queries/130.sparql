PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?w1 ?z1 ?q1 ?o1 WHERE {
	?id1 wdt:P2244 ?w1 ;
		wdt:P1457 ?z1 ;
		wdt:P4501 ?q1 ;
		wdt:P2325 ?o1 .
}
SIMILARITY JOIN ON (?w1 ?z1 ?q1 ?o1) (?w2 ?z2 ?q2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?w2 ?z2 ?q2 ?o2 WHERE {
	?id2 wdt:P2244 ?w2 ;
		wdt:P1457 ?z2 ;
		wdt:P4501 ?q2 ;
		wdt:P2325 ?o2 .
}
