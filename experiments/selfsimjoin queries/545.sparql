PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?j1 ?k1 ?y1 ?a1 WHERE {
	?id1 wdt:P2146 ?r1 ;
		wdt:P2244 ?j1 ;
		wdt:P1457 ?k1 ;
		wdt:P2233 ?y1 ;
		wdt:P2248 ?a1 .
}
SIMILARITY JOIN ON (?r1 ?j1 ?k1 ?y1 ?a1) (?r2 ?j2 ?k2 ?y2 ?a2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?j2 ?k2 ?y2 ?a2 WHERE {
	?id2 wdt:P2146 ?r2 ;
		wdt:P2244 ?j2 ;
		wdt:P1457 ?k2 ;
		wdt:P2233 ?y2 ;
		wdt:P2248 ?a2 .
}
