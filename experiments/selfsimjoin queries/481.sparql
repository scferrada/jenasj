PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?e1 ?a1 ?p1 ?z1 WHERE {
	?id1 wdt:P2244 ?m1 ;
		wdt:P1096 ?e1 ;
		wdt:P2233 ?a1 ;
		wdt:P4501 ?p1 ;
		wdt:P2325 ?z1 .
}
SIMILARITY JOIN ON (?m1 ?e1 ?a1 ?p1 ?z1) (?m2 ?e2 ?a2 ?p2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?e2 ?a2 ?p2 ?z2 WHERE {
	?id2 wdt:P2244 ?m2 ;
		wdt:P1096 ?e2 ;
		wdt:P2233 ?a2 ;
		wdt:P4501 ?p2 ;
		wdt:P2325 ?z2 .
}
