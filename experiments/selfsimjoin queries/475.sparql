PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?w1 ?q1 ?x1 ?p1 WHERE {
	?id1 wdt:P2146 ?r1 ;
		wdt:P1457 ?w1 ;
		wdt:P1096 ?q1 ;
		wdt:P2233 ?x1 ;
		wdt:P2248 ?p1 .
}
SIMILARITY JOIN ON (?r1 ?w1 ?q1 ?x1 ?p1) (?r2 ?w2 ?q2 ?x2 ?p2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?w2 ?q2 ?x2 ?p2 WHERE {
	?id2 wdt:P2146 ?r2 ;
		wdt:P1457 ?w2 ;
		wdt:P1096 ?q2 ;
		wdt:P2233 ?x2 ;
		wdt:P2248 ?p2 .
}
