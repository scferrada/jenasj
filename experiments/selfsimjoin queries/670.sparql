PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?x1 ?r1 ?e1 ?o1 ?s1 WHERE {
	?id1 wdt:P2146 ?t1 ;
		wdt:P4501 ?x1 ;
		wdt:P2244 ?r1 ;
		wdt:P2243 ?e1 ;
		wdt:P2045 ?o1 ;
		wdt:P2325 ?s1 .
}
SIMILARITY JOIN ON (?t1 ?x1 ?r1 ?e1 ?o1 ?s1) (?t2 ?x2 ?r2 ?e2 ?o2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?x2 ?r2 ?e2 ?o2 ?s2 WHERE {
	?id2 wdt:P2146 ?t2 ;
		wdt:P4501 ?x2 ;
		wdt:P2244 ?r2 ;
		wdt:P2243 ?e2 ;
		wdt:P2045 ?o2 ;
		wdt:P2325 ?s2 .
}
