PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?r1 ?e1 ?q1 WHERE {
	?id1 wdt:P1457 ?t1 ;
		wdt:P2233 ?r1 ;
		wdt:P4501 ?e1 ;
		wdt:P2243 ?q1 .
}
SIMILARITY JOIN ON (?t1 ?r1 ?e1 ?q1) (?t2 ?r2 ?e2 ?q2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?r2 ?e2 ?q2 WHERE {
	?id2 wdt:P1457 ?t2 ;
		wdt:P2233 ?r2 ;
		wdt:P4501 ?e2 ;
		wdt:P2243 ?q2 .
}
