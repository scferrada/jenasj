PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?v1 ?f1 ?j1 ?n1 WHERE {
	?id1 wdt:P2146 ?d1 ;
		wdt:P2213 ?v1 ;
		wdt:P2045 ?f1 ;
		wdt:P2248 ?j1 ;
		wdt:P4501 ?n1 .
}
SIMILARITY JOIN ON (?d1 ?v1 ?f1 ?j1 ?n1) (?d2 ?v2 ?f2 ?j2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?v2 ?f2 ?j2 ?n2 WHERE {
	?id2 wdt:P2146 ?d2 ;
		wdt:P2213 ?v2 ;
		wdt:P2045 ?f2 ;
		wdt:P2248 ?j2 ;
		wdt:P4501 ?n2 .
}
