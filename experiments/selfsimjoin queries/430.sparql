PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?v1 ?a1 ?l1 WHERE {
	?id1 wdt:P1457 ?d1 ;
		wdt:P1096 ?v1 ;
		wdt:P2233 ?a1 ;
		wdt:P2045 ?l1 .
}
SIMILARITY JOIN ON (?d1 ?v1 ?a1 ?l1) (?d2 ?v2 ?a2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?v2 ?a2 ?l2 WHERE {
	?id2 wdt:P1457 ?d2 ;
		wdt:P1096 ?v2 ;
		wdt:P2233 ?a2 ;
		wdt:P2045 ?l2 .
}
