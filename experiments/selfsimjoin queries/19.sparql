PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?a1 ?m1 ?v1 ?f1 ?i1 WHERE {
	?id1 wdt:P2248 ?a1 ;
		wdt:P2213 ?m1 ;
		wdt:P1457 ?v1 ;
		wdt:P4501 ?f1 ;
		wdt:P2045 ?i1 .
}
SIMILARITY JOIN ON (?a1 ?m1 ?v1 ?f1 ?i1) (?a2 ?m2 ?v2 ?f2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?a2 ?m2 ?v2 ?f2 ?i2 WHERE {
	?id2 wdt:P2248 ?a2 ;
		wdt:P2213 ?m2 ;
		wdt:P1457 ?v2 ;
		wdt:P4501 ?f2 ;
		wdt:P2045 ?i2 .
}
