PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?o1 ?q1 ?r1 ?y1 WHERE {
	?id1 wdt:P2248 ?o1 ;
		wdt:P1457 ?q1 ;
		wdt:P2045 ?r1 ;
		wdt:P2325 ?y1 .
}
SIMILARITY JOIN ON (?o1 ?q1 ?r1 ?y1) (?o2 ?q2 ?r2 ?y2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?o2 ?q2 ?r2 ?y2 WHERE {
	?id2 wdt:P2248 ?o2 ;
		wdt:P1457 ?q2 ;
		wdt:P2045 ?r2 ;
		wdt:P2325 ?y2 .
}
