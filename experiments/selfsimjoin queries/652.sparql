PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?c1 ?v1 ?w1 ?a1 WHERE {
	?id1 wdt:P2146 ?c1 ;
		wdt:P1457 ?v1 ;
		wdt:P2233 ?w1 ;
		wdt:P2045 ?a1 .
}
SIMILARITY JOIN ON (?c1 ?v1 ?w1 ?a1) (?c2 ?v2 ?w2 ?a2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?c2 ?v2 ?w2 ?a2 WHERE {
	?id2 wdt:P2146 ?c2 ;
		wdt:P1457 ?v2 ;
		wdt:P2233 ?w2 ;
		wdt:P2045 ?a2 .
}
