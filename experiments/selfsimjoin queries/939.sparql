PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?a1 ?f1 ?z1 ?h1 WHERE {
	?id1 wdt:P2244 ?a1 ;
		wdt:P1457 ?f1 ;
		wdt:P2233 ?z1 ;
		wdt:P2213 ?h1 .
}
SIMILARITY JOIN ON (?a1 ?f1 ?z1 ?h1) (?a2 ?f2 ?z2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?a2 ?f2 ?z2 ?h2 WHERE {
	?id2 wdt:P2244 ?a2 ;
		wdt:P1457 ?f2 ;
		wdt:P2233 ?z2 ;
		wdt:P2213 ?h2 .
}
