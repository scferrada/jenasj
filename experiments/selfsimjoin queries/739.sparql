PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?w1 ?o1 ?t1 WHERE {
	?id1 wdt:P2146 ?x1 ;
		wdt:P2045 ?w1 ;
		wdt:P4501 ?o1 ;
		wdt:P2325 ?t1 .
}
SIMILARITY JOIN ON (?x1 ?w1 ?o1 ?t1) (?x2 ?w2 ?o2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?w2 ?o2 ?t2 WHERE {
	?id2 wdt:P2146 ?x2 ;
		wdt:P2045 ?w2 ;
		wdt:P4501 ?o2 ;
		wdt:P2325 ?t2 .
}
