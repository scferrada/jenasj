PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?k1 ?r1 ?f1 WHERE {
	?id1 wdt:P2146 ?k1 ;
		wdt:P2244 ?r1 ;
		wdt:P2325 ?f1 .
}
SIMILARITY JOIN ON (?k1 ?r1 ?f1) (?k2 ?r2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?k2 ?r2 ?f2 WHERE {
	?id2 wdt:P2146 ?k2 ;
		wdt:P2244 ?r2 ;
		wdt:P2325 ?f2 .
}
