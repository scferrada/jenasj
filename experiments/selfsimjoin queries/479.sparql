PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?x1 ?q1 ?t1 ?d1 WHERE {
	?id1 wdt:P4501 ?p1 ;
		wdt:P1096 ?x1 ;
		wdt:P2213 ?q1 ;
		wdt:P2248 ?t1 ;
		wdt:P2325 ?d1 .
}
SIMILARITY JOIN ON (?p1 ?x1 ?q1 ?t1 ?d1) (?p2 ?x2 ?q2 ?t2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?x2 ?q2 ?t2 ?d2 WHERE {
	?id2 wdt:P4501 ?p2 ;
		wdt:P1096 ?x2 ;
		wdt:P2213 ?q2 ;
		wdt:P2248 ?t2 ;
		wdt:P2325 ?d2 .
}
