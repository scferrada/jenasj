PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?d1 ?y1 ?s1 ?j1 WHERE {
	?id1 wdt:P2244 ?q1 ;
		wdt:P1457 ?d1 ;
		wdt:P1096 ?y1 ;
		wdt:P2233 ?s1 ;
		wdt:P2213 ?j1 .
}
SIMILARITY JOIN ON (?q1 ?d1 ?y1 ?s1 ?j1) (?q2 ?d2 ?y2 ?s2 ?j2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?d2 ?y2 ?s2 ?j2 WHERE {
	?id2 wdt:P2244 ?q2 ;
		wdt:P1457 ?d2 ;
		wdt:P1096 ?y2 ;
		wdt:P2233 ?s2 ;
		wdt:P2213 ?j2 .
}
