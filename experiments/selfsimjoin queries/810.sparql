PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?m1 ?x1 ?g1 ?z1 WHERE {
	?id1 wdt:P2248 ?d1 ;
		wdt:P2213 ?m1 ;
		wdt:P1457 ?x1 ;
		wdt:P2045 ?g1 ;
		wdt:P2325 ?z1 .
}
SIMILARITY JOIN ON (?d1 ?m1 ?x1 ?g1 ?z1) (?d2 ?m2 ?x2 ?g2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?m2 ?x2 ?g2 ?z2 WHERE {
	?id2 wdt:P2248 ?d2 ;
		wdt:P2213 ?m2 ;
		wdt:P1457 ?x2 ;
		wdt:P2045 ?g2 ;
		wdt:P2325 ?z2 .
}
