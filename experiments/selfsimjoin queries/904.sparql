PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?h1 ?d1 ?e1 ?o1 ?c1 WHERE {
	?id1 wdt:P2146 ?i1 ;
		wdt:P1457 ?h1 ;
		wdt:P4501 ?d1 ;
		wdt:P2213 ?e1 ;
		wdt:P2045 ?o1 ;
		wdt:P2233 ?c1 .
}
SIMILARITY JOIN ON (?i1 ?h1 ?d1 ?e1 ?o1 ?c1) (?i2 ?h2 ?d2 ?e2 ?o2 ?c2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?h2 ?d2 ?e2 ?o2 ?c2 WHERE {
	?id2 wdt:P2146 ?i2 ;
		wdt:P1457 ?h2 ;
		wdt:P4501 ?d2 ;
		wdt:P2213 ?e2 ;
		wdt:P2045 ?o2 ;
		wdt:P2233 ?c2 .
}
