PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?y1 ?n1 ?x1 ?t1 WHERE {
	?id1 wdt:P2213 ?i1 ;
		wdt:P1457 ?y1 ;
		wdt:P1096 ?n1 ;
		wdt:P2045 ?x1 ;
		wdt:P4501 ?t1 .
}
SIMILARITY JOIN ON (?i1 ?y1 ?n1 ?x1 ?t1) (?i2 ?y2 ?n2 ?x2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?y2 ?n2 ?x2 ?t2 WHERE {
	?id2 wdt:P2213 ?i2 ;
		wdt:P1457 ?y2 ;
		wdt:P1096 ?n2 ;
		wdt:P2045 ?x2 ;
		wdt:P4501 ?t2 .
}
