PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?t1 ?l1 WHERE {
	?id1 wdt:P2146 ?r1 ;
		wdt:P2045 ?t1 ;
		wdt:P1096 ?l1 .
}
SIMILARITY JOIN ON (?r1 ?t1 ?l1) (?r2 ?t2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?t2 ?l2 WHERE {
	?id2 wdt:P2146 ?r2 ;
		wdt:P2045 ?t2 ;
		wdt:P1096 ?l2 .
}
