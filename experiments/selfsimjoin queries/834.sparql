PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?i1 ?s1 ?p1 ?k1 WHERE {
	?id1 wdt:P2146 ?e1 ;
		wdt:P2244 ?i1 ;
		wdt:P2233 ?s1 ;
		wdt:P2248 ?p1 ;
		wdt:P2213 ?k1 .
}
SIMILARITY JOIN ON (?e1 ?i1 ?s1 ?p1 ?k1) (?e2 ?i2 ?s2 ?p2 ?k2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?i2 ?s2 ?p2 ?k2 WHERE {
	?id2 wdt:P2146 ?e2 ;
		wdt:P2244 ?i2 ;
		wdt:P2233 ?s2 ;
		wdt:P2248 ?p2 ;
		wdt:P2213 ?k2 .
}
