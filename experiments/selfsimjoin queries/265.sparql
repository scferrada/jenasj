PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?n1 ?u1 ?p1 ?i1 ?j1 WHERE {
	?id1 wdt:P1457 ?r1 ;
		wdt:P2248 ?n1 ;
		wdt:P2244 ?u1 ;
		wdt:P2045 ?p1 ;
		wdt:P2325 ?i1 ;
		wdt:P4501 ?j1 .
}
SIMILARITY JOIN ON (?r1 ?n1 ?u1 ?p1 ?i1 ?j1) (?r2 ?n2 ?u2 ?p2 ?i2 ?j2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?n2 ?u2 ?p2 ?i2 ?j2 WHERE {
	?id2 wdt:P1457 ?r2 ;
		wdt:P2248 ?n2 ;
		wdt:P2244 ?u2 ;
		wdt:P2045 ?p2 ;
		wdt:P2325 ?i2 ;
		wdt:P4501 ?j2 .
}
