PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?c1 ?b1 ?e1 ?w1 WHERE {
	?id1 wdt:P2154 ?t1 ;
		wdt:P1123 ?c1 ;
		wdt:P1148 ?b1 ;
		wdt:P2160 ?e1 ;
		wdt:P1122 ?w1 .
}
SIMILARITY JOIN ON (?t1 ?c1 ?b1 ?e1 ?w1) (?t2 ?c2 ?b2 ?e2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?c2 ?b2 ?e2 ?w2 WHERE {
	?id2 wdt:P2154 ?t2 ;
		wdt:P1123 ?c2 ;
		wdt:P1148 ?b2 ;
		wdt:P2160 ?e2 ;
		wdt:P1122 ?w2 .
}
