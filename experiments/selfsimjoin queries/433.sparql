PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?n1 ?v1 ?r1 ?d1 ?w1 WHERE {
	?id1 wdt:P2244 ?n1 ;
		wdt:P1096 ?v1 ;
		wdt:P2233 ?r1 ;
		wdt:P2248 ?d1 ;
		wdt:P2325 ?w1 .
}
SIMILARITY JOIN ON (?n1 ?v1 ?r1 ?d1 ?w1) (?n2 ?v2 ?r2 ?d2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?n2 ?v2 ?r2 ?d2 ?w2 WHERE {
	?id2 wdt:P2244 ?n2 ;
		wdt:P1096 ?v2 ;
		wdt:P2233 ?r2 ;
		wdt:P2248 ?d2 ;
		wdt:P2325 ?w2 .
}
