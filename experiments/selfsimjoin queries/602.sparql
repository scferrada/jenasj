PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?v1 ?r1 ?w1 WHERE {
	?id1 wdt:P2233 ?y1 ;
		wdt:P4501 ?v1 ;
		wdt:P2243 ?r1 ;
		wdt:P2325 ?w1 .
}
SIMILARITY JOIN ON (?y1 ?v1 ?r1 ?w1) (?y2 ?v2 ?r2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?v2 ?r2 ?w2 WHERE {
	?id2 wdt:P2233 ?y2 ;
		wdt:P4501 ?v2 ;
		wdt:P2243 ?r2 ;
		wdt:P2325 ?w2 .
}
