PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?b1 ?l1 ?s1 ?q1 ?c1 WHERE {
	?id1 wdt:P4501 ?d1 ;
		wdt:P2243 ?b1 ;
		wdt:P2213 ?l1 ;
		wdt:P2045 ?s1 ;
		wdt:P1096 ?q1 ;
		wdt:P2248 ?c1 .
}
SIMILARITY JOIN ON (?d1 ?b1 ?l1 ?s1 ?q1 ?c1) (?d2 ?b2 ?l2 ?s2 ?q2 ?c2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?b2 ?l2 ?s2 ?q2 ?c2 WHERE {
	?id2 wdt:P4501 ?d2 ;
		wdt:P2243 ?b2 ;
		wdt:P2213 ?l2 ;
		wdt:P2045 ?s2 ;
		wdt:P1096 ?q2 ;
		wdt:P2248 ?c2 .
}
