PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?t1 ?p1 WHERE {
	?id1 wdt:P2244 ?d1 ;
		wdt:P1096 ?t1 ;
		wdt:P2325 ?p1 .
}
SIMILARITY JOIN ON (?d1 ?t1 ?p1) (?d2 ?t2 ?p2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?t2 ?p2 WHERE {
	?id2 wdt:P2244 ?d2 ;
		wdt:P1096 ?t2 ;
		wdt:P2325 ?p2 .
}
