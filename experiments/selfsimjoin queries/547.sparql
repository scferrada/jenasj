PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?q1 ?n1 ?l1 ?k1 WHERE {
	?id1 wdt:P2244 ?m1 ;
		wdt:P1457 ?q1 ;
		wdt:P2045 ?n1 ;
		wdt:P4501 ?l1 ;
		wdt:P2325 ?k1 .
}
SIMILARITY JOIN ON (?m1 ?q1 ?n1 ?l1 ?k1) (?m2 ?q2 ?n2 ?l2 ?k2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?q2 ?n2 ?l2 ?k2 WHERE {
	?id2 wdt:P2244 ?m2 ;
		wdt:P1457 ?q2 ;
		wdt:P2045 ?n2 ;
		wdt:P4501 ?l2 ;
		wdt:P2325 ?k2 .
}
