PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?s1 ?r1 ?n1 ?p1 ?x1 WHERE {
	?id1 wdt:P2146 ?l1 ;
		wdt:P1457 ?s1 ;
		wdt:P4501 ?r1 ;
		wdt:P2243 ?n1 ;
		wdt:P2045 ?p1 ;
		wdt:P2325 ?x1 .
}
SIMILARITY JOIN ON (?l1 ?s1 ?r1 ?n1 ?p1 ?x1) (?l2 ?s2 ?r2 ?n2 ?p2 ?x2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?s2 ?r2 ?n2 ?p2 ?x2 WHERE {
	?id2 wdt:P2146 ?l2 ;
		wdt:P1457 ?s2 ;
		wdt:P4501 ?r2 ;
		wdt:P2243 ?n2 ;
		wdt:P2045 ?p2 ;
		wdt:P2325 ?x2 .
}
