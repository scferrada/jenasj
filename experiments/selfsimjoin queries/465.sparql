PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?f1 ?t1 ?v1 ?c1 ?w1 WHERE {
	?id1 wdt:P2244 ?f1 ;
		wdt:P1457 ?t1 ;
		wdt:P1096 ?v1 ;
		wdt:P2045 ?c1 ;
		wdt:P2325 ?w1 .
}
SIMILARITY JOIN ON (?f1 ?t1 ?v1 ?c1 ?w1) (?f2 ?t2 ?v2 ?c2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?f2 ?t2 ?v2 ?c2 ?w2 WHERE {
	?id2 wdt:P2244 ?f2 ;
		wdt:P1457 ?t2 ;
		wdt:P1096 ?v2 ;
		wdt:P2045 ?c2 ;
		wdt:P2325 ?w2 .
}
