PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?b1 ?g1 ?n1 ?a1 ?e1 WHERE {
	?id1 wdt:P2146 ?x1 ;
		wdt:P1457 ?b1 ;
		wdt:P2248 ?g1 ;
		wdt:P2244 ?n1 ;
		wdt:P2325 ?a1 ;
		wdt:P2233 ?e1 .
}
SIMILARITY JOIN ON (?x1 ?b1 ?g1 ?n1 ?a1 ?e1) (?x2 ?b2 ?g2 ?n2 ?a2 ?e2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?b2 ?g2 ?n2 ?a2 ?e2 WHERE {
	?id2 wdt:P2146 ?x2 ;
		wdt:P1457 ?b2 ;
		wdt:P2248 ?g2 ;
		wdt:P2244 ?n2 ;
		wdt:P2325 ?a2 ;
		wdt:P2233 ?e2 .
}
