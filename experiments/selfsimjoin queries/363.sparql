PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?f1 ?t1 ?y1 ?b1 ?r1 ?h1 WHERE {
	?id1 wdt:P1457 ?f1 ;
		wdt:P4501 ?t1 ;
		wdt:P2244 ?y1 ;
		wdt:P2045 ?b1 ;
		wdt:P2325 ?r1 ;
		wdt:P2233 ?h1 .
}
SIMILARITY JOIN ON (?f1 ?t1 ?y1 ?b1 ?r1 ?h1) (?f2 ?t2 ?y2 ?b2 ?r2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?f2 ?t2 ?y2 ?b2 ?r2 ?h2 WHERE {
	?id2 wdt:P1457 ?f2 ;
		wdt:P4501 ?t2 ;
		wdt:P2244 ?y2 ;
		wdt:P2045 ?b2 ;
		wdt:P2325 ?r2 ;
		wdt:P2233 ?h2 .
}
