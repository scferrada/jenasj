PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?o1 ?u1 ?b1 ?n1 ?g1 ?h1 WHERE {
	?id1 wdt:P2146 ?o1 ;
		wdt:P2244 ?u1 ;
		wdt:P2243 ?b1 ;
		wdt:P2213 ?n1 ;
		wdt:P2045 ?g1 ;
		wdt:P2325 ?h1 .
}
SIMILARITY JOIN ON (?o1 ?u1 ?b1 ?n1 ?g1 ?h1) (?o2 ?u2 ?b2 ?n2 ?g2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?o2 ?u2 ?b2 ?n2 ?g2 ?h2 WHERE {
	?id2 wdt:P2146 ?o2 ;
		wdt:P2244 ?u2 ;
		wdt:P2243 ?b2 ;
		wdt:P2213 ?n2 ;
		wdt:P2045 ?g2 ;
		wdt:P2325 ?h2 .
}
