PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?t1 ?j1 ?h1 ?k1 ?m1 WHERE {
	?id1 wdt:P1457 ?p1 ;
		wdt:P4501 ?t1 ;
		wdt:P2244 ?j1 ;
		wdt:P2213 ?h1 ;
		wdt:P1096 ?k1 ;
		wdt:P2325 ?m1 .
}
SIMILARITY JOIN ON (?p1 ?t1 ?j1 ?h1 ?k1 ?m1) (?p2 ?t2 ?j2 ?h2 ?k2 ?m2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?t2 ?j2 ?h2 ?k2 ?m2 WHERE {
	?id2 wdt:P1457 ?p2 ;
		wdt:P4501 ?t2 ;
		wdt:P2244 ?j2 ;
		wdt:P2213 ?h2 ;
		wdt:P1096 ?k2 ;
		wdt:P2325 ?m2 .
}
