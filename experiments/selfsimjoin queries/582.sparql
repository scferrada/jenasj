PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?n1 ?o1 ?r1 ?j1 ?e1 ?d1 WHERE {
	?id1 wdt:P2248 ?n1 ;
		wdt:P2244 ?o1 ;
		wdt:P2243 ?r1 ;
		wdt:P2213 ?j1 ;
		wdt:P2045 ?e1 ;
		wdt:P2233 ?d1 .
}
SIMILARITY JOIN ON (?n1 ?o1 ?r1 ?j1 ?e1 ?d1) (?n2 ?o2 ?r2 ?j2 ?e2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?n2 ?o2 ?r2 ?j2 ?e2 ?d2 WHERE {
	?id2 wdt:P2248 ?n2 ;
		wdt:P2244 ?o2 ;
		wdt:P2243 ?r2 ;
		wdt:P2213 ?j2 ;
		wdt:P2045 ?e2 ;
		wdt:P2233 ?d2 .
}
