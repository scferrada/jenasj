PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?h1 ?i1 ?a1 WHERE {
	?id1 wdt:P2146 ?m1 ;
		wdt:P1096 ?h1 ;
		wdt:P2243 ?i1 ;
		wdt:P2325 ?a1 .
}
SIMILARITY JOIN ON (?m1 ?h1 ?i1 ?a1) (?m2 ?h2 ?i2 ?a2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?h2 ?i2 ?a2 WHERE {
	?id2 wdt:P2146 ?m2 ;
		wdt:P1096 ?h2 ;
		wdt:P2243 ?i2 ;
		wdt:P2325 ?a2 .
}
