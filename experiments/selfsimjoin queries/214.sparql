PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?r1 ?d1 ?i1 ?n1 ?y1 WHERE {
	?id1 wdt:P4501 ?s1 ;
		wdt:P2244 ?r1 ;
		wdt:P2243 ?d1 ;
		wdt:P2213 ?i1 ;
		wdt:P2045 ?n1 ;
		wdt:P1096 ?y1 .
}
SIMILARITY JOIN ON (?s1 ?r1 ?d1 ?i1 ?n1 ?y1) (?s2 ?r2 ?d2 ?i2 ?n2 ?y2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?r2 ?d2 ?i2 ?n2 ?y2 WHERE {
	?id2 wdt:P4501 ?s2 ;
		wdt:P2244 ?r2 ;
		wdt:P2243 ?d2 ;
		wdt:P2213 ?i2 ;
		wdt:P2045 ?n2 ;
		wdt:P1096 ?y2 .
}
