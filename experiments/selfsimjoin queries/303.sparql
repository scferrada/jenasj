PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?a1 ?w1 ?c1 ?k1 ?u1 WHERE {
	?id1 wdt:P2146 ?a1 ;
		wdt:P1457 ?w1 ;
		wdt:P2233 ?c1 ;
		wdt:P2045 ?k1 ;
		wdt:P2248 ?u1 .
}
SIMILARITY JOIN ON (?a1 ?w1 ?c1 ?k1 ?u1) (?a2 ?w2 ?c2 ?k2 ?u2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?a2 ?w2 ?c2 ?k2 ?u2 WHERE {
	?id2 wdt:P2146 ?a2 ;
		wdt:P1457 ?w2 ;
		wdt:P2233 ?c2 ;
		wdt:P2045 ?k2 ;
		wdt:P2248 ?u2 .
}
