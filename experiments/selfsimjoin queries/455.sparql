PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?u1 ?x1 ?i1 ?w1 ?m1 ?f1 WHERE {
	?id1 wdt:P4501 ?u1 ;
		wdt:P2244 ?x1 ;
		wdt:P2243 ?i1 ;
		wdt:P2045 ?w1 ;
		wdt:P2325 ?m1 ;
		wdt:P2233 ?f1 .
}
SIMILARITY JOIN ON (?u1 ?x1 ?i1 ?w1 ?m1 ?f1) (?u2 ?x2 ?i2 ?w2 ?m2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?u2 ?x2 ?i2 ?w2 ?m2 ?f2 WHERE {
	?id2 wdt:P4501 ?u2 ;
		wdt:P2244 ?x2 ;
		wdt:P2243 ?i2 ;
		wdt:P2045 ?w2 ;
		wdt:P2325 ?m2 ;
		wdt:P2233 ?f2 .
}
