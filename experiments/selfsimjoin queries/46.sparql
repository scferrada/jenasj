PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?t1 ?c1 ?n1 ?j1 ?b1 WHERE {
	?id1 wdt:P2146 ?l1 ;
		wdt:P1457 ?t1 ;
		wdt:P2248 ?c1 ;
		wdt:P2244 ?n1 ;
		wdt:P2243 ?j1 ;
		wdt:P4501 ?b1 .
}
SIMILARITY JOIN ON (?l1 ?t1 ?c1 ?n1 ?j1 ?b1) (?l2 ?t2 ?c2 ?n2 ?j2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?t2 ?c2 ?n2 ?j2 ?b2 WHERE {
	?id2 wdt:P2146 ?l2 ;
		wdt:P1457 ?t2 ;
		wdt:P2248 ?c2 ;
		wdt:P2244 ?n2 ;
		wdt:P2243 ?j2 ;
		wdt:P4501 ?b2 .
}
