PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?a1 ?z1 ?b1 ?q1 ?h1 WHERE {
	?id1 wdt:P1457 ?a1 ;
		wdt:P2233 ?z1 ;
		wdt:P2045 ?b1 ;
		wdt:P2243 ?q1 ;
		wdt:P2325 ?h1 .
}
SIMILARITY JOIN ON (?a1 ?z1 ?b1 ?q1 ?h1) (?a2 ?z2 ?b2 ?q2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?a2 ?z2 ?b2 ?q2 ?h2 WHERE {
	?id2 wdt:P1457 ?a2 ;
		wdt:P2233 ?z2 ;
		wdt:P2045 ?b2 ;
		wdt:P2243 ?q2 ;
		wdt:P2325 ?h2 .
}
