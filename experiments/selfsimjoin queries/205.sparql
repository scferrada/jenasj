PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?y1 ?s1 ?l1 WHERE {
	?id1 wdt:P2244 ?v1 ;
		wdt:P4501 ?y1 ;
		wdt:P2243 ?s1 ;
		wdt:P2325 ?l1 .
}
SIMILARITY JOIN ON (?v1 ?y1 ?s1 ?l1) (?v2 ?y2 ?s2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?y2 ?s2 ?l2 WHERE {
	?id2 wdt:P2244 ?v2 ;
		wdt:P4501 ?y2 ;
		wdt:P2243 ?s2 ;
		wdt:P2325 ?l2 .
}
