PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?f1 ?s1 ?m1 ?z1 WHERE {
	?id1 wdt:P2154 ?f1 ;
		wdt:P1122 ?s1 ;
		wdt:P2160 ?m1 ;
		wdt:P2114 ?z1 .
}
SIMILARITY JOIN ON (?f1 ?s1 ?m1 ?z1) (?f2 ?s2 ?m2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?f2 ?s2 ?m2 ?z2 WHERE {
	?id2 wdt:P2154 ?f2 ;
		wdt:P1122 ?s2 ;
		wdt:P2160 ?m2 ;
		wdt:P2114 ?z2 .
}
