PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?e1 ?r1 ?f1 ?d1 WHERE {
	?id1 wdt:P1457 ?i1 ;
		wdt:P1096 ?e1 ;
		wdt:P2233 ?r1 ;
		wdt:P2045 ?f1 ;
		wdt:P4501 ?d1 .
}
SIMILARITY JOIN ON (?i1 ?e1 ?r1 ?f1 ?d1) (?i2 ?e2 ?r2 ?f2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?e2 ?r2 ?f2 ?d2 WHERE {
	?id2 wdt:P1457 ?i2 ;
		wdt:P1096 ?e2 ;
		wdt:P2233 ?r2 ;
		wdt:P2045 ?f2 ;
		wdt:P4501 ?d2 .
}
