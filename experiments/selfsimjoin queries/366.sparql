PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?k1 ?m1 ?e1 ?i1 ?j1 ?c1 WHERE {
	?id1 wdt:P2146 ?k1 ;
		wdt:P1457 ?m1 ;
		wdt:P4501 ?e1 ;
		wdt:P1096 ?i1 ;
		wdt:P2325 ?j1 ;
		wdt:P2233 ?c1 .
}
SIMILARITY JOIN ON (?k1 ?m1 ?e1 ?i1 ?j1 ?c1) (?k2 ?m2 ?e2 ?i2 ?j2 ?c2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?k2 ?m2 ?e2 ?i2 ?j2 ?c2 WHERE {
	?id2 wdt:P2146 ?k2 ;
		wdt:P1457 ?m2 ;
		wdt:P4501 ?e2 ;
		wdt:P1096 ?i2 ;
		wdt:P2325 ?j2 ;
		wdt:P2233 ?c2 .
}
