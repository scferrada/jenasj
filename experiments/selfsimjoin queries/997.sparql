PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?r1 ?f1 ?b1 ?q1 ?u1 WHERE {
	?id1 wdt:P2146 ?p1 ;
		wdt:P1457 ?r1 ;
		wdt:P4501 ?f1 ;
		wdt:P2244 ?b1 ;
		wdt:P1096 ?q1 ;
		wdt:P2233 ?u1 .
}
SIMILARITY JOIN ON (?p1 ?r1 ?f1 ?b1 ?q1 ?u1) (?p2 ?r2 ?f2 ?b2 ?q2 ?u2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?r2 ?f2 ?b2 ?q2 ?u2 WHERE {
	?id2 wdt:P2146 ?p2 ;
		wdt:P1457 ?r2 ;
		wdt:P4501 ?f2 ;
		wdt:P2244 ?b2 ;
		wdt:P1096 ?q2 ;
		wdt:P2233 ?u2 .
}
