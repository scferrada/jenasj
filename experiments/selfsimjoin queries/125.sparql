PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?p1 ?f1 ?v1 ?u1 ?n1 WHERE {
	?id1 wdt:P2146 ?e1 ;
		wdt:P1457 ?p1 ;
		wdt:P2248 ?f1 ;
		wdt:P2213 ?v1 ;
		wdt:P1096 ?u1 ;
		wdt:P2325 ?n1 .
}
SIMILARITY JOIN ON (?e1 ?p1 ?f1 ?v1 ?u1 ?n1) (?e2 ?p2 ?f2 ?v2 ?u2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?p2 ?f2 ?v2 ?u2 ?n2 WHERE {
	?id2 wdt:P2146 ?e2 ;
		wdt:P1457 ?p2 ;
		wdt:P2248 ?f2 ;
		wdt:P2213 ?v2 ;
		wdt:P1096 ?u2 ;
		wdt:P2325 ?n2 .
}
