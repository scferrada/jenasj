PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?z1 ?w1 ?g1 ?d1 ?v1 ?r1 WHERE {
	?id1 wdt:P2146 ?z1 ;
		wdt:P4501 ?w1 ;
		wdt:P2243 ?g1 ;
		wdt:P2213 ?d1 ;
		wdt:P2045 ?v1 ;
		wdt:P2325 ?r1 .
}
SIMILARITY JOIN ON (?z1 ?w1 ?g1 ?d1 ?v1 ?r1) (?z2 ?w2 ?g2 ?d2 ?v2 ?r2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?z2 ?w2 ?g2 ?d2 ?v2 ?r2 WHERE {
	?id2 wdt:P2146 ?z2 ;
		wdt:P4501 ?w2 ;
		wdt:P2243 ?g2 ;
		wdt:P2213 ?d2 ;
		wdt:P2045 ?v2 ;
		wdt:P2325 ?r2 .
}
