PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?q1 ?w1 ?l1 ?k1 ?g1 WHERE {
	?id1 wdt:P2146 ?i1 ;
		wdt:P4501 ?q1 ;
		wdt:P2243 ?w1 ;
		wdt:P2045 ?l1 ;
		wdt:P1096 ?k1 ;
		wdt:P2325 ?g1 .
}
SIMILARITY JOIN ON (?i1 ?q1 ?w1 ?l1 ?k1 ?g1) (?i2 ?q2 ?w2 ?l2 ?k2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?q2 ?w2 ?l2 ?k2 ?g2 WHERE {
	?id2 wdt:P2146 ?i2 ;
		wdt:P4501 ?q2 ;
		wdt:P2243 ?w2 ;
		wdt:P2045 ?l2 ;
		wdt:P1096 ?k2 ;
		wdt:P2325 ?g2 .
}
