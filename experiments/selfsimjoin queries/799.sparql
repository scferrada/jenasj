PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?j1 ?o1 ?p1 ?u1 ?r1 WHERE {
	?id1 wdt:P1457 ?s1 ;
		wdt:P2213 ?j1 ;
		wdt:P2045 ?o1 ;
		wdt:P1096 ?p1 ;
		wdt:P2325 ?u1 ;
		wdt:P2233 ?r1 .
}
SIMILARITY JOIN ON (?s1 ?j1 ?o1 ?p1 ?u1 ?r1) (?s2 ?j2 ?o2 ?p2 ?u2 ?r2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?j2 ?o2 ?p2 ?u2 ?r2 WHERE {
	?id2 wdt:P1457 ?s2 ;
		wdt:P2213 ?j2 ;
		wdt:P2045 ?o2 ;
		wdt:P1096 ?p2 ;
		wdt:P2325 ?u2 ;
		wdt:P2233 ?r2 .
}
