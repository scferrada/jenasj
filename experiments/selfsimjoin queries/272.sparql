PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?g1 ?n1 ?k1 ?f1 WHERE {
	?id1 wdt:P2213 ?g1 ;
		wdt:P2233 ?n1 ;
		wdt:P4501 ?k1 ;
		wdt:P2325 ?f1 .
}
SIMILARITY JOIN ON (?g1 ?n1 ?k1 ?f1) (?g2 ?n2 ?k2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?g2 ?n2 ?k2 ?f2 WHERE {
	?id2 wdt:P2213 ?g2 ;
		wdt:P2233 ?n2 ;
		wdt:P4501 ?k2 ;
		wdt:P2325 ?f2 .
}
