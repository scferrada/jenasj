PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?c1 ?z1 ?l1 ?t1 WHERE {
	?id1 wdt:P2146 ?c1 ;
		wdt:P1457 ?z1 ;
		wdt:P2233 ?l1 ;
		wdt:P2325 ?t1 .
}
SIMILARITY JOIN ON (?c1 ?z1 ?l1 ?t1) (?c2 ?z2 ?l2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?c2 ?z2 ?l2 ?t2 WHERE {
	?id2 wdt:P2146 ?c2 ;
		wdt:P1457 ?z2 ;
		wdt:P2233 ?l2 ;
		wdt:P2325 ?t2 .
}
