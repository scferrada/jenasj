PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?p1 ?w1 ?r1 ?q1 WHERE {
	?id1 wdt:P2244 ?v1 ;
		wdt:P1457 ?p1 ;
		wdt:P2233 ?w1 ;
		wdt:P4501 ?r1 ;
		wdt:P2243 ?q1 .
}
SIMILARITY JOIN ON (?v1 ?p1 ?w1 ?r1 ?q1) (?v2 ?p2 ?w2 ?r2 ?q2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?p2 ?w2 ?r2 ?q2 WHERE {
	?id2 wdt:P2244 ?v2 ;
		wdt:P1457 ?p2 ;
		wdt:P2233 ?w2 ;
		wdt:P4501 ?r2 ;
		wdt:P2243 ?q2 .
}
