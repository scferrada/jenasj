PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?f1 ?e1 ?n1 ?x1 WHERE {
	?id1 wdt:P2045 ?f1 ;
		wdt:P1096 ?e1 ;
		wdt:P2233 ?n1 ;
		wdt:P2325 ?x1 .
}
SIMILARITY JOIN ON (?f1 ?e1 ?n1 ?x1) (?f2 ?e2 ?n2 ?x2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?f2 ?e2 ?n2 ?x2 WHERE {
	?id2 wdt:P2045 ?f2 ;
		wdt:P1096 ?e2 ;
		wdt:P2233 ?n2 ;
		wdt:P2325 ?x2 .
}
