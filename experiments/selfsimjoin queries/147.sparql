PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?w1 ?l1 ?i1 WHERE {
	?id1 wdt:P2146 ?q1 ;
		wdt:P2244 ?w1 ;
		wdt:P1457 ?l1 ;
		wdt:P2045 ?i1 .
}
SIMILARITY JOIN ON (?q1 ?w1 ?l1 ?i1) (?q2 ?w2 ?l2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?w2 ?l2 ?i2 WHERE {
	?id2 wdt:P2146 ?q2 ;
		wdt:P2244 ?w2 ;
		wdt:P1457 ?l2 ;
		wdt:P2045 ?i2 .
}
