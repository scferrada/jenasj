PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?b1 ?o1 ?c1 WHERE {
	?id1 wdt:P2146 ?y1 ;
		wdt:P2213 ?b1 ;
		wdt:P1457 ?o1 ;
		wdt:P2233 ?c1 .
}
SIMILARITY JOIN ON (?y1 ?b1 ?o1 ?c1) (?y2 ?b2 ?o2 ?c2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?b2 ?o2 ?c2 WHERE {
	?id2 wdt:P2146 ?y2 ;
		wdt:P2213 ?b2 ;
		wdt:P1457 ?o2 ;
		wdt:P2233 ?c2 .
}
