PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?b1 ?r1 ?f1 ?d1 ?j1 WHERE {
	?id1 wdt:P1457 ?t1 ;
		wdt:P2248 ?b1 ;
		wdt:P2244 ?r1 ;
		wdt:P2045 ?f1 ;
		wdt:P1096 ?d1 ;
		wdt:P2233 ?j1 .
}
SIMILARITY JOIN ON (?t1 ?b1 ?r1 ?f1 ?d1 ?j1) (?t2 ?b2 ?r2 ?f2 ?d2 ?j2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?b2 ?r2 ?f2 ?d2 ?j2 WHERE {
	?id2 wdt:P1457 ?t2 ;
		wdt:P2248 ?b2 ;
		wdt:P2244 ?r2 ;
		wdt:P2045 ?f2 ;
		wdt:P1096 ?d2 ;
		wdt:P2233 ?j2 .
}
