PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?u1 ?a1 ?h1 ?z1 WHERE {
	?id1 wdt:P2146 ?i1 ;
		wdt:P2045 ?u1 ;
		wdt:P1096 ?a1 ;
		wdt:P2233 ?h1 ;
		wdt:P4501 ?z1 .
}
SIMILARITY JOIN ON (?i1 ?u1 ?a1 ?h1 ?z1) (?i2 ?u2 ?a2 ?h2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?u2 ?a2 ?h2 ?z2 WHERE {
	?id2 wdt:P2146 ?i2 ;
		wdt:P2045 ?u2 ;
		wdt:P1096 ?a2 ;
		wdt:P2233 ?h2 ;
		wdt:P4501 ?z2 .
}
