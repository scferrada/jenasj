PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?s1 ?r1 ?h1 WHERE {
	?id1 wdt:P2146 ?d1 ;
		wdt:P2045 ?s1 ;
		wdt:P4501 ?r1 ;
		wdt:P2243 ?h1 .
}
SIMILARITY JOIN ON (?d1 ?s1 ?r1 ?h1) (?d2 ?s2 ?r2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?s2 ?r2 ?h2 WHERE {
	?id2 wdt:P2146 ?d2 ;
		wdt:P2045 ?s2 ;
		wdt:P4501 ?r2 ;
		wdt:P2243 ?h2 .
}
