PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?n1 ?l1 ?c1 ?a1 ?w1 WHERE {
	?id1 wdt:P2146 ?n1 ;
		wdt:P1457 ?l1 ;
		wdt:P2233 ?c1 ;
		wdt:P2045 ?a1 ;
		wdt:P2325 ?w1 .
}
SIMILARITY JOIN ON (?n1 ?l1 ?c1 ?a1 ?w1) (?n2 ?l2 ?c2 ?a2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?n2 ?l2 ?c2 ?a2 ?w2 WHERE {
	?id2 wdt:P2146 ?n2 ;
		wdt:P1457 ?l2 ;
		wdt:P2233 ?c2 ;
		wdt:P2045 ?a2 ;
		wdt:P2325 ?w2 .
}
