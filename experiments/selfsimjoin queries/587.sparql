PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?u1 ?d1 ?j1 WHERE {
	?id1 wdt:P1457 ?v1 ;
		wdt:P1096 ?u1 ;
		wdt:P2233 ?d1 ;
		wdt:P4501 ?j1 .
}
SIMILARITY JOIN ON (?v1 ?u1 ?d1 ?j1) (?v2 ?u2 ?d2 ?j2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?u2 ?d2 ?j2 WHERE {
	?id2 wdt:P1457 ?v2 ;
		wdt:P1096 ?u2 ;
		wdt:P2233 ?d2 ;
		wdt:P4501 ?j2 .
}
