PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?p1 ?c1 WHERE {
	?id1 wdt:P1086 ?e1 ;
		wdt:P2154 ?p1 ;
		wdt:P2114 ?c1 .
}
SIMILARITY JOIN ON (?e1 ?p1 ?c1) (?e2 ?p2 ?c2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?p2 ?c2 WHERE {
	?id2 wdt:P1086 ?e2 ;
		wdt:P2154 ?p2 ;
		wdt:P2114 ?c2 .
}
