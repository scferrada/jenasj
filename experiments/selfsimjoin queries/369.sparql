PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?y1 ?r1 ?b1 WHERE {
	?id1 wdt:P2244 ?s1 ;
		wdt:P2233 ?y1 ;
		wdt:P2248 ?r1 ;
		wdt:P2325 ?b1 .
}
SIMILARITY JOIN ON (?s1 ?y1 ?r1 ?b1) (?s2 ?y2 ?r2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?y2 ?r2 ?b2 WHERE {
	?id2 wdt:P2244 ?s2 ;
		wdt:P2233 ?y2 ;
		wdt:P2248 ?r2 ;
		wdt:P2325 ?b2 .
}
