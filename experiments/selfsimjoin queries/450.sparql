PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?c1 ?j1 ?e1 ?h1 ?m1 ?s1 WHERE {
	?id1 wdt:P2146 ?c1 ;
		wdt:P1457 ?j1 ;
		wdt:P4501 ?e1 ;
		wdt:P2244 ?h1 ;
		wdt:P2243 ?m1 ;
		wdt:P1096 ?s1 .
}
SIMILARITY JOIN ON (?c1 ?j1 ?e1 ?h1 ?m1 ?s1) (?c2 ?j2 ?e2 ?h2 ?m2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?c2 ?j2 ?e2 ?h2 ?m2 ?s2 WHERE {
	?id2 wdt:P2146 ?c2 ;
		wdt:P1457 ?j2 ;
		wdt:P4501 ?e2 ;
		wdt:P2244 ?h2 ;
		wdt:P2243 ?m2 ;
		wdt:P1096 ?s2 .
}
