PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?f1 ?q1 ?o1 ?m1 ?i1 WHERE {
	?id1 wdt:P2146 ?f1 ;
		wdt:P2213 ?q1 ;
		wdt:P1457 ?o1 ;
		wdt:P4501 ?m1 ;
		wdt:P2248 ?i1 .
}
SIMILARITY JOIN ON (?f1 ?q1 ?o1 ?m1 ?i1) (?f2 ?q2 ?o2 ?m2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?f2 ?q2 ?o2 ?m2 ?i2 WHERE {
	?id2 wdt:P2146 ?f2 ;
		wdt:P2213 ?q2 ;
		wdt:P1457 ?o2 ;
		wdt:P4501 ?m2 ;
		wdt:P2248 ?i2 .
}
