PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?w1 ?r1 ?g1 ?l1 ?m1 WHERE {
	?id1 wdt:P2213 ?w1 ;
		wdt:P1457 ?r1 ;
		wdt:P4501 ?g1 ;
		wdt:P2243 ?l1 ;
		wdt:P2325 ?m1 .
}
SIMILARITY JOIN ON (?w1 ?r1 ?g1 ?l1 ?m1) (?w2 ?r2 ?g2 ?l2 ?m2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?w2 ?r2 ?g2 ?l2 ?m2 WHERE {
	?id2 wdt:P2213 ?w2 ;
		wdt:P1457 ?r2 ;
		wdt:P4501 ?g2 ;
		wdt:P2243 ?l2 ;
		wdt:P2325 ?m2 .
}
