PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?g1 ?u1 ?v1 ?a1 WHERE {
	?id1 wdt:P2146 ?g1 ;
		wdt:P2244 ?u1 ;
		wdt:P2045 ?v1 ;
		wdt:P2243 ?a1 .
}
SIMILARITY JOIN ON (?g1 ?u1 ?v1 ?a1) (?g2 ?u2 ?v2 ?a2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?g2 ?u2 ?v2 ?a2 WHERE {
	?id2 wdt:P2146 ?g2 ;
		wdt:P2244 ?u2 ;
		wdt:P2045 ?v2 ;
		wdt:P2243 ?a2 .
}
