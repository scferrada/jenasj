PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?d1 ?m1 ?j1 WHERE {
	?id1 wdt:P2146 ?x1 ;
		wdt:P2213 ?d1 ;
		wdt:P1096 ?m1 ;
		wdt:P2243 ?j1 .
}
SIMILARITY JOIN ON (?x1 ?d1 ?m1 ?j1) (?x2 ?d2 ?m2 ?j2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?d2 ?m2 ?j2 WHERE {
	?id2 wdt:P2146 ?x2 ;
		wdt:P2213 ?d2 ;
		wdt:P1096 ?m2 ;
		wdt:P2243 ?j2 .
}
