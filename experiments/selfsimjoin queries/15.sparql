PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?d1 ?o1 ?j1 ?v1 WHERE {
	?id1 wdt:P2146 ?s1 ;
		wdt:P2213 ?d1 ;
		wdt:P1457 ?o1 ;
		wdt:P4501 ?j1 ;
		wdt:P2243 ?v1 .
}
SIMILARITY JOIN ON (?s1 ?d1 ?o1 ?j1 ?v1) (?s2 ?d2 ?o2 ?j2 ?v2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?d2 ?o2 ?j2 ?v2 WHERE {
	?id2 wdt:P2146 ?s2 ;
		wdt:P2213 ?d2 ;
		wdt:P1457 ?o2 ;
		wdt:P4501 ?j2 ;
		wdt:P2243 ?v2 .
}
