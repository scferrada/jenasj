PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?a1 ?o1 WHERE {
	?id1 wdt:P1457 ?j1 ;
		wdt:P1096 ?a1 ;
		wdt:P2325 ?o1 .
}
SIMILARITY JOIN ON (?j1 ?a1 ?o1) (?j2 ?a2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?a2 ?o2 WHERE {
	?id2 wdt:P1457 ?j2 ;
		wdt:P1096 ?a2 ;
		wdt:P2325 ?o2 .
}
