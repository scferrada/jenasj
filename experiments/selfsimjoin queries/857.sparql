PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?n1 ?t1 ?a1 ?w1 ?o1 WHERE {
	?id1 wdt:P2248 ?e1 ;
		wdt:P2244 ?n1 ;
		wdt:P2213 ?t1 ;
		wdt:P1096 ?a1 ;
		wdt:P2233 ?w1 ;
		wdt:P4501 ?o1 .
}
SIMILARITY JOIN ON (?e1 ?n1 ?t1 ?a1 ?w1 ?o1) (?e2 ?n2 ?t2 ?a2 ?w2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?n2 ?t2 ?a2 ?w2 ?o2 WHERE {
	?id2 wdt:P2248 ?e2 ;
		wdt:P2244 ?n2 ;
		wdt:P2213 ?t2 ;
		wdt:P1096 ?a2 ;
		wdt:P2233 ?w2 ;
		wdt:P4501 ?o2 .
}
