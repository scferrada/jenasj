PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?h1 ?t1 ?o1 WHERE {
	?id1 wdt:P1086 ?x1 ;
		wdt:P1122 ?h1 ;
		wdt:P2160 ?t1 ;
		wdt:P2114 ?o1 .
}
SIMILARITY JOIN ON (?x1 ?h1 ?t1 ?o1) (?x2 ?h2 ?t2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?h2 ?t2 ?o2 WHERE {
	?id2 wdt:P1086 ?x2 ;
		wdt:P1122 ?h2 ;
		wdt:P2160 ?t2 ;
		wdt:P2114 ?o2 .
}
