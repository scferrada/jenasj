PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?p1 ?w1 WHERE {
	?id1 wdt:P2146 ?h1 ;
		wdt:P2244 ?p1 ;
		wdt:P1096 ?w1 .
}
SIMILARITY JOIN ON (?h1 ?p1 ?w1) (?h2 ?p2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?p2 ?w2 WHERE {
	?id2 wdt:P2146 ?h2 ;
		wdt:P2244 ?p2 ;
		wdt:P1096 ?w2 .
}
