PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?k1 ?y1 ?h1 ?d1 ?g1 WHERE {
	?id1 wdt:P1457 ?l1 ;
		wdt:P2248 ?k1 ;
		wdt:P2244 ?y1 ;
		wdt:P2243 ?h1 ;
		wdt:P2045 ?d1 ;
		wdt:P2325 ?g1 .
}
SIMILARITY JOIN ON (?l1 ?k1 ?y1 ?h1 ?d1 ?g1) (?l2 ?k2 ?y2 ?h2 ?d2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?k2 ?y2 ?h2 ?d2 ?g2 WHERE {
	?id2 wdt:P1457 ?l2 ;
		wdt:P2248 ?k2 ;
		wdt:P2244 ?y2 ;
		wdt:P2243 ?h2 ;
		wdt:P2045 ?d2 ;
		wdt:P2325 ?g2 .
}
