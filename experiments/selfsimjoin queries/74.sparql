PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?i1 ?s1 ?g1 ?q1 WHERE {
	?id1 wdt:P2146 ?r1 ;
		wdt:P2244 ?i1 ;
		wdt:P1096 ?s1 ;
		wdt:P2233 ?g1 ;
		wdt:P2243 ?q1 .
}
SIMILARITY JOIN ON (?r1 ?i1 ?s1 ?g1 ?q1) (?r2 ?i2 ?s2 ?g2 ?q2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?i2 ?s2 ?g2 ?q2 WHERE {
	?id2 wdt:P2146 ?r2 ;
		wdt:P2244 ?i2 ;
		wdt:P1096 ?s2 ;
		wdt:P2233 ?g2 ;
		wdt:P2243 ?q2 .
}
