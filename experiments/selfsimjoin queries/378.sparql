PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?a1 ?o1 ?b1 ?m1 ?g1 WHERE {
	?id1 wdt:P2146 ?r1 ;
		wdt:P4501 ?a1 ;
		wdt:P2243 ?o1 ;
		wdt:P2325 ?b1 ;
		wdt:P2233 ?m1 ;
		wdt:P2248 ?g1 .
}
SIMILARITY JOIN ON (?r1 ?a1 ?o1 ?b1 ?m1 ?g1) (?r2 ?a2 ?o2 ?b2 ?m2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?a2 ?o2 ?b2 ?m2 ?g2 WHERE {
	?id2 wdt:P2146 ?r2 ;
		wdt:P4501 ?a2 ;
		wdt:P2243 ?o2 ;
		wdt:P2325 ?b2 ;
		wdt:P2233 ?m2 ;
		wdt:P2248 ?g2 .
}
