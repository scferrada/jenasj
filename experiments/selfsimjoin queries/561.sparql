PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?g1 ?v1 ?m1 ?f1 WHERE {
	?id1 wdt:P2213 ?g1 ;
		wdt:P2325 ?v1 ;
		wdt:P2248 ?m1 ;
		wdt:P4501 ?f1 .
}
SIMILARITY JOIN ON (?g1 ?v1 ?m1 ?f1) (?g2 ?v2 ?m2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?g2 ?v2 ?m2 ?f2 WHERE {
	?id2 wdt:P2213 ?g2 ;
		wdt:P2325 ?v2 ;
		wdt:P2248 ?m2 ;
		wdt:P4501 ?f2 .
}
