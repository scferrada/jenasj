PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?c1 ?v1 ?h1 ?t1 ?o1 WHERE {
	?id1 wdt:P2146 ?q1 ;
		wdt:P1457 ?c1 ;
		wdt:P2248 ?v1 ;
		wdt:P2213 ?h1 ;
		wdt:P2325 ?t1 ;
		wdt:P4501 ?o1 .
}
SIMILARITY JOIN ON (?q1 ?c1 ?v1 ?h1 ?t1 ?o1) (?q2 ?c2 ?v2 ?h2 ?t2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?c2 ?v2 ?h2 ?t2 ?o2 WHERE {
	?id2 wdt:P2146 ?q2 ;
		wdt:P1457 ?c2 ;
		wdt:P2248 ?v2 ;
		wdt:P2213 ?h2 ;
		wdt:P2325 ?t2 ;
		wdt:P4501 ?o2 .
}
