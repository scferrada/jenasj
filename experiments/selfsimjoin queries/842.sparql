PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?g1 ?p1 ?v1 ?m1 ?t1 ?e1 WHERE {
	?id1 wdt:P1457 ?g1 ;
		wdt:P2248 ?p1 ;
		wdt:P2244 ?v1 ;
		wdt:P2243 ?m1 ;
		wdt:P2213 ?t1 ;
		wdt:P1096 ?e1 .
}
SIMILARITY JOIN ON (?g1 ?p1 ?v1 ?m1 ?t1 ?e1) (?g2 ?p2 ?v2 ?m2 ?t2 ?e2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?g2 ?p2 ?v2 ?m2 ?t2 ?e2 WHERE {
	?id2 wdt:P1457 ?g2 ;
		wdt:P2248 ?p2 ;
		wdt:P2244 ?v2 ;
		wdt:P2243 ?m2 ;
		wdt:P2213 ?t2 ;
		wdt:P1096 ?e2 .
}
