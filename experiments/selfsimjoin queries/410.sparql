PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?j1 ?p1 ?f1 ?i1 WHERE {
	?id1 wdt:P2146 ?e1 ;
		wdt:P2244 ?j1 ;
		wdt:P2213 ?p1 ;
		wdt:P2248 ?f1 ;
		wdt:P4501 ?i1 .
}
SIMILARITY JOIN ON (?e1 ?j1 ?p1 ?f1 ?i1) (?e2 ?j2 ?p2 ?f2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?j2 ?p2 ?f2 ?i2 WHERE {
	?id2 wdt:P2146 ?e2 ;
		wdt:P2244 ?j2 ;
		wdt:P2213 ?p2 ;
		wdt:P2248 ?f2 ;
		wdt:P4501 ?i2 .
}
