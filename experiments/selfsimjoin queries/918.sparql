PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?f1 ?l1 ?z1 ?n1 ?t1 WHERE {
	?id1 wdt:P2146 ?f1 ;
		wdt:P2244 ?l1 ;
		wdt:P2045 ?z1 ;
		wdt:P1096 ?n1 ;
		wdt:P2213 ?t1 .
}
SIMILARITY JOIN ON (?f1 ?l1 ?z1 ?n1 ?t1) (?f2 ?l2 ?z2 ?n2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?f2 ?l2 ?z2 ?n2 ?t2 WHERE {
	?id2 wdt:P2146 ?f2 ;
		wdt:P2244 ?l2 ;
		wdt:P2045 ?z2 ;
		wdt:P1096 ?n2 ;
		wdt:P2213 ?t2 .
}
