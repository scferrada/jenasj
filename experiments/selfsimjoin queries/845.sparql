PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?f1 ?v1 ?d1 WHERE {
	?id1 wdt:P2244 ?l1 ;
		wdt:P1096 ?f1 ;
		wdt:P4501 ?v1 ;
		wdt:P2213 ?d1 .
}
SIMILARITY JOIN ON (?l1 ?f1 ?v1 ?d1) (?l2 ?f2 ?v2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?f2 ?v2 ?d2 WHERE {
	?id2 wdt:P2244 ?l2 ;
		wdt:P1096 ?f2 ;
		wdt:P4501 ?v2 ;
		wdt:P2213 ?d2 .
}
