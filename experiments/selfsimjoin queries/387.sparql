PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?m1 ?n1 ?s1 ?g1 WHERE {
	?id1 wdt:P2146 ?t1 ;
		wdt:P2244 ?m1 ;
		wdt:P2248 ?n1 ;
		wdt:P2243 ?s1 ;
		wdt:P2325 ?g1 .
}
SIMILARITY JOIN ON (?t1 ?m1 ?n1 ?s1 ?g1) (?t2 ?m2 ?n2 ?s2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?m2 ?n2 ?s2 ?g2 WHERE {
	?id2 wdt:P2146 ?t2 ;
		wdt:P2244 ?m2 ;
		wdt:P2248 ?n2 ;
		wdt:P2243 ?s2 ;
		wdt:P2325 ?g2 .
}
