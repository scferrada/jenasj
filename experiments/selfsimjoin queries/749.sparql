PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?h1 ?f1 ?v1 ?b1 ?n1 WHERE {
	?id1 wdt:P1457 ?x1 ;
		wdt:P2248 ?h1 ;
		wdt:P2244 ?f1 ;
		wdt:P2243 ?v1 ;
		wdt:P2213 ?b1 ;
		wdt:P4501 ?n1 .
}
SIMILARITY JOIN ON (?x1 ?h1 ?f1 ?v1 ?b1 ?n1) (?x2 ?h2 ?f2 ?v2 ?b2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?h2 ?f2 ?v2 ?b2 ?n2 WHERE {
	?id2 wdt:P1457 ?x2 ;
		wdt:P2248 ?h2 ;
		wdt:P2244 ?f2 ;
		wdt:P2243 ?v2 ;
		wdt:P2213 ?b2 ;
		wdt:P4501 ?n2 .
}
