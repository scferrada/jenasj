PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?r1 ?h1 ?q1 WHERE {
	?id1 wdt:P2045 ?v1 ;
		wdt:P2248 ?r1 ;
		wdt:P2243 ?h1 ;
		wdt:P2325 ?q1 .
}
SIMILARITY JOIN ON (?v1 ?r1 ?h1 ?q1) (?v2 ?r2 ?h2 ?q2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?r2 ?h2 ?q2 WHERE {
	?id2 wdt:P2045 ?v2 ;
		wdt:P2248 ?r2 ;
		wdt:P2243 ?h2 ;
		wdt:P2325 ?q2 .
}
