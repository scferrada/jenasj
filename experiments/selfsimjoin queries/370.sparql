PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?i1 ?x1 ?k1 ?n1 WHERE {
	?id1 wdt:P2244 ?p1 ;
		wdt:P1096 ?i1 ;
		wdt:P2213 ?x1 ;
		wdt:P2248 ?k1 ;
		wdt:P2325 ?n1 .
}
SIMILARITY JOIN ON (?p1 ?i1 ?x1 ?k1 ?n1) (?p2 ?i2 ?x2 ?k2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?i2 ?x2 ?k2 ?n2 WHERE {
	?id2 wdt:P2244 ?p2 ;
		wdt:P1096 ?i2 ;
		wdt:P2213 ?x2 ;
		wdt:P2248 ?k2 ;
		wdt:P2325 ?n2 .
}
