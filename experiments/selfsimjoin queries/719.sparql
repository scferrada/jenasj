PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?z1 ?g1 ?m1 ?v1 ?r1 ?s1 WHERE {
	?id1 wdt:P2146 ?z1 ;
		wdt:P1457 ?g1 ;
		wdt:P2248 ?m1 ;
		wdt:P2243 ?v1 ;
		wdt:P2213 ?r1 ;
		wdt:P2325 ?s1 .
}
SIMILARITY JOIN ON (?z1 ?g1 ?m1 ?v1 ?r1 ?s1) (?z2 ?g2 ?m2 ?v2 ?r2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?z2 ?g2 ?m2 ?v2 ?r2 ?s2 WHERE {
	?id2 wdt:P2146 ?z2 ;
		wdt:P1457 ?g2 ;
		wdt:P2248 ?m2 ;
		wdt:P2243 ?v2 ;
		wdt:P2213 ?r2 ;
		wdt:P2325 ?s2 .
}
