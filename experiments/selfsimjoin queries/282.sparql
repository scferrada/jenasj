PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?u1 ?a1 ?z1 ?f1 WHERE {
	?id1 wdt:P2244 ?h1 ;
		wdt:P1457 ?u1 ;
		wdt:P2213 ?a1 ;
		wdt:P2248 ?z1 ;
		wdt:P2045 ?f1 .
}
SIMILARITY JOIN ON (?h1 ?u1 ?a1 ?z1 ?f1) (?h2 ?u2 ?a2 ?z2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?u2 ?a2 ?z2 ?f2 WHERE {
	?id2 wdt:P2244 ?h2 ;
		wdt:P1457 ?u2 ;
		wdt:P2213 ?a2 ;
		wdt:P2248 ?z2 ;
		wdt:P2045 ?f2 .
}
