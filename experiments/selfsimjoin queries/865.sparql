PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?q1 ?f1 WHERE {
	?id1 wdt:P4511 ?h1 ;
		wdt:P2234 ?q1 ;
		wdt:P2547 ?f1 .
}
SIMILARITY JOIN ON (?h1 ?q1 ?f1) (?h2 ?q2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?q2 ?f2 WHERE {
	?id2 wdt:P4511 ?h2 ;
		wdt:P2234 ?q2 ;
		wdt:P2547 ?f2 .
}
