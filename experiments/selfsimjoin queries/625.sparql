PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?x1 ?r1 ?v1 WHERE {
	?id1 wdt:P2146 ?m1 ;
		wdt:P1096 ?x1 ;
		wdt:P2233 ?r1 ;
		wdt:P4501 ?v1 .
}
SIMILARITY JOIN ON (?m1 ?x1 ?r1 ?v1) (?m2 ?x2 ?r2 ?v2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?x2 ?r2 ?v2 WHERE {
	?id2 wdt:P2146 ?m2 ;
		wdt:P1096 ?x2 ;
		wdt:P2233 ?r2 ;
		wdt:P4501 ?v2 .
}
