PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?j1 ?i1 ?v1 ?f1 ?k1 WHERE {
	?id1 wdt:P1457 ?r1 ;
		wdt:P4501 ?j1 ;
		wdt:P2244 ?i1 ;
		wdt:P2243 ?v1 ;
		wdt:P2213 ?f1 ;
		wdt:P1096 ?k1 .
}
SIMILARITY JOIN ON (?r1 ?j1 ?i1 ?v1 ?f1 ?k1) (?r2 ?j2 ?i2 ?v2 ?f2 ?k2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?j2 ?i2 ?v2 ?f2 ?k2 WHERE {
	?id2 wdt:P1457 ?r2 ;
		wdt:P4501 ?j2 ;
		wdt:P2244 ?i2 ;
		wdt:P2243 ?v2 ;
		wdt:P2213 ?f2 ;
		wdt:P1096 ?k2 .
}
