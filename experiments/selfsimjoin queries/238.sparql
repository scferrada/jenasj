PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?a1 ?h1 ?u1 ?o1 ?v1 WHERE {
	?id1 wdt:P2146 ?j1 ;
		wdt:P4501 ?a1 ;
		wdt:P2244 ?h1 ;
		wdt:P2213 ?u1 ;
		wdt:P2045 ?o1 ;
		wdt:P2248 ?v1 .
}
SIMILARITY JOIN ON (?j1 ?a1 ?h1 ?u1 ?o1 ?v1) (?j2 ?a2 ?h2 ?u2 ?o2 ?v2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?a2 ?h2 ?u2 ?o2 ?v2 WHERE {
	?id2 wdt:P2146 ?j2 ;
		wdt:P4501 ?a2 ;
		wdt:P2244 ?h2 ;
		wdt:P2213 ?u2 ;
		wdt:P2045 ?o2 ;
		wdt:P2248 ?v2 .
}
