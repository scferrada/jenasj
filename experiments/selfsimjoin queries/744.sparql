PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?f1 ?l1 ?c1 ?q1 ?o1 WHERE {
	?id1 wdt:P2146 ?r1 ;
		wdt:P4501 ?f1 ;
		wdt:P2243 ?l1 ;
		wdt:P1096 ?c1 ;
		wdt:P2233 ?q1 ;
		wdt:P2248 ?o1 .
}
SIMILARITY JOIN ON (?r1 ?f1 ?l1 ?c1 ?q1 ?o1) (?r2 ?f2 ?l2 ?c2 ?q2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?f2 ?l2 ?c2 ?q2 ?o2 WHERE {
	?id2 wdt:P2146 ?r2 ;
		wdt:P4501 ?f2 ;
		wdt:P2243 ?l2 ;
		wdt:P1096 ?c2 ;
		wdt:P2233 ?q2 ;
		wdt:P2248 ?o2 .
}
