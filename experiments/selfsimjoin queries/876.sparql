PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?d1 ?w1 ?h1 WHERE {
	?id1 wdt:P2244 ?s1 ;
		wdt:P1096 ?d1 ;
		wdt:P2233 ?w1 ;
		wdt:P2325 ?h1 .
}
SIMILARITY JOIN ON (?s1 ?d1 ?w1 ?h1) (?s2 ?d2 ?w2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?d2 ?w2 ?h2 WHERE {
	?id2 wdt:P2244 ?s2 ;
		wdt:P1096 ?d2 ;
		wdt:P2233 ?w2 ;
		wdt:P2325 ?h2 .
}
