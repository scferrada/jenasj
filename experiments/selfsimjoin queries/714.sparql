PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?c1 ?z1 WHERE {
	?id1 wdt:P2213 ?v1 ;
		wdt:P2045 ?c1 ;
		wdt:P2248 ?z1 .
}
SIMILARITY JOIN ON (?v1 ?c1 ?z1) (?v2 ?c2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?c2 ?z2 WHERE {
	?id2 wdt:P2213 ?v2 ;
		wdt:P2045 ?c2 ;
		wdt:P2248 ?z2 .
}
