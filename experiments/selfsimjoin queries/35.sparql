PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?y1 ?p1 ?n1 WHERE {
	?id1 wdt:P2154 ?d1 ;
		wdt:P1148 ?y1 ;
		wdt:P2160 ?p1 ;
		wdt:P1122 ?n1 .
}
SIMILARITY JOIN ON (?d1 ?y1 ?p1 ?n1) (?d2 ?y2 ?p2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?y2 ?p2 ?n2 WHERE {
	?id2 wdt:P2154 ?d2 ;
		wdt:P1148 ?y2 ;
		wdt:P2160 ?p2 ;
		wdt:P1122 ?n2 .
}
