PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?s1 ?a1 ?q1 ?z1 WHERE {
	?id1 wdt:P2146 ?j1 ;
		wdt:P2213 ?s1 ;
		wdt:P2233 ?a1 ;
		wdt:P4501 ?q1 ;
		wdt:P2325 ?z1 .
}
SIMILARITY JOIN ON (?j1 ?s1 ?a1 ?q1 ?z1) (?j2 ?s2 ?a2 ?q2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?s2 ?a2 ?q2 ?z2 WHERE {
	?id2 wdt:P2146 ?j2 ;
		wdt:P2213 ?s2 ;
		wdt:P2233 ?a2 ;
		wdt:P4501 ?q2 ;
		wdt:P2325 ?z2 .
}
