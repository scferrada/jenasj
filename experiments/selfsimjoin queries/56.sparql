PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?w1 ?r1 ?h1 WHERE {
	?id1 wdt:P1457 ?y1 ;
		wdt:P2233 ?w1 ;
		wdt:P2045 ?r1 ;
		wdt:P2243 ?h1 .
}
SIMILARITY JOIN ON (?y1 ?w1 ?r1 ?h1) (?y2 ?w2 ?r2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?w2 ?r2 ?h2 WHERE {
	?id2 wdt:P1457 ?y2 ;
		wdt:P2233 ?w2 ;
		wdt:P2045 ?r2 ;
		wdt:P2243 ?h2 .
}
