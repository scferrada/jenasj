PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?x1 ?o1 ?m1 ?g1 WHERE {
	?id1 wdt:P2146 ?y1 ;
		wdt:P2213 ?x1 ;
		wdt:P1457 ?o1 ;
		wdt:P2045 ?m1 ;
		wdt:P2243 ?g1 .
}
SIMILARITY JOIN ON (?y1 ?x1 ?o1 ?m1 ?g1) (?y2 ?x2 ?o2 ?m2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?x2 ?o2 ?m2 ?g2 WHERE {
	?id2 wdt:P2146 ?y2 ;
		wdt:P2213 ?x2 ;
		wdt:P1457 ?o2 ;
		wdt:P2045 ?m2 ;
		wdt:P2243 ?g2 .
}
