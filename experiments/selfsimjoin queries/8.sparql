PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?u1 ?c1 ?m1 ?z1 WHERE {
	?id1 wdt:P1457 ?u1 ;
		wdt:P2248 ?c1 ;
		wdt:P2243 ?m1 ;
		wdt:P2325 ?z1 .
}
SIMILARITY JOIN ON (?u1 ?c1 ?m1 ?z1) (?u2 ?c2 ?m2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?u2 ?c2 ?m2 ?z2 WHERE {
	?id2 wdt:P1457 ?u2 ;
		wdt:P2248 ?c2 ;
		wdt:P2243 ?m2 ;
		wdt:P2325 ?z2 .
}
