PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?u1 ?g1 ?b1 ?l1 ?e1 WHERE {
	?id1 wdt:P2213 ?u1 ;
		wdt:P1457 ?g1 ;
		wdt:P2233 ?b1 ;
		wdt:P2045 ?l1 ;
		wdt:P2325 ?e1 .
}
SIMILARITY JOIN ON (?u1 ?g1 ?b1 ?l1 ?e1) (?u2 ?g2 ?b2 ?l2 ?e2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?u2 ?g2 ?b2 ?l2 ?e2 WHERE {
	?id2 wdt:P2213 ?u2 ;
		wdt:P1457 ?g2 ;
		wdt:P2233 ?b2 ;
		wdt:P2045 ?l2 ;
		wdt:P2325 ?e2 .
}
