PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?g1 ?z1 ?i1 ?d1 ?a1 WHERE {
	?id1 wdt:P1457 ?g1 ;
		wdt:P2233 ?z1 ;
		wdt:P2045 ?i1 ;
		wdt:P2243 ?d1 ;
		wdt:P2248 ?a1 .
}
SIMILARITY JOIN ON (?g1 ?z1 ?i1 ?d1 ?a1) (?g2 ?z2 ?i2 ?d2 ?a2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?g2 ?z2 ?i2 ?d2 ?a2 WHERE {
	?id2 wdt:P1457 ?g2 ;
		wdt:P2233 ?z2 ;
		wdt:P2045 ?i2 ;
		wdt:P2243 ?d2 ;
		wdt:P2248 ?a2 .
}
