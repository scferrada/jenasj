PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?n1 ?g1 ?j1 ?k1 ?p1 ?s1 WHERE {
	?id1 wdt:P1457 ?n1 ;
		wdt:P2248 ?g1 ;
		wdt:P1096 ?j1 ;
		wdt:P2325 ?k1 ;
		wdt:P2233 ?p1 ;
		wdt:P4501 ?s1 .
}
SIMILARITY JOIN ON (?n1 ?g1 ?j1 ?k1 ?p1 ?s1) (?n2 ?g2 ?j2 ?k2 ?p2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?n2 ?g2 ?j2 ?k2 ?p2 ?s2 WHERE {
	?id2 wdt:P1457 ?n2 ;
		wdt:P2248 ?g2 ;
		wdt:P1096 ?j2 ;
		wdt:P2325 ?k2 ;
		wdt:P2233 ?p2 ;
		wdt:P4501 ?s2 .
}
