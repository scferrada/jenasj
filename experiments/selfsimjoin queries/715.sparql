PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?m1 ?p1 ?j1 ?b1 ?k1 WHERE {
	?id1 wdt:P2146 ?t1 ;
		wdt:P2248 ?m1 ;
		wdt:P2243 ?p1 ;
		wdt:P2045 ?j1 ;
		wdt:P2325 ?b1 ;
		wdt:P2233 ?k1 .
}
SIMILARITY JOIN ON (?t1 ?m1 ?p1 ?j1 ?b1 ?k1) (?t2 ?m2 ?p2 ?j2 ?b2 ?k2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?m2 ?p2 ?j2 ?b2 ?k2 WHERE {
	?id2 wdt:P2146 ?t2 ;
		wdt:P2248 ?m2 ;
		wdt:P2243 ?p2 ;
		wdt:P2045 ?j2 ;
		wdt:P2325 ?b2 ;
		wdt:P2233 ?k2 .
}
