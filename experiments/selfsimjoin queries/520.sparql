PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?k1 ?n1 ?l1 ?o1 WHERE {
	?id1 wdt:P2146 ?y1 ;
		wdt:P2244 ?k1 ;
		wdt:P2325 ?n1 ;
		wdt:P2248 ?l1 ;
		wdt:P4501 ?o1 .
}
SIMILARITY JOIN ON (?y1 ?k1 ?n1 ?l1 ?o1) (?y2 ?k2 ?n2 ?l2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?k2 ?n2 ?l2 ?o2 WHERE {
	?id2 wdt:P2146 ?y2 ;
		wdt:P2244 ?k2 ;
		wdt:P2325 ?n2 ;
		wdt:P2248 ?l2 ;
		wdt:P4501 ?o2 .
}
