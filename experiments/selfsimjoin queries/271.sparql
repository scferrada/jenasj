PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?w1 ?k1 ?o1 ?i1 WHERE {
	?id1 wdt:P2146 ?w1 ;
		wdt:P2213 ?k1 ;
		wdt:P1096 ?o1 ;
		wdt:P2233 ?i1 .
}
SIMILARITY JOIN ON (?w1 ?k1 ?o1 ?i1) (?w2 ?k2 ?o2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?w2 ?k2 ?o2 ?i2 WHERE {
	?id2 wdt:P2146 ?w2 ;
		wdt:P2213 ?k2 ;
		wdt:P1096 ?o2 ;
		wdt:P2233 ?i2 .
}
