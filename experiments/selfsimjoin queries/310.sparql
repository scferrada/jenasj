PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?n1 ?v1 ?x1 ?h1 ?e1 WHERE {
	?id1 wdt:P2244 ?n1 ;
		wdt:P2045 ?v1 ;
		wdt:P4501 ?x1 ;
		wdt:P2243 ?h1 ;
		wdt:P2213 ?e1 .
}
SIMILARITY JOIN ON (?n1 ?v1 ?x1 ?h1 ?e1) (?n2 ?v2 ?x2 ?h2 ?e2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?n2 ?v2 ?x2 ?h2 ?e2 WHERE {
	?id2 wdt:P2244 ?n2 ;
		wdt:P2045 ?v2 ;
		wdt:P4501 ?x2 ;
		wdt:P2243 ?h2 ;
		wdt:P2213 ?e2 .
}
