PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?u1 ?v1 ?k1 ?m1 ?l1 WHERE {
	?id1 wdt:P2146 ?y1 ;
		wdt:P1457 ?u1 ;
		wdt:P4501 ?v1 ;
		wdt:P2244 ?k1 ;
		wdt:P2243 ?m1 ;
		wdt:P2233 ?l1 .
}
SIMILARITY JOIN ON (?y1 ?u1 ?v1 ?k1 ?m1 ?l1) (?y2 ?u2 ?v2 ?k2 ?m2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?u2 ?v2 ?k2 ?m2 ?l2 WHERE {
	?id2 wdt:P2146 ?y2 ;
		wdt:P1457 ?u2 ;
		wdt:P4501 ?v2 ;
		wdt:P2244 ?k2 ;
		wdt:P2243 ?m2 ;
		wdt:P2233 ?l2 .
}
