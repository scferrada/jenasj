PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?m1 ?f1 ?w1 ?z1 WHERE {
	?id1 wdt:P1457 ?t1 ;
		wdt:P2233 ?m1 ;
		wdt:P4501 ?f1 ;
		wdt:P2243 ?w1 ;
		wdt:P2045 ?z1 .
}
SIMILARITY JOIN ON (?t1 ?m1 ?f1 ?w1 ?z1) (?t2 ?m2 ?f2 ?w2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?m2 ?f2 ?w2 ?z2 WHERE {
	?id2 wdt:P1457 ?t2 ;
		wdt:P2233 ?m2 ;
		wdt:P4501 ?f2 ;
		wdt:P2243 ?w2 ;
		wdt:P2045 ?z2 .
}
