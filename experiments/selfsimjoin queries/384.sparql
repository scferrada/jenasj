PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?p1 ?y1 ?k1 ?j1 ?f1 WHERE {
	?id1 wdt:P4501 ?h1 ;
		wdt:P2244 ?p1 ;
		wdt:P2243 ?y1 ;
		wdt:P2045 ?k1 ;
		wdt:P1096 ?j1 ;
		wdt:P2233 ?f1 .
}
SIMILARITY JOIN ON (?h1 ?p1 ?y1 ?k1 ?j1 ?f1) (?h2 ?p2 ?y2 ?k2 ?j2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?p2 ?y2 ?k2 ?j2 ?f2 WHERE {
	?id2 wdt:P4501 ?h2 ;
		wdt:P2244 ?p2 ;
		wdt:P2243 ?y2 ;
		wdt:P2045 ?k2 ;
		wdt:P1096 ?j2 ;
		wdt:P2233 ?f2 .
}
