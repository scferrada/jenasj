PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?z1 ?a1 WHERE {
	?id1 wdt:P2146 ?q1 ;
		wdt:P2244 ?z1 ;
		wdt:P2248 ?a1 .
}
SIMILARITY JOIN ON (?q1 ?z1 ?a1) (?q2 ?z2 ?a2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?z2 ?a2 WHERE {
	?id2 wdt:P2146 ?q2 ;
		wdt:P2244 ?z2 ;
		wdt:P2248 ?a2 .
}
