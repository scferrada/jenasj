PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?e1 ?m1 ?t1 ?g1 WHERE {
	?id1 wdt:P2244 ?h1 ;
		wdt:P1457 ?e1 ;
		wdt:P2233 ?m1 ;
		wdt:P2243 ?t1 ;
		wdt:P2213 ?g1 .
}
SIMILARITY JOIN ON (?h1 ?e1 ?m1 ?t1 ?g1) (?h2 ?e2 ?m2 ?t2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?e2 ?m2 ?t2 ?g2 WHERE {
	?id2 wdt:P2244 ?h2 ;
		wdt:P1457 ?e2 ;
		wdt:P2233 ?m2 ;
		wdt:P2243 ?t2 ;
		wdt:P2213 ?g2 .
}
