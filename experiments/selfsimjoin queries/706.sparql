PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?b1 ?d1 ?k1 WHERE {
	?id1 wdt:P2213 ?t1 ;
		wdt:P1096 ?b1 ;
		wdt:P4501 ?d1 ;
		wdt:P2325 ?k1 .
}
SIMILARITY JOIN ON (?t1 ?b1 ?d1 ?k1) (?t2 ?b2 ?d2 ?k2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?b2 ?d2 ?k2 WHERE {
	?id2 wdt:P2213 ?t2 ;
		wdt:P1096 ?b2 ;
		wdt:P4501 ?d2 ;
		wdt:P2325 ?k2 .
}
