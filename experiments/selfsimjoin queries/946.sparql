PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?n1 ?r1 ?o1 ?h1 ?e1 WHERE {
	?id1 wdt:P2213 ?n1 ;
		wdt:P1457 ?r1 ;
		wdt:P4501 ?o1 ;
		wdt:P2243 ?h1 ;
		wdt:P2045 ?e1 .
}
SIMILARITY JOIN ON (?n1 ?r1 ?o1 ?h1 ?e1) (?n2 ?r2 ?o2 ?h2 ?e2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?n2 ?r2 ?o2 ?h2 ?e2 WHERE {
	?id2 wdt:P2213 ?n2 ;
		wdt:P1457 ?r2 ;
		wdt:P4501 ?o2 ;
		wdt:P2243 ?h2 ;
		wdt:P2045 ?e2 .
}
