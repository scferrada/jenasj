PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?d1 ?w1 ?i1 WHERE {
	?id1 wdt:P2146 ?s1 ;
		wdt:P2213 ?d1 ;
		wdt:P4501 ?w1 ;
		wdt:P2325 ?i1 .
}
SIMILARITY JOIN ON (?s1 ?d1 ?w1 ?i1) (?s2 ?d2 ?w2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?d2 ?w2 ?i2 WHERE {
	?id2 wdt:P2146 ?s2 ;
		wdt:P2213 ?d2 ;
		wdt:P4501 ?w2 ;
		wdt:P2325 ?i2 .
}
