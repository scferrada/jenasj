PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?k1 ?d1 ?q1 ?z1 WHERE {
	?id1 wdt:P2213 ?e1 ;
		wdt:P1457 ?k1 ;
		wdt:P2248 ?d1 ;
		wdt:P2243 ?q1 ;
		wdt:P2325 ?z1 .
}
SIMILARITY JOIN ON (?e1 ?k1 ?d1 ?q1 ?z1) (?e2 ?k2 ?d2 ?q2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?k2 ?d2 ?q2 ?z2 WHERE {
	?id2 wdt:P2213 ?e2 ;
		wdt:P1457 ?k2 ;
		wdt:P2248 ?d2 ;
		wdt:P2243 ?q2 ;
		wdt:P2325 ?z2 .
}
