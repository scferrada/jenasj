PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?u1 ?b1 ?i1 ?t1 WHERE {
	?id1 wdt:P2146 ?e1 ;
		wdt:P2244 ?u1 ;
		wdt:P1096 ?b1 ;
		wdt:P2243 ?i1 ;
		wdt:P2213 ?t1 .
}
SIMILARITY JOIN ON (?e1 ?u1 ?b1 ?i1 ?t1) (?e2 ?u2 ?b2 ?i2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?u2 ?b2 ?i2 ?t2 WHERE {
	?id2 wdt:P2146 ?e2 ;
		wdt:P2244 ?u2 ;
		wdt:P1096 ?b2 ;
		wdt:P2243 ?i2 ;
		wdt:P2213 ?t2 .
}
