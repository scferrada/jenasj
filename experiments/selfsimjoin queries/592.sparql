PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?o1 ?j1 ?b1 ?w1 ?v1 WHERE {
	?id1 wdt:P2244 ?o1 ;
		wdt:P2045 ?j1 ;
		wdt:P2213 ?b1 ;
		wdt:P2248 ?w1 ;
		wdt:P4501 ?v1 .
}
SIMILARITY JOIN ON (?o1 ?j1 ?b1 ?w1 ?v1) (?o2 ?j2 ?b2 ?w2 ?v2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?o2 ?j2 ?b2 ?w2 ?v2 WHERE {
	?id2 wdt:P2244 ?o2 ;
		wdt:P2045 ?j2 ;
		wdt:P2213 ?b2 ;
		wdt:P2248 ?w2 ;
		wdt:P4501 ?v2 .
}
