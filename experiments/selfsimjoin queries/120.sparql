PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?j1 ?t1 ?c1 ?m1 ?f1 WHERE {
	?id1 wdt:P2146 ?v1 ;
		wdt:P2248 ?j1 ;
		wdt:P2243 ?t1 ;
		wdt:P2045 ?c1 ;
		wdt:P1096 ?m1 ;
		wdt:P2325 ?f1 .
}
SIMILARITY JOIN ON (?v1 ?j1 ?t1 ?c1 ?m1 ?f1) (?v2 ?j2 ?t2 ?c2 ?m2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?j2 ?t2 ?c2 ?m2 ?f2 WHERE {
	?id2 wdt:P2146 ?v2 ;
		wdt:P2248 ?j2 ;
		wdt:P2243 ?t2 ;
		wdt:P2045 ?c2 ;
		wdt:P1096 ?m2 ;
		wdt:P2325 ?f2 .
}
