PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?i1 ?w1 WHERE {
	?id1 wdt:P2154 ?l1 ;
		wdt:P1122 ?i1 ;
		wdt:P2114 ?w1 .
}
SIMILARITY JOIN ON (?l1 ?i1 ?w1) (?l2 ?i2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?i2 ?w2 WHERE {
	?id2 wdt:P2154 ?l2 ;
		wdt:P1122 ?i2 ;
		wdt:P2114 ?w2 .
}
