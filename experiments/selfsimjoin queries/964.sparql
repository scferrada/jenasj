PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?z1 ?p1 ?x1 ?o1 ?u1 ?n1 WHERE {
	?id1 wdt:P1457 ?z1 ;
		wdt:P2248 ?p1 ;
		wdt:P2244 ?x1 ;
		wdt:P1096 ?o1 ;
		wdt:P2325 ?u1 ;
		wdt:P2233 ?n1 .
}
SIMILARITY JOIN ON (?z1 ?p1 ?x1 ?o1 ?u1 ?n1) (?z2 ?p2 ?x2 ?o2 ?u2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?z2 ?p2 ?x2 ?o2 ?u2 ?n2 WHERE {
	?id2 wdt:P1457 ?z2 ;
		wdt:P2248 ?p2 ;
		wdt:P2244 ?x2 ;
		wdt:P1096 ?o2 ;
		wdt:P2325 ?u2 ;
		wdt:P2233 ?n2 .
}
