PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?v1 ?e1 ?z1 ?p1 ?k1 WHERE {
	?id1 wdt:P2146 ?t1 ;
		wdt:P1457 ?v1 ;
		wdt:P2213 ?e1 ;
		wdt:P1096 ?z1 ;
		wdt:P2325 ?p1 ;
		wdt:P2233 ?k1 .
}
SIMILARITY JOIN ON (?t1 ?v1 ?e1 ?z1 ?p1 ?k1) (?t2 ?v2 ?e2 ?z2 ?p2 ?k2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?v2 ?e2 ?z2 ?p2 ?k2 WHERE {
	?id2 wdt:P2146 ?t2 ;
		wdt:P1457 ?v2 ;
		wdt:P2213 ?e2 ;
		wdt:P1096 ?z2 ;
		wdt:P2325 ?p2 ;
		wdt:P2233 ?k2 .
}
