PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?h1 ?x1 ?m1 ?r1 ?p1 WHERE {
	?id1 wdt:P2146 ?y1 ;
		wdt:P4501 ?h1 ;
		wdt:P2244 ?x1 ;
		wdt:P2213 ?m1 ;
		wdt:P2325 ?r1 ;
		wdt:P2233 ?p1 .
}
SIMILARITY JOIN ON (?y1 ?h1 ?x1 ?m1 ?r1 ?p1) (?y2 ?h2 ?x2 ?m2 ?r2 ?p2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?h2 ?x2 ?m2 ?r2 ?p2 WHERE {
	?id2 wdt:P2146 ?y2 ;
		wdt:P4501 ?h2 ;
		wdt:P2244 ?x2 ;
		wdt:P2213 ?m2 ;
		wdt:P2325 ?r2 ;
		wdt:P2233 ?p2 .
}
