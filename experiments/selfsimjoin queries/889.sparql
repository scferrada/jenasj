PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?n1 ?e1 ?v1 ?g1 ?a1 WHERE {
	?id1 wdt:P2244 ?n1 ;
		wdt:P1457 ?e1 ;
		wdt:P4501 ?v1 ;
		wdt:P2243 ?g1 ;
		wdt:P2248 ?a1 .
}
SIMILARITY JOIN ON (?n1 ?e1 ?v1 ?g1 ?a1) (?n2 ?e2 ?v2 ?g2 ?a2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?n2 ?e2 ?v2 ?g2 ?a2 WHERE {
	?id2 wdt:P2244 ?n2 ;
		wdt:P1457 ?e2 ;
		wdt:P4501 ?v2 ;
		wdt:P2243 ?g2 ;
		wdt:P2248 ?a2 .
}
