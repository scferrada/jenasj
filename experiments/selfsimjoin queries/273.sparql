PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?a1 ?s1 ?c1 ?h1 ?n1 WHERE {
	?id1 wdt:P2146 ?a1 ;
		wdt:P2244 ?s1 ;
		wdt:P2045 ?c1 ;
		wdt:P1096 ?h1 ;
		wdt:P2243 ?n1 .
}
SIMILARITY JOIN ON (?a1 ?s1 ?c1 ?h1 ?n1) (?a2 ?s2 ?c2 ?h2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?a2 ?s2 ?c2 ?h2 ?n2 WHERE {
	?id2 wdt:P2146 ?a2 ;
		wdt:P2244 ?s2 ;
		wdt:P2045 ?c2 ;
		wdt:P1096 ?h2 ;
		wdt:P2243 ?n2 .
}
