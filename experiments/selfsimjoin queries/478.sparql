PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?l1 ?r1 ?f1 WHERE {
	?id1 wdt:P2213 ?s1 ;
		wdt:P2233 ?l1 ;
		wdt:P2248 ?r1 ;
		wdt:P4501 ?f1 .
}
SIMILARITY JOIN ON (?s1 ?l1 ?r1 ?f1) (?s2 ?l2 ?r2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?l2 ?r2 ?f2 WHERE {
	?id2 wdt:P2213 ?s2 ;
		wdt:P2233 ?l2 ;
		wdt:P2248 ?r2 ;
		wdt:P4501 ?f2 .
}
