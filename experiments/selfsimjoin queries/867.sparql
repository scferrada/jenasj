PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?g1 ?l1 ?m1 WHERE {
	?id1 wdt:P1457 ?g1 ;
		wdt:P2248 ?l1 ;
		wdt:P2243 ?m1 .
}
SIMILARITY JOIN ON (?g1 ?l1 ?m1) (?g2 ?l2 ?m2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?g2 ?l2 ?m2 WHERE {
	?id2 wdt:P1457 ?g2 ;
		wdt:P2248 ?l2 ;
		wdt:P2243 ?m2 .
}
