PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?q1 ?o1 ?e1 ?m1 ?i1 WHERE {
	?id1 wdt:P2146 ?p1 ;
		wdt:P2248 ?q1 ;
		wdt:P2243 ?o1 ;
		wdt:P2213 ?e1 ;
		wdt:P2045 ?m1 ;
		wdt:P1096 ?i1 .
}
SIMILARITY JOIN ON (?p1 ?q1 ?o1 ?e1 ?m1 ?i1) (?p2 ?q2 ?o2 ?e2 ?m2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?q2 ?o2 ?e2 ?m2 ?i2 WHERE {
	?id2 wdt:P2146 ?p2 ;
		wdt:P2248 ?q2 ;
		wdt:P2243 ?o2 ;
		wdt:P2213 ?e2 ;
		wdt:P2045 ?m2 ;
		wdt:P1096 ?i2 .
}
