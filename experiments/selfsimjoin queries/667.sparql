PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?t1 ?i1 ?w1 ?a1 WHERE {
	?id1 wdt:P2244 ?r1 ;
		wdt:P2233 ?t1 ;
		wdt:P2248 ?i1 ;
		wdt:P2243 ?w1 ;
		wdt:P2213 ?a1 .
}
SIMILARITY JOIN ON (?r1 ?t1 ?i1 ?w1 ?a1) (?r2 ?t2 ?i2 ?w2 ?a2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?t2 ?i2 ?w2 ?a2 WHERE {
	?id2 wdt:P2244 ?r2 ;
		wdt:P2233 ?t2 ;
		wdt:P2248 ?i2 ;
		wdt:P2243 ?w2 ;
		wdt:P2213 ?a2 .
}
