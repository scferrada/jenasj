PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?q1 ?f1 ?x1 ?k1 ?r1 WHERE {
	?id1 wdt:P2146 ?y1 ;
		wdt:P4501 ?q1 ;
		wdt:P2244 ?f1 ;
		wdt:P2243 ?x1 ;
		wdt:P2325 ?k1 ;
		wdt:P2248 ?r1 .
}
SIMILARITY JOIN ON (?y1 ?q1 ?f1 ?x1 ?k1 ?r1) (?y2 ?q2 ?f2 ?x2 ?k2 ?r2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?q2 ?f2 ?x2 ?k2 ?r2 WHERE {
	?id2 wdt:P2146 ?y2 ;
		wdt:P4501 ?q2 ;
		wdt:P2244 ?f2 ;
		wdt:P2243 ?x2 ;
		wdt:P2325 ?k2 ;
		wdt:P2248 ?r2 .
}
