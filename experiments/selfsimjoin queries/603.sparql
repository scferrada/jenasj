PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?a1 ?o1 ?s1 ?t1 ?g1 WHERE {
	?id1 wdt:P1457 ?h1 ;
		wdt:P2248 ?a1 ;
		wdt:P2045 ?o1 ;
		wdt:P1096 ?s1 ;
		wdt:P2325 ?t1 ;
		wdt:P2233 ?g1 .
}
SIMILARITY JOIN ON (?h1 ?a1 ?o1 ?s1 ?t1 ?g1) (?h2 ?a2 ?o2 ?s2 ?t2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?a2 ?o2 ?s2 ?t2 ?g2 WHERE {
	?id2 wdt:P1457 ?h2 ;
		wdt:P2248 ?a2 ;
		wdt:P2045 ?o2 ;
		wdt:P1096 ?s2 ;
		wdt:P2325 ?t2 ;
		wdt:P2233 ?g2 .
}
