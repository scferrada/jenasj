PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?p1 ?l1 ?j1 ?h1 WHERE {
	?id1 wdt:P1457 ?y1 ;
		wdt:P1096 ?p1 ;
		wdt:P2233 ?l1 ;
		wdt:P2045 ?j1 ;
		wdt:P2248 ?h1 .
}
SIMILARITY JOIN ON (?y1 ?p1 ?l1 ?j1 ?h1) (?y2 ?p2 ?l2 ?j2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?p2 ?l2 ?j2 ?h2 WHERE {
	?id2 wdt:P1457 ?y2 ;
		wdt:P1096 ?p2 ;
		wdt:P2233 ?l2 ;
		wdt:P2045 ?j2 ;
		wdt:P2248 ?h2 .
}
