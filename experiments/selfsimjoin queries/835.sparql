PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?q1 ?m1 ?p1 ?g1 WHERE {
	?id1 wdt:P2146 ?e1 ;
		wdt:P2244 ?q1 ;
		wdt:P1457 ?m1 ;
		wdt:P2233 ?p1 ;
		wdt:P2325 ?g1 .
}
SIMILARITY JOIN ON (?e1 ?q1 ?m1 ?p1 ?g1) (?e2 ?q2 ?m2 ?p2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?q2 ?m2 ?p2 ?g2 WHERE {
	?id2 wdt:P2146 ?e2 ;
		wdt:P2244 ?q2 ;
		wdt:P1457 ?m2 ;
		wdt:P2233 ?p2 ;
		wdt:P2325 ?g2 .
}
