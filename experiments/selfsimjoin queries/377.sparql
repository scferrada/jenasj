PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?l1 ?v1 WHERE {
	?id1 wdt:P2146 ?m1 ;
		wdt:P2233 ?l1 ;
		wdt:P2243 ?v1 .
}
SIMILARITY JOIN ON (?m1 ?l1 ?v1) (?m2 ?l2 ?v2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?l2 ?v2 WHERE {
	?id2 wdt:P2146 ?m2 ;
		wdt:P2233 ?l2 ;
		wdt:P2243 ?v2 .
}
