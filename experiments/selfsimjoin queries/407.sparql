PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?w1 ?x1 ?t1 ?f1 WHERE {
	?id1 wdt:P2146 ?l1 ;
		wdt:P2244 ?w1 ;
		wdt:P1096 ?x1 ;
		wdt:P2213 ?t1 ;
		wdt:P2325 ?f1 .
}
SIMILARITY JOIN ON (?l1 ?w1 ?x1 ?t1 ?f1) (?l2 ?w2 ?x2 ?t2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?w2 ?x2 ?t2 ?f2 WHERE {
	?id2 wdt:P2146 ?l2 ;
		wdt:P2244 ?w2 ;
		wdt:P1096 ?x2 ;
		wdt:P2213 ?t2 ;
		wdt:P2325 ?f2 .
}
