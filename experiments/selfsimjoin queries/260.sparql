PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?f1 ?k1 ?q1 ?z1 ?x1 WHERE {
	?id1 wdt:P2146 ?f1 ;
		wdt:P2244 ?k1 ;
		wdt:P1457 ?q1 ;
		wdt:P2233 ?z1 ;
		wdt:P2045 ?x1 .
}
SIMILARITY JOIN ON (?f1 ?k1 ?q1 ?z1 ?x1) (?f2 ?k2 ?q2 ?z2 ?x2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?f2 ?k2 ?q2 ?z2 ?x2 WHERE {
	?id2 wdt:P2146 ?f2 ;
		wdt:P2244 ?k2 ;
		wdt:P1457 ?q2 ;
		wdt:P2233 ?z2 ;
		wdt:P2045 ?x2 .
}
