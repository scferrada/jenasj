PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?a1 ?g1 ?n1 ?f1 ?q1 WHERE {
	?id1 wdt:P2146 ?p1 ;
		wdt:P4501 ?a1 ;
		wdt:P2244 ?g1 ;
		wdt:P2213 ?n1 ;
		wdt:P1096 ?f1 ;
		wdt:P2233 ?q1 .
}
SIMILARITY JOIN ON (?p1 ?a1 ?g1 ?n1 ?f1 ?q1) (?p2 ?a2 ?g2 ?n2 ?f2 ?q2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?a2 ?g2 ?n2 ?f2 ?q2 WHERE {
	?id2 wdt:P2146 ?p2 ;
		wdt:P4501 ?a2 ;
		wdt:P2244 ?g2 ;
		wdt:P2213 ?n2 ;
		wdt:P1096 ?f2 ;
		wdt:P2233 ?q2 .
}
