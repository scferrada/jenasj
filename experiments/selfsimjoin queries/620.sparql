PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?b1 ?p1 ?c1 ?o1 WHERE {
	?id1 wdt:P2146 ?b1 ;
		wdt:P1457 ?p1 ;
		wdt:P2233 ?c1 ;
		wdt:P2243 ?o1 .
}
SIMILARITY JOIN ON (?b1 ?p1 ?c1 ?o1) (?b2 ?p2 ?c2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?b2 ?p2 ?c2 ?o2 WHERE {
	?id2 wdt:P2146 ?b2 ;
		wdt:P1457 ?p2 ;
		wdt:P2233 ?c2 ;
		wdt:P2243 ?o2 .
}
