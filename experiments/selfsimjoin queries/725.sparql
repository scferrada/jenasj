PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?w1 ?d1 WHERE {
	?id1 wdt:P2244 ?m1 ;
		wdt:P2248 ?w1 ;
		wdt:P2213 ?d1 .
}
SIMILARITY JOIN ON (?m1 ?w1 ?d1) (?m2 ?w2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?w2 ?d2 WHERE {
	?id2 wdt:P2244 ?m2 ;
		wdt:P2248 ?w2 ;
		wdt:P2213 ?d2 .
}
