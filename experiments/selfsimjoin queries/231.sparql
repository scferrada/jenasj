PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?u1 ?c1 ?o1 ?b1 ?x1 ?k1 WHERE {
	?id1 wdt:P2154 ?u1 ;
		wdt:P1122 ?c1 ;
		wdt:P2114 ?o1 ;
		wdt:P1086 ?b1 ;
		wdt:P1123 ?x1 ;
		wdt:P1148 ?k1 .
}
SIMILARITY JOIN ON (?u1 ?c1 ?o1 ?b1 ?x1 ?k1) (?u2 ?c2 ?o2 ?b2 ?x2 ?k2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?u2 ?c2 ?o2 ?b2 ?x2 ?k2 WHERE {
	?id2 wdt:P2154 ?u2 ;
		wdt:P1122 ?c2 ;
		wdt:P2114 ?o2 ;
		wdt:P1086 ?b2 ;
		wdt:P1123 ?x2 ;
		wdt:P1148 ?k2 .
}
