PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?m1 ?w1 ?x1 ?t1 WHERE {
	?id1 wdt:P2244 ?q1 ;
		wdt:P1457 ?m1 ;
		wdt:P1096 ?w1 ;
		wdt:P4501 ?x1 ;
		wdt:P2248 ?t1 .
}
SIMILARITY JOIN ON (?q1 ?m1 ?w1 ?x1 ?t1) (?q2 ?m2 ?w2 ?x2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?m2 ?w2 ?x2 ?t2 WHERE {
	?id2 wdt:P2244 ?q2 ;
		wdt:P1457 ?m2 ;
		wdt:P1096 ?w2 ;
		wdt:P4501 ?x2 ;
		wdt:P2248 ?t2 .
}
