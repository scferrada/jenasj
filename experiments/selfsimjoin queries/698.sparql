PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?z1 ?p1 ?x1 ?j1 ?y1 WHERE {
	?id1 wdt:P2146 ?z1 ;
		wdt:P2244 ?p1 ;
		wdt:P1096 ?x1 ;
		wdt:P2233 ?j1 ;
		wdt:P4501 ?y1 .
}
SIMILARITY JOIN ON (?z1 ?p1 ?x1 ?j1 ?y1) (?z2 ?p2 ?x2 ?j2 ?y2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?z2 ?p2 ?x2 ?j2 ?y2 WHERE {
	?id2 wdt:P2146 ?z2 ;
		wdt:P2244 ?p2 ;
		wdt:P1096 ?x2 ;
		wdt:P2233 ?j2 ;
		wdt:P4501 ?y2 .
}
