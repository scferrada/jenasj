PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?f1 ?g1 ?d1 ?e1 ?m1 ?q1 WHERE {
	?id1 wdt:P1457 ?f1 ;
		wdt:P4501 ?g1 ;
		wdt:P2244 ?d1 ;
		wdt:P2243 ?e1 ;
		wdt:P2045 ?m1 ;
		wdt:P2233 ?q1 .
}
SIMILARITY JOIN ON (?f1 ?g1 ?d1 ?e1 ?m1 ?q1) (?f2 ?g2 ?d2 ?e2 ?m2 ?q2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?f2 ?g2 ?d2 ?e2 ?m2 ?q2 WHERE {
	?id2 wdt:P1457 ?f2 ;
		wdt:P4501 ?g2 ;
		wdt:P2244 ?d2 ;
		wdt:P2243 ?e2 ;
		wdt:P2045 ?m2 ;
		wdt:P2233 ?q2 .
}
