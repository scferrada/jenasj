PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?w1 ?f1 ?v1 ?j1 ?x1 WHERE {
	?id1 wdt:P2146 ?w1 ;
		wdt:P2213 ?f1 ;
		wdt:P2045 ?v1 ;
		wdt:P1096 ?j1 ;
		wdt:P2243 ?x1 .
}
SIMILARITY JOIN ON (?w1 ?f1 ?v1 ?j1 ?x1) (?w2 ?f2 ?v2 ?j2 ?x2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?w2 ?f2 ?v2 ?j2 ?x2 WHERE {
	?id2 wdt:P2146 ?w2 ;
		wdt:P2213 ?f2 ;
		wdt:P2045 ?v2 ;
		wdt:P1096 ?j2 ;
		wdt:P2243 ?x2 .
}
