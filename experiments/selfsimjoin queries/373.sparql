PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?k1 ?j1 ?b1 WHERE {
	?id1 wdt:P2295 ?k1 ;
		wdt:P2139 ?j1 ;
		wdt:P3362 ?b1 .
}
SIMILARITY JOIN ON (?k1 ?j1 ?b1) (?k2 ?j2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?k2 ?j2 ?b2 WHERE {
	?id2 wdt:P2295 ?k2 ;
		wdt:P2139 ?j2 ;
		wdt:P3362 ?b2 .
}
