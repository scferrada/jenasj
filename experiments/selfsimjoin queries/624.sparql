PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?c1 ?p1 ?e1 ?r1 ?f1 WHERE {
	?id1 wdt:P2244 ?c1 ;
		wdt:P2045 ?p1 ;
		wdt:P1096 ?e1 ;
		wdt:P4501 ?r1 ;
		wdt:P2213 ?f1 .
}
SIMILARITY JOIN ON (?c1 ?p1 ?e1 ?r1 ?f1) (?c2 ?p2 ?e2 ?r2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?c2 ?p2 ?e2 ?r2 ?f2 WHERE {
	?id2 wdt:P2244 ?c2 ;
		wdt:P2045 ?p2 ;
		wdt:P1096 ?e2 ;
		wdt:P4501 ?r2 ;
		wdt:P2213 ?f2 .
}
