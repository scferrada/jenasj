PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?b1 ?z1 ?a1 ?s1 ?i1 WHERE {
	?id1 wdt:P1086 ?b1 ;
		wdt:P2154 ?z1 ;
		wdt:P1123 ?a1 ;
		wdt:P1148 ?s1 ;
		wdt:P2160 ?i1 .
}
SIMILARITY JOIN ON (?b1 ?z1 ?a1 ?s1 ?i1) (?b2 ?z2 ?a2 ?s2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?b2 ?z2 ?a2 ?s2 ?i2 WHERE {
	?id2 wdt:P1086 ?b2 ;
		wdt:P2154 ?z2 ;
		wdt:P1123 ?a2 ;
		wdt:P1148 ?s2 ;
		wdt:P2160 ?i2 .
}
