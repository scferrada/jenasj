PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?w1 ?d1 ?t1 ?k1 ?f1 WHERE {
	?id1 wdt:P2146 ?x1 ;
		wdt:P2248 ?w1 ;
		wdt:P2243 ?d1 ;
		wdt:P2213 ?t1 ;
		wdt:P2325 ?k1 ;
		wdt:P2233 ?f1 .
}
SIMILARITY JOIN ON (?x1 ?w1 ?d1 ?t1 ?k1 ?f1) (?x2 ?w2 ?d2 ?t2 ?k2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?w2 ?d2 ?t2 ?k2 ?f2 WHERE {
	?id2 wdt:P2146 ?x2 ;
		wdt:P2248 ?w2 ;
		wdt:P2243 ?d2 ;
		wdt:P2213 ?t2 ;
		wdt:P2325 ?k2 ;
		wdt:P2233 ?f2 .
}
