PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?i1 ?m1 ?h1 WHERE {
	?id1 wdt:P6343 ?t1 ;
		wdt:P6344 ?i1 ;
		wdt:P1538 ?m1 ;
		wdt:P1540 ?h1 .
}
SIMILARITY JOIN ON (?t1 ?i1 ?m1 ?h1) (?t2 ?i2 ?m2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?i2 ?m2 ?h2 WHERE {
	?id2 wdt:P6343 ?t2 ;
		wdt:P6344 ?i2 ;
		wdt:P1538 ?m2 ;
		wdt:P1540 ?h2 .
}
