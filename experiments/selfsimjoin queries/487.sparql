PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?d1 ?j1 ?e1 ?i1 ?l1 WHERE {
	?id1 wdt:P2146 ?v1 ;
		wdt:P2248 ?d1 ;
		wdt:P2244 ?j1 ;
		wdt:P2213 ?e1 ;
		wdt:P2045 ?i1 ;
		wdt:P2325 ?l1 .
}
SIMILARITY JOIN ON (?v1 ?d1 ?j1 ?e1 ?i1 ?l1) (?v2 ?d2 ?j2 ?e2 ?i2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?d2 ?j2 ?e2 ?i2 ?l2 WHERE {
	?id2 wdt:P2146 ?v2 ;
		wdt:P2248 ?d2 ;
		wdt:P2244 ?j2 ;
		wdt:P2213 ?e2 ;
		wdt:P2045 ?i2 ;
		wdt:P2325 ?l2 .
}
