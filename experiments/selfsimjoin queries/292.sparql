PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?b1 ?q1 ?l1 ?y1 ?s1 WHERE {
	?id1 wdt:P2146 ?t1 ;
		wdt:P1457 ?b1 ;
		wdt:P2248 ?q1 ;
		wdt:P2045 ?l1 ;
		wdt:P2325 ?y1 ;
		wdt:P4501 ?s1 .
}
SIMILARITY JOIN ON (?t1 ?b1 ?q1 ?l1 ?y1 ?s1) (?t2 ?b2 ?q2 ?l2 ?y2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?b2 ?q2 ?l2 ?y2 ?s2 WHERE {
	?id2 wdt:P2146 ?t2 ;
		wdt:P1457 ?b2 ;
		wdt:P2248 ?q2 ;
		wdt:P2045 ?l2 ;
		wdt:P2325 ?y2 ;
		wdt:P4501 ?s2 .
}
