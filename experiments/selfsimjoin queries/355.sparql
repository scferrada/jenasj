PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?k1 ?v1 ?a1 ?s1 WHERE {
	?id1 wdt:P1457 ?k1 ;
		wdt:P2045 ?v1 ;
		wdt:P2243 ?a1 ;
		wdt:P2248 ?s1 .
}
SIMILARITY JOIN ON (?k1 ?v1 ?a1 ?s1) (?k2 ?v2 ?a2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?k2 ?v2 ?a2 ?s2 WHERE {
	?id2 wdt:P1457 ?k2 ;
		wdt:P2045 ?v2 ;
		wdt:P2243 ?a2 ;
		wdt:P2248 ?s2 .
}
