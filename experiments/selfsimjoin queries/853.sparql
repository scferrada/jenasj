PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?m1 ?z1 ?t1 ?x1 ?c1 WHERE {
	?id1 wdt:P1457 ?d1 ;
		wdt:P2248 ?m1 ;
		wdt:P2244 ?z1 ;
		wdt:P2045 ?t1 ;
		wdt:P1096 ?x1 ;
		wdt:P4501 ?c1 .
}
SIMILARITY JOIN ON (?d1 ?m1 ?z1 ?t1 ?x1 ?c1) (?d2 ?m2 ?z2 ?t2 ?x2 ?c2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?m2 ?z2 ?t2 ?x2 ?c2 WHERE {
	?id2 wdt:P1457 ?d2 ;
		wdt:P2248 ?m2 ;
		wdt:P2244 ?z2 ;
		wdt:P2045 ?t2 ;
		wdt:P1096 ?x2 ;
		wdt:P4501 ?c2 .
}
