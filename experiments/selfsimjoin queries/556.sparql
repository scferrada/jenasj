PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?p1 ?b1 WHERE {
	?id1 wdt:P1086 ?l1 ;
		wdt:P1122 ?p1 ;
		wdt:P2160 ?b1 .
}
SIMILARITY JOIN ON (?l1 ?p1 ?b1) (?l2 ?p2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?p2 ?b2 WHERE {
	?id2 wdt:P1086 ?l2 ;
		wdt:P1122 ?p2 ;
		wdt:P2160 ?b2 .
}
