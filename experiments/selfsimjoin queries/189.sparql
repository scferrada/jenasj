PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?n1 ?g1 ?q1 ?e1 ?b1 WHERE {
	?id1 wdt:P2146 ?n1 ;
		wdt:P2213 ?g1 ;
		wdt:P2233 ?q1 ;
		wdt:P2248 ?e1 ;
		wdt:P2243 ?b1 .
}
SIMILARITY JOIN ON (?n1 ?g1 ?q1 ?e1 ?b1) (?n2 ?g2 ?q2 ?e2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?n2 ?g2 ?q2 ?e2 ?b2 WHERE {
	?id2 wdt:P2146 ?n2 ;
		wdt:P2213 ?g2 ;
		wdt:P2233 ?q2 ;
		wdt:P2248 ?e2 ;
		wdt:P2243 ?b2 .
}
