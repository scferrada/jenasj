PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?w1 ?t1 ?v1 ?y1 ?r1 WHERE {
	?id1 wdt:P2213 ?w1 ;
		wdt:P2045 ?t1 ;
		wdt:P1096 ?v1 ;
		wdt:P2233 ?y1 ;
		wdt:P4501 ?r1 .
}
SIMILARITY JOIN ON (?w1 ?t1 ?v1 ?y1 ?r1) (?w2 ?t2 ?v2 ?y2 ?r2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?w2 ?t2 ?v2 ?y2 ?r2 WHERE {
	?id2 wdt:P2213 ?w2 ;
		wdt:P2045 ?t2 ;
		wdt:P1096 ?v2 ;
		wdt:P2233 ?y2 ;
		wdt:P4501 ?r2 .
}
