PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?o1 ?u1 WHERE {
	?id1 wdt:P1096 ?l1 ;
		wdt:P4501 ?o1 ;
		wdt:P2325 ?u1 .
}
SIMILARITY JOIN ON (?l1 ?o1 ?u1) (?l2 ?o2 ?u2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?o2 ?u2 WHERE {
	?id2 wdt:P1096 ?l2 ;
		wdt:P4501 ?o2 ;
		wdt:P2325 ?u2 .
}
