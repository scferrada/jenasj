PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?q1 ?b1 ?v1 ?w1 ?d1 WHERE {
	?id1 wdt:P1457 ?t1 ;
		wdt:P2248 ?q1 ;
		wdt:P2244 ?b1 ;
		wdt:P1096 ?v1 ;
		wdt:P2233 ?w1 ;
		wdt:P4501 ?d1 .
}
SIMILARITY JOIN ON (?t1 ?q1 ?b1 ?v1 ?w1 ?d1) (?t2 ?q2 ?b2 ?v2 ?w2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?q2 ?b2 ?v2 ?w2 ?d2 WHERE {
	?id2 wdt:P1457 ?t2 ;
		wdt:P2248 ?q2 ;
		wdt:P2244 ?b2 ;
		wdt:P1096 ?v2 ;
		wdt:P2233 ?w2 ;
		wdt:P4501 ?d2 .
}
