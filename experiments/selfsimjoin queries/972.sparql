PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?q1 ?i1 ?u1 ?y1 WHERE {
	?id1 wdt:P2146 ?x1 ;
		wdt:P2213 ?q1 ;
		wdt:P2045 ?i1 ;
		wdt:P1096 ?u1 ;
		wdt:P4501 ?y1 .
}
SIMILARITY JOIN ON (?x1 ?q1 ?i1 ?u1 ?y1) (?x2 ?q2 ?i2 ?u2 ?y2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?q2 ?i2 ?u2 ?y2 WHERE {
	?id2 wdt:P2146 ?x2 ;
		wdt:P2213 ?q2 ;
		wdt:P2045 ?i2 ;
		wdt:P1096 ?u2 ;
		wdt:P4501 ?y2 .
}
