PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?j1 ?b1 ?p1 ?e1 WHERE {
	?id1 wdt:P2146 ?q1 ;
		wdt:P2244 ?j1 ;
		wdt:P2233 ?b1 ;
		wdt:P2248 ?p1 ;
		wdt:P4501 ?e1 .
}
SIMILARITY JOIN ON (?q1 ?j1 ?b1 ?p1 ?e1) (?q2 ?j2 ?b2 ?p2 ?e2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?j2 ?b2 ?p2 ?e2 WHERE {
	?id2 wdt:P2146 ?q2 ;
		wdt:P2244 ?j2 ;
		wdt:P2233 ?b2 ;
		wdt:P2248 ?p2 ;
		wdt:P4501 ?e2 .
}
