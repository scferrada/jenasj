PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?w1 ?c1 ?h1 ?l1 ?r1 WHERE {
	?id1 wdt:P2154 ?p1 ;
		wdt:P1122 ?w1 ;
		wdt:P2160 ?c1 ;
		wdt:P1086 ?h1 ;
		wdt:P1123 ?l1 ;
		wdt:P1148 ?r1 .
}
SIMILARITY JOIN ON (?p1 ?w1 ?c1 ?h1 ?l1 ?r1) (?p2 ?w2 ?c2 ?h2 ?l2 ?r2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?w2 ?c2 ?h2 ?l2 ?r2 WHERE {
	?id2 wdt:P2154 ?p2 ;
		wdt:P1122 ?w2 ;
		wdt:P2160 ?c2 ;
		wdt:P1086 ?h2 ;
		wdt:P1123 ?l2 ;
		wdt:P1148 ?r2 .
}
