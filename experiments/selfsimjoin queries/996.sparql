PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?g1 ?a1 ?u1 ?z1 WHERE {
	?id1 wdt:P2146 ?m1 ;
		wdt:P2244 ?g1 ;
		wdt:P1457 ?a1 ;
		wdt:P2045 ?u1 ;
		wdt:P2213 ?z1 .
}
SIMILARITY JOIN ON (?m1 ?g1 ?a1 ?u1 ?z1) (?m2 ?g2 ?a2 ?u2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?g2 ?a2 ?u2 ?z2 WHERE {
	?id2 wdt:P2146 ?m2 ;
		wdt:P2244 ?g2 ;
		wdt:P1457 ?a2 ;
		wdt:P2045 ?u2 ;
		wdt:P2213 ?z2 .
}
