PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?p1 ?o1 ?m1 ?e1 WHERE {
	?id1 wdt:P2146 ?y1 ;
		wdt:P2045 ?p1 ;
		wdt:P1096 ?o1 ;
		wdt:P2233 ?m1 ;
		wdt:P2325 ?e1 .
}
SIMILARITY JOIN ON (?y1 ?p1 ?o1 ?m1 ?e1) (?y2 ?p2 ?o2 ?m2 ?e2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?p2 ?o2 ?m2 ?e2 WHERE {
	?id2 wdt:P2146 ?y2 ;
		wdt:P2045 ?p2 ;
		wdt:P1096 ?o2 ;
		wdt:P2233 ?m2 ;
		wdt:P2325 ?e2 .
}
