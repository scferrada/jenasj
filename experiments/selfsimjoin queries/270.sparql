PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?a1 ?y1 ?j1 ?z1 ?i1 ?n1 WHERE {
	?id1 wdt:P2146 ?a1 ;
		wdt:P1457 ?y1 ;
		wdt:P2243 ?j1 ;
		wdt:P2213 ?z1 ;
		wdt:P2325 ?i1 ;
		wdt:P2233 ?n1 .
}
SIMILARITY JOIN ON (?a1 ?y1 ?j1 ?z1 ?i1 ?n1) (?a2 ?y2 ?j2 ?z2 ?i2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?a2 ?y2 ?j2 ?z2 ?i2 ?n2 WHERE {
	?id2 wdt:P2146 ?a2 ;
		wdt:P1457 ?y2 ;
		wdt:P2243 ?j2 ;
		wdt:P2213 ?z2 ;
		wdt:P2325 ?i2 ;
		wdt:P2233 ?n2 .
}
