PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?b1 ?f1 ?a1 ?i1 WHERE {
	?id1 wdt:P2244 ?b1 ;
		wdt:P2233 ?f1 ;
		wdt:P2243 ?a1 ;
		wdt:P2325 ?i1 .
}
SIMILARITY JOIN ON (?b1 ?f1 ?a1 ?i1) (?b2 ?f2 ?a2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?b2 ?f2 ?a2 ?i2 WHERE {
	?id2 wdt:P2244 ?b2 ;
		wdt:P2233 ?f2 ;
		wdt:P2243 ?a2 ;
		wdt:P2325 ?i2 .
}
