PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?s1 ?l1 WHERE {
	?id1 wdt:P2146 ?v1 ;
		wdt:P2233 ?s1 ;
		wdt:P4501 ?l1 .
}
SIMILARITY JOIN ON (?v1 ?s1 ?l1) (?v2 ?s2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?s2 ?l2 WHERE {
	?id2 wdt:P2146 ?v2 ;
		wdt:P2233 ?s2 ;
		wdt:P4501 ?l2 .
}
