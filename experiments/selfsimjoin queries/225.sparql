PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?f1 ?w1 ?u1 ?e1 ?d1 WHERE {
	?id1 wdt:P2146 ?p1 ;
		wdt:P1457 ?f1 ;
		wdt:P2248 ?w1 ;
		wdt:P1096 ?u1 ;
		wdt:P2325 ?e1 ;
		wdt:P4501 ?d1 .
}
SIMILARITY JOIN ON (?p1 ?f1 ?w1 ?u1 ?e1 ?d1) (?p2 ?f2 ?w2 ?u2 ?e2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?f2 ?w2 ?u2 ?e2 ?d2 WHERE {
	?id2 wdt:P2146 ?p2 ;
		wdt:P1457 ?f2 ;
		wdt:P2248 ?w2 ;
		wdt:P1096 ?u2 ;
		wdt:P2325 ?e2 ;
		wdt:P4501 ?d2 .
}
