PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?e1 ?z1 ?m1 ?b1 ?c1 WHERE {
	?id1 wdt:P2146 ?h1 ;
		wdt:P1457 ?e1 ;
		wdt:P4501 ?z1 ;
		wdt:P2213 ?m1 ;
		wdt:P2325 ?b1 ;
		wdt:P2233 ?c1 .
}
SIMILARITY JOIN ON (?h1 ?e1 ?z1 ?m1 ?b1 ?c1) (?h2 ?e2 ?z2 ?m2 ?b2 ?c2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?e2 ?z2 ?m2 ?b2 ?c2 WHERE {
	?id2 wdt:P2146 ?h2 ;
		wdt:P1457 ?e2 ;
		wdt:P4501 ?z2 ;
		wdt:P2213 ?m2 ;
		wdt:P2325 ?b2 ;
		wdt:P2233 ?c2 .
}
