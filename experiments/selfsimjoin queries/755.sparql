PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?w1 ?f1 ?j1 ?x1 ?e1 ?l1 WHERE {
	?id1 wdt:P2146 ?w1 ;
		wdt:P1457 ?f1 ;
		wdt:P2244 ?j1 ;
		wdt:P2045 ?x1 ;
		wdt:P2325 ?e1 ;
		wdt:P2233 ?l1 .
}
SIMILARITY JOIN ON (?w1 ?f1 ?j1 ?x1 ?e1 ?l1) (?w2 ?f2 ?j2 ?x2 ?e2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?w2 ?f2 ?j2 ?x2 ?e2 ?l2 WHERE {
	?id2 wdt:P2146 ?w2 ;
		wdt:P1457 ?f2 ;
		wdt:P2244 ?j2 ;
		wdt:P2045 ?x2 ;
		wdt:P2325 ?e2 ;
		wdt:P2233 ?l2 .
}
