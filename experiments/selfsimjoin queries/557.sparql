PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?l1 ?y1 ?d1 ?c1 ?n1 WHERE {
	?id1 wdt:P2146 ?i1 ;
		wdt:P2244 ?l1 ;
		wdt:P2243 ?y1 ;
		wdt:P2213 ?d1 ;
		wdt:P1096 ?c1 ;
		wdt:P2233 ?n1 .
}
SIMILARITY JOIN ON (?i1 ?l1 ?y1 ?d1 ?c1 ?n1) (?i2 ?l2 ?y2 ?d2 ?c2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?l2 ?y2 ?d2 ?c2 ?n2 WHERE {
	?id2 wdt:P2146 ?i2 ;
		wdt:P2244 ?l2 ;
		wdt:P2243 ?y2 ;
		wdt:P2213 ?d2 ;
		wdt:P1096 ?c2 ;
		wdt:P2233 ?n2 .
}
