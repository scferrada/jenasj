PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?c1 ?n1 ?z1 ?p1 ?s1 ?m1 WHERE {
	?id1 wdt:P1457 ?c1 ;
		wdt:P2248 ?n1 ;
		wdt:P2243 ?z1 ;
		wdt:P2045 ?p1 ;
		wdt:P1096 ?s1 ;
		wdt:P2325 ?m1 .
}
SIMILARITY JOIN ON (?c1 ?n1 ?z1 ?p1 ?s1 ?m1) (?c2 ?n2 ?z2 ?p2 ?s2 ?m2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?c2 ?n2 ?z2 ?p2 ?s2 ?m2 WHERE {
	?id2 wdt:P1457 ?c2 ;
		wdt:P2248 ?n2 ;
		wdt:P2243 ?z2 ;
		wdt:P2045 ?p2 ;
		wdt:P1096 ?s2 ;
		wdt:P2325 ?m2 .
}
