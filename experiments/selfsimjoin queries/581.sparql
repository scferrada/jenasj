PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?i1 ?a1 ?g1 ?q1 WHERE {
	?id1 wdt:P2244 ?r1 ;
		wdt:P1096 ?i1 ;
		wdt:P2248 ?a1 ;
		wdt:P2243 ?g1 ;
		wdt:P4501 ?q1 .
}
SIMILARITY JOIN ON (?r1 ?i1 ?a1 ?g1 ?q1) (?r2 ?i2 ?a2 ?g2 ?q2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?i2 ?a2 ?g2 ?q2 WHERE {
	?id2 wdt:P2244 ?r2 ;
		wdt:P1096 ?i2 ;
		wdt:P2248 ?a2 ;
		wdt:P2243 ?g2 ;
		wdt:P4501 ?q2 .
}
