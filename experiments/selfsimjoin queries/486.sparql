PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?b1 ?u1 ?j1 ?q1 ?o1 WHERE {
	?id1 wdt:P2146 ?b1 ;
		wdt:P2213 ?u1 ;
		wdt:P1457 ?j1 ;
		wdt:P2233 ?q1 ;
		wdt:P2045 ?o1 .
}
SIMILARITY JOIN ON (?b1 ?u1 ?j1 ?q1 ?o1) (?b2 ?u2 ?j2 ?q2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?b2 ?u2 ?j2 ?q2 ?o2 WHERE {
	?id2 wdt:P2146 ?b2 ;
		wdt:P2213 ?u2 ;
		wdt:P1457 ?j2 ;
		wdt:P2233 ?q2 ;
		wdt:P2045 ?o2 .
}
