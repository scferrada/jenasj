PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?z1 ?s1 ?l1 ?p1 ?a1 WHERE {
	?id1 wdt:P2146 ?j1 ;
		wdt:P2243 ?z1 ;
		wdt:P2213 ?s1 ;
		wdt:P1096 ?l1 ;
		wdt:P2325 ?p1 ;
		wdt:P2233 ?a1 .
}
SIMILARITY JOIN ON (?j1 ?z1 ?s1 ?l1 ?p1 ?a1) (?j2 ?z2 ?s2 ?l2 ?p2 ?a2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?z2 ?s2 ?l2 ?p2 ?a2 WHERE {
	?id2 wdt:P2146 ?j2 ;
		wdt:P2243 ?z2 ;
		wdt:P2213 ?s2 ;
		wdt:P1096 ?l2 ;
		wdt:P2325 ?p2 ;
		wdt:P2233 ?a2 .
}
