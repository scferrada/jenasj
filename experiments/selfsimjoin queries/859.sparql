PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?a1 ?z1 WHERE {
	?id1 wdt:P2244 ?q1 ;
		wdt:P1457 ?a1 ;
		wdt:P1096 ?z1 .
}
SIMILARITY JOIN ON (?q1 ?a1 ?z1) (?q2 ?a2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?a2 ?z2 WHERE {
	?id2 wdt:P2244 ?q2 ;
		wdt:P1457 ?a2 ;
		wdt:P1096 ?z2 .
}
