PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?x1 ?z1 ?g1 WHERE {
	?id1 wdt:P2295 ?q1 ;
		wdt:P2403 ?x1 ;
		wdt:P2139 ?z1 ;
		wdt:P3362 ?g1 .
}
SIMILARITY JOIN ON (?q1 ?x1 ?z1 ?g1) (?q2 ?x2 ?z2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?x2 ?z2 ?g2 WHERE {
	?id2 wdt:P2295 ?q2 ;
		wdt:P2403 ?x2 ;
		wdt:P2139 ?z2 ;
		wdt:P3362 ?g2 .
}
