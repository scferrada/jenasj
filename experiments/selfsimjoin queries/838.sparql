PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?s1 ?a1 ?j1 ?e1 ?z1 WHERE {
	?id1 wdt:P2146 ?d1 ;
		wdt:P2248 ?s1 ;
		wdt:P2045 ?a1 ;
		wdt:P1096 ?j1 ;
		wdt:P2325 ?e1 ;
		wdt:P2233 ?z1 .
}
SIMILARITY JOIN ON (?d1 ?s1 ?a1 ?j1 ?e1 ?z1) (?d2 ?s2 ?a2 ?j2 ?e2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?s2 ?a2 ?j2 ?e2 ?z2 WHERE {
	?id2 wdt:P2146 ?d2 ;
		wdt:P2248 ?s2 ;
		wdt:P2045 ?a2 ;
		wdt:P1096 ?j2 ;
		wdt:P2325 ?e2 ;
		wdt:P2233 ?z2 .
}
