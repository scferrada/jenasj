PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?o1 ?j1 ?d1 ?b1 ?r1 ?g1 WHERE {
	?id1 wdt:P4501 ?o1 ;
		wdt:P2244 ?j1 ;
		wdt:P2243 ?d1 ;
		wdt:P1096 ?b1 ;
		wdt:P2325 ?r1 ;
		wdt:P2233 ?g1 .
}
SIMILARITY JOIN ON (?o1 ?j1 ?d1 ?b1 ?r1 ?g1) (?o2 ?j2 ?d2 ?b2 ?r2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?o2 ?j2 ?d2 ?b2 ?r2 ?g2 WHERE {
	?id2 wdt:P4501 ?o2 ;
		wdt:P2244 ?j2 ;
		wdt:P2243 ?d2 ;
		wdt:P1096 ?b2 ;
		wdt:P2325 ?r2 ;
		wdt:P2233 ?g2 .
}
