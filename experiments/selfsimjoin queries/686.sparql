PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?f1 ?n1 ?g1 ?y1 WHERE {
	?id1 wdt:P1086 ?f1 ;
		wdt:P2154 ?n1 ;
		wdt:P2160 ?g1 ;
		wdt:P2114 ?y1 .
}
SIMILARITY JOIN ON (?f1 ?n1 ?g1 ?y1) (?f2 ?n2 ?g2 ?y2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?f2 ?n2 ?g2 ?y2 WHERE {
	?id2 wdt:P1086 ?f2 ;
		wdt:P2154 ?n2 ;
		wdt:P2160 ?g2 ;
		wdt:P2114 ?y2 .
}
