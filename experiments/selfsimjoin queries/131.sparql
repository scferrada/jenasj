PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?o1 ?h1 ?e1 ?f1 ?w1 ?b1 WHERE {
	?id1 wdt:P2146 ?o1 ;
		wdt:P4501 ?h1 ;
		wdt:P2244 ?e1 ;
		wdt:P1096 ?f1 ;
		wdt:P2233 ?w1 ;
		wdt:P2248 ?b1 .
}
SIMILARITY JOIN ON (?o1 ?h1 ?e1 ?f1 ?w1 ?b1) (?o2 ?h2 ?e2 ?f2 ?w2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?o2 ?h2 ?e2 ?f2 ?w2 ?b2 WHERE {
	?id2 wdt:P2146 ?o2 ;
		wdt:P4501 ?h2 ;
		wdt:P2244 ?e2 ;
		wdt:P1096 ?f2 ;
		wdt:P2233 ?w2 ;
		wdt:P2248 ?b2 .
}
