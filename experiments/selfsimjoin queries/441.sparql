PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?h1 ?f1 WHERE {
	?id1 wdt:P2244 ?m1 ;
		wdt:P1457 ?h1 ;
		wdt:P2243 ?f1 .
}
SIMILARITY JOIN ON (?m1 ?h1 ?f1) (?m2 ?h2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?h2 ?f2 WHERE {
	?id2 wdt:P2244 ?m2 ;
		wdt:P1457 ?h2 ;
		wdt:P2243 ?f2 .
}
