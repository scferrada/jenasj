PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?n1 ?a1 ?e1 ?b1 WHERE {
	?id1 wdt:P2146 ?t1 ;
		wdt:P2213 ?n1 ;
		wdt:P2233 ?a1 ;
		wdt:P2248 ?e1 ;
		wdt:P2325 ?b1 .
}
SIMILARITY JOIN ON (?t1 ?n1 ?a1 ?e1 ?b1) (?t2 ?n2 ?a2 ?e2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?n2 ?a2 ?e2 ?b2 WHERE {
	?id2 wdt:P2146 ?t2 ;
		wdt:P2213 ?n2 ;
		wdt:P2233 ?a2 ;
		wdt:P2248 ?e2 ;
		wdt:P2325 ?b2 .
}
