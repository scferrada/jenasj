PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?r1 ?s1 ?l1 ?f1 WHERE {
	?id1 wdt:P2146 ?v1 ;
		wdt:P1457 ?r1 ;
		wdt:P1096 ?s1 ;
		wdt:P2248 ?l1 ;
		wdt:P4501 ?f1 .
}
SIMILARITY JOIN ON (?v1 ?r1 ?s1 ?l1 ?f1) (?v2 ?r2 ?s2 ?l2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?r2 ?s2 ?l2 ?f2 WHERE {
	?id2 wdt:P2146 ?v2 ;
		wdt:P1457 ?r2 ;
		wdt:P1096 ?s2 ;
		wdt:P2248 ?l2 ;
		wdt:P4501 ?f2 .
}
