PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?f1 ?g1 ?d1 ?x1 ?s1 ?i1 WHERE {
	?id1 wdt:P2146 ?f1 ;
		wdt:P1457 ?g1 ;
		wdt:P2248 ?d1 ;
		wdt:P2243 ?x1 ;
		wdt:P1096 ?s1 ;
		wdt:P4501 ?i1 .
}
SIMILARITY JOIN ON (?f1 ?g1 ?d1 ?x1 ?s1 ?i1) (?f2 ?g2 ?d2 ?x2 ?s2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?f2 ?g2 ?d2 ?x2 ?s2 ?i2 WHERE {
	?id2 wdt:P2146 ?f2 ;
		wdt:P1457 ?g2 ;
		wdt:P2248 ?d2 ;
		wdt:P2243 ?x2 ;
		wdt:P1096 ?s2 ;
		wdt:P4501 ?i2 .
}
