PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?o1 ?g1 ?d1 ?t1 WHERE {
	?id1 wdt:P1096 ?o1 ;
		wdt:P2233 ?g1 ;
		wdt:P4501 ?d1 ;
		wdt:P2243 ?t1 .
}
SIMILARITY JOIN ON (?o1 ?g1 ?d1 ?t1) (?o2 ?g2 ?d2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?o2 ?g2 ?d2 ?t2 WHERE {
	?id2 wdt:P1096 ?o2 ;
		wdt:P2233 ?g2 ;
		wdt:P4501 ?d2 ;
		wdt:P2243 ?t2 .
}
