PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?q1 ?x1 ?e1 ?c1 WHERE {
	?id1 wdt:P2146 ?m1 ;
		wdt:P1457 ?q1 ;
		wdt:P2233 ?x1 ;
		wdt:P4501 ?e1 ;
		wdt:P2243 ?c1 .
}
SIMILARITY JOIN ON (?m1 ?q1 ?x1 ?e1 ?c1) (?m2 ?q2 ?x2 ?e2 ?c2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?q2 ?x2 ?e2 ?c2 WHERE {
	?id2 wdt:P2146 ?m2 ;
		wdt:P1457 ?q2 ;
		wdt:P2233 ?x2 ;
		wdt:P4501 ?e2 ;
		wdt:P2243 ?c2 .
}
