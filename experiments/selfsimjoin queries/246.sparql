PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?c1 ?d1 ?i1 ?b1 ?x1 ?s1 WHERE {
	?id1 wdt:P2146 ?c1 ;
		wdt:P4501 ?d1 ;
		wdt:P2243 ?i1 ;
		wdt:P2213 ?b1 ;
		wdt:P2233 ?x1 ;
		wdt:P2248 ?s1 .
}
SIMILARITY JOIN ON (?c1 ?d1 ?i1 ?b1 ?x1 ?s1) (?c2 ?d2 ?i2 ?b2 ?x2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?c2 ?d2 ?i2 ?b2 ?x2 ?s2 WHERE {
	?id2 wdt:P2146 ?c2 ;
		wdt:P4501 ?d2 ;
		wdt:P2243 ?i2 ;
		wdt:P2213 ?b2 ;
		wdt:P2233 ?x2 ;
		wdt:P2248 ?s2 .
}
