PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?s1 ?f1 ?u1 ?c1 WHERE {
	?id1 wdt:P2146 ?d1 ;
		wdt:P2213 ?s1 ;
		wdt:P2233 ?f1 ;
		wdt:P2248 ?u1 ;
		wdt:P4501 ?c1 .
}
SIMILARITY JOIN ON (?d1 ?s1 ?f1 ?u1 ?c1) (?d2 ?s2 ?f2 ?u2 ?c2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?s2 ?f2 ?u2 ?c2 WHERE {
	?id2 wdt:P2146 ?d2 ;
		wdt:P2213 ?s2 ;
		wdt:P2233 ?f2 ;
		wdt:P2248 ?u2 ;
		wdt:P4501 ?c2 .
}
