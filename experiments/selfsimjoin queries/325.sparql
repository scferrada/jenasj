PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?h1 ?m1 ?d1 ?f1 WHERE {
	?id1 wdt:P2213 ?x1 ;
		wdt:P2045 ?h1 ;
		wdt:P2248 ?m1 ;
		wdt:P2243 ?d1 ;
		wdt:P4501 ?f1 .
}
SIMILARITY JOIN ON (?x1 ?h1 ?m1 ?d1 ?f1) (?x2 ?h2 ?m2 ?d2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?h2 ?m2 ?d2 ?f2 WHERE {
	?id2 wdt:P2213 ?x2 ;
		wdt:P2045 ?h2 ;
		wdt:P2248 ?m2 ;
		wdt:P2243 ?d2 ;
		wdt:P4501 ?f2 .
}
