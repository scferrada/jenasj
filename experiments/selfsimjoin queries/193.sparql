PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?a1 ?y1 ?c1 WHERE {
	?id1 wdt:P2244 ?x1 ;
		wdt:P1457 ?a1 ;
		wdt:P1096 ?y1 ;
		wdt:P4501 ?c1 .
}
SIMILARITY JOIN ON (?x1 ?a1 ?y1 ?c1) (?x2 ?a2 ?y2 ?c2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?a2 ?y2 ?c2 WHERE {
	?id2 wdt:P2244 ?x2 ;
		wdt:P1457 ?a2 ;
		wdt:P1096 ?y2 ;
		wdt:P4501 ?c2 .
}
