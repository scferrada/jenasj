PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?v1 ?c1 ?d1 ?l1 WHERE {
	?id1 wdt:P2244 ?e1 ;
		wdt:P1457 ?v1 ;
		wdt:P1096 ?c1 ;
		wdt:P2213 ?d1 ;
		wdt:P2325 ?l1 .
}
SIMILARITY JOIN ON (?e1 ?v1 ?c1 ?d1 ?l1) (?e2 ?v2 ?c2 ?d2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?v2 ?c2 ?d2 ?l2 WHERE {
	?id2 wdt:P2244 ?e2 ;
		wdt:P1457 ?v2 ;
		wdt:P1096 ?c2 ;
		wdt:P2213 ?d2 ;
		wdt:P2325 ?l2 .
}
