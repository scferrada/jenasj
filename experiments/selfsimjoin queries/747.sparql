PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?a1 ?x1 ?o1 WHERE {
	?id1 wdt:P2045 ?e1 ;
		wdt:P1096 ?a1 ;
		wdt:P2248 ?x1 ;
		wdt:P2325 ?o1 .
}
SIMILARITY JOIN ON (?e1 ?a1 ?x1 ?o1) (?e2 ?a2 ?x2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?a2 ?x2 ?o2 WHERE {
	?id2 wdt:P2045 ?e2 ;
		wdt:P1096 ?a2 ;
		wdt:P2248 ?x2 ;
		wdt:P2325 ?o2 .
}
