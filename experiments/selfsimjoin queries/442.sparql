PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?w1 ?f1 ?k1 ?j1 ?p1 ?l1 WHERE {
	?id1 wdt:P1457 ?w1 ;
		wdt:P4501 ?f1 ;
		wdt:P2244 ?k1 ;
		wdt:P1096 ?j1 ;
		wdt:P2325 ?p1 ;
		wdt:P2233 ?l1 .
}
SIMILARITY JOIN ON (?w1 ?f1 ?k1 ?j1 ?p1 ?l1) (?w2 ?f2 ?k2 ?j2 ?p2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?w2 ?f2 ?k2 ?j2 ?p2 ?l2 WHERE {
	?id2 wdt:P1457 ?w2 ;
		wdt:P4501 ?f2 ;
		wdt:P2244 ?k2 ;
		wdt:P1096 ?j2 ;
		wdt:P2325 ?p2 ;
		wdt:P2233 ?l2 .
}
