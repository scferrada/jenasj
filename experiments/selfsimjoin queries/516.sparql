PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?z1 ?q1 ?r1 ?d1 WHERE {
	?id1 wdt:P2244 ?h1 ;
		wdt:P1096 ?z1 ;
		wdt:P4501 ?q1 ;
		wdt:P2248 ?r1 ;
		wdt:P2325 ?d1 .
}
SIMILARITY JOIN ON (?h1 ?z1 ?q1 ?r1 ?d1) (?h2 ?z2 ?q2 ?r2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?z2 ?q2 ?r2 ?d2 WHERE {
	?id2 wdt:P2244 ?h2 ;
		wdt:P1096 ?z2 ;
		wdt:P4501 ?q2 ;
		wdt:P2248 ?r2 ;
		wdt:P2325 ?d2 .
}
