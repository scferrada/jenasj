PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?f1 ?g1 ?z1 ?l1 WHERE {
	?id1 wdt:P2146 ?f1 ;
		wdt:P2244 ?g1 ;
		wdt:P2045 ?z1 ;
		wdt:P2233 ?l1 .
}
SIMILARITY JOIN ON (?f1 ?g1 ?z1 ?l1) (?f2 ?g2 ?z2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?f2 ?g2 ?z2 ?l2 WHERE {
	?id2 wdt:P2146 ?f2 ;
		wdt:P2244 ?g2 ;
		wdt:P2045 ?z2 ;
		wdt:P2233 ?l2 .
}
