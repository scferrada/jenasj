PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?w1 ?g1 ?p1 ?s1 ?k1 ?j1 WHERE {
	?id1 wdt:P2146 ?w1 ;
		wdt:P2243 ?g1 ;
		wdt:P2213 ?p1 ;
		wdt:P2045 ?s1 ;
		wdt:P1096 ?k1 ;
		wdt:P2325 ?j1 .
}
SIMILARITY JOIN ON (?w1 ?g1 ?p1 ?s1 ?k1 ?j1) (?w2 ?g2 ?p2 ?s2 ?k2 ?j2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?w2 ?g2 ?p2 ?s2 ?k2 ?j2 WHERE {
	?id2 wdt:P2146 ?w2 ;
		wdt:P2243 ?g2 ;
		wdt:P2213 ?p2 ;
		wdt:P2045 ?s2 ;
		wdt:P1096 ?k2 ;
		wdt:P2325 ?j2 .
}
