PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?w1 ?m1 ?t1 WHERE {
	?id1 wdt:P2213 ?r1 ;
		wdt:P1096 ?w1 ;
		wdt:P2233 ?m1 ;
		wdt:P2243 ?t1 .
}
SIMILARITY JOIN ON (?r1 ?w1 ?m1 ?t1) (?r2 ?w2 ?m2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?w2 ?m2 ?t2 WHERE {
	?id2 wdt:P2213 ?r2 ;
		wdt:P1096 ?w2 ;
		wdt:P2233 ?m2 ;
		wdt:P2243 ?t2 .
}
