PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?b1 ?s1 ?d1 ?m1 ?i1 ?z1 WHERE {
	?id1 wdt:P1457 ?b1 ;
		wdt:P2248 ?s1 ;
		wdt:P2244 ?d1 ;
		wdt:P2045 ?m1 ;
		wdt:P2233 ?i1 ;
		wdt:P4501 ?z1 .
}
SIMILARITY JOIN ON (?b1 ?s1 ?d1 ?m1 ?i1 ?z1) (?b2 ?s2 ?d2 ?m2 ?i2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?b2 ?s2 ?d2 ?m2 ?i2 ?z2 WHERE {
	?id2 wdt:P1457 ?b2 ;
		wdt:P2248 ?s2 ;
		wdt:P2244 ?d2 ;
		wdt:P2045 ?m2 ;
		wdt:P2233 ?i2 ;
		wdt:P4501 ?z2 .
}
