PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?i1 ?n1 ?z1 ?q1 WHERE {
	?id1 wdt:P2244 ?m1 ;
		wdt:P2213 ?i1 ;
		wdt:P2248 ?n1 ;
		wdt:P2243 ?z1 ;
		wdt:P4501 ?q1 .
}
SIMILARITY JOIN ON (?m1 ?i1 ?n1 ?z1 ?q1) (?m2 ?i2 ?n2 ?z2 ?q2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?i2 ?n2 ?z2 ?q2 WHERE {
	?id2 wdt:P2244 ?m2 ;
		wdt:P2213 ?i2 ;
		wdt:P2248 ?n2 ;
		wdt:P2243 ?z2 ;
		wdt:P4501 ?q2 .
}
