PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?z1 ?h1 ?j1 WHERE {
	?id1 wdt:P1123 ?s1 ;
		wdt:P1148 ?z1 ;
		wdt:P2114 ?h1 ;
		wdt:P1122 ?j1 .
}
SIMILARITY JOIN ON (?s1 ?z1 ?h1 ?j1) (?s2 ?z2 ?h2 ?j2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?z2 ?h2 ?j2 WHERE {
	?id2 wdt:P1123 ?s2 ;
		wdt:P1148 ?z2 ;
		wdt:P2114 ?h2 ;
		wdt:P1122 ?j2 .
}
