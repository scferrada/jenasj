PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?f1 ?x1 ?k1 ?w1 WHERE {
	?id1 wdt:P2244 ?e1 ;
		wdt:P2045 ?f1 ;
		wdt:P2248 ?x1 ;
		wdt:P2243 ?k1 ;
		wdt:P2325 ?w1 .
}
SIMILARITY JOIN ON (?e1 ?f1 ?x1 ?k1 ?w1) (?e2 ?f2 ?x2 ?k2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?f2 ?x2 ?k2 ?w2 WHERE {
	?id2 wdt:P2244 ?e2 ;
		wdt:P2045 ?f2 ;
		wdt:P2248 ?x2 ;
		wdt:P2243 ?k2 ;
		wdt:P2325 ?w2 .
}
