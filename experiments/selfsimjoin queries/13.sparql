PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?z1 ?j1 ?p1 ?i1 WHERE {
	?id1 wdt:P1457 ?e1 ;
		wdt:P1096 ?z1 ;
		wdt:P2233 ?j1 ;
		wdt:P2248 ?p1 ;
		wdt:P2243 ?i1 .
}
SIMILARITY JOIN ON (?e1 ?z1 ?j1 ?p1 ?i1) (?e2 ?z2 ?j2 ?p2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?z2 ?j2 ?p2 ?i2 WHERE {
	?id2 wdt:P1457 ?e2 ;
		wdt:P1096 ?z2 ;
		wdt:P2233 ?j2 ;
		wdt:P2248 ?p2 ;
		wdt:P2243 ?i2 .
}
