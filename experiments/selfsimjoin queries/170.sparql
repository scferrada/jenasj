PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?e1 ?z1 ?a1 ?s1 WHERE {
	?id1 wdt:P2146 ?q1 ;
		wdt:P2213 ?e1 ;
		wdt:P2045 ?z1 ;
		wdt:P2248 ?a1 ;
		wdt:P2325 ?s1 .
}
SIMILARITY JOIN ON (?q1 ?e1 ?z1 ?a1 ?s1) (?q2 ?e2 ?z2 ?a2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?e2 ?z2 ?a2 ?s2 WHERE {
	?id2 wdt:P2146 ?q2 ;
		wdt:P2213 ?e2 ;
		wdt:P2045 ?z2 ;
		wdt:P2248 ?a2 ;
		wdt:P2325 ?s2 .
}
