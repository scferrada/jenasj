PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?i1 ?w1 ?v1 WHERE {
	?id1 wdt:P2244 ?m1 ;
		wdt:P1096 ?i1 ;
		wdt:P2233 ?w1 ;
		wdt:P2243 ?v1 .
}
SIMILARITY JOIN ON (?m1 ?i1 ?w1 ?v1) (?m2 ?i2 ?w2 ?v2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?i2 ?w2 ?v2 WHERE {
	?id2 wdt:P2244 ?m2 ;
		wdt:P1096 ?i2 ;
		wdt:P2233 ?w2 ;
		wdt:P2243 ?v2 .
}
