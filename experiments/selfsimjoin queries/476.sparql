PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?q1 ?z1 ?s1 ?f1 ?n1 WHERE {
	?id1 wdt:P2248 ?h1 ;
		wdt:P2244 ?q1 ;
		wdt:P2243 ?z1 ;
		wdt:P2213 ?s1 ;
		wdt:P1096 ?f1 ;
		wdt:P2325 ?n1 .
}
SIMILARITY JOIN ON (?h1 ?q1 ?z1 ?s1 ?f1 ?n1) (?h2 ?q2 ?z2 ?s2 ?f2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?q2 ?z2 ?s2 ?f2 ?n2 WHERE {
	?id2 wdt:P2248 ?h2 ;
		wdt:P2244 ?q2 ;
		wdt:P2243 ?z2 ;
		wdt:P2213 ?s2 ;
		wdt:P1096 ?f2 ;
		wdt:P2325 ?n2 .
}
