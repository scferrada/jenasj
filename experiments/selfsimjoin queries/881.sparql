PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?u1 ?w1 ?r1 WHERE {
	?id1 wdt:P1148 ?u1 ;
		wdt:P2160 ?w1 ;
		wdt:P2114 ?r1 .
}
SIMILARITY JOIN ON (?u1 ?w1 ?r1) (?u2 ?w2 ?r2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?u2 ?w2 ?r2 WHERE {
	?id2 wdt:P1148 ?u2 ;
		wdt:P2160 ?w2 ;
		wdt:P2114 ?r2 .
}
