PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?l1 ?b1 ?w1 ?q1 ?z1 WHERE {
	?id1 wdt:P2146 ?t1 ;
		wdt:P1457 ?l1 ;
		wdt:P2248 ?b1 ;
		wdt:P2243 ?w1 ;
		wdt:P2325 ?q1 ;
		wdt:P2233 ?z1 .
}
SIMILARITY JOIN ON (?t1 ?l1 ?b1 ?w1 ?q1 ?z1) (?t2 ?l2 ?b2 ?w2 ?q2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?l2 ?b2 ?w2 ?q2 ?z2 WHERE {
	?id2 wdt:P2146 ?t2 ;
		wdt:P1457 ?l2 ;
		wdt:P2248 ?b2 ;
		wdt:P2243 ?w2 ;
		wdt:P2325 ?q2 ;
		wdt:P2233 ?z2 .
}
