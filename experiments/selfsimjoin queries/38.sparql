PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?h1 ?y1 ?t1 WHERE {
	?id1 wdt:P1457 ?x1 ;
		wdt:P1096 ?h1 ;
		wdt:P2045 ?y1 ;
		wdt:P2243 ?t1 .
}
SIMILARITY JOIN ON (?x1 ?h1 ?y1 ?t1) (?x2 ?h2 ?y2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?h2 ?y2 ?t2 WHERE {
	?id2 wdt:P1457 ?x2 ;
		wdt:P1096 ?h2 ;
		wdt:P2045 ?y2 ;
		wdt:P2243 ?t2 .
}
