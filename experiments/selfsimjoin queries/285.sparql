PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?t1 ?i1 ?v1 ?o1 WHERE {
	?id1 wdt:P1086 ?j1 ;
		wdt:P2154 ?t1 ;
		wdt:P1123 ?i1 ;
		wdt:P1148 ?v1 ;
		wdt:P2114 ?o1 .
}
SIMILARITY JOIN ON (?j1 ?t1 ?i1 ?v1 ?o1) (?j2 ?t2 ?i2 ?v2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?t2 ?i2 ?v2 ?o2 WHERE {
	?id2 wdt:P1086 ?j2 ;
		wdt:P2154 ?t2 ;
		wdt:P1123 ?i2 ;
		wdt:P1148 ?v2 ;
		wdt:P2114 ?o2 .
}
