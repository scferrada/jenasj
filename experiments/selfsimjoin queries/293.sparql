PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?n1 ?g1 ?p1 ?m1 ?c1 ?s1 WHERE {
	?id1 wdt:P2146 ?n1 ;
		wdt:P4501 ?g1 ;
		wdt:P2243 ?p1 ;
		wdt:P2045 ?m1 ;
		wdt:P2233 ?c1 ;
		wdt:P2248 ?s1 .
}
SIMILARITY JOIN ON (?n1 ?g1 ?p1 ?m1 ?c1 ?s1) (?n2 ?g2 ?p2 ?m2 ?c2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?n2 ?g2 ?p2 ?m2 ?c2 ?s2 WHERE {
	?id2 wdt:P2146 ?n2 ;
		wdt:P4501 ?g2 ;
		wdt:P2243 ?p2 ;
		wdt:P2045 ?m2 ;
		wdt:P2233 ?c2 ;
		wdt:P2248 ?s2 .
}
