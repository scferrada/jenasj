PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?a1 ?w1 ?b1 ?d1 ?z1 WHERE {
	?id1 wdt:P2146 ?m1 ;
		wdt:P4501 ?a1 ;
		wdt:P2243 ?w1 ;
		wdt:P2213 ?b1 ;
		wdt:P2325 ?d1 ;
		wdt:P2233 ?z1 .
}
SIMILARITY JOIN ON (?m1 ?a1 ?w1 ?b1 ?d1 ?z1) (?m2 ?a2 ?w2 ?b2 ?d2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?a2 ?w2 ?b2 ?d2 ?z2 WHERE {
	?id2 wdt:P2146 ?m2 ;
		wdt:P4501 ?a2 ;
		wdt:P2243 ?w2 ;
		wdt:P2213 ?b2 ;
		wdt:P2325 ?d2 ;
		wdt:P2233 ?z2 .
}
