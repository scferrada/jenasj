PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?o1 ?l1 ?d1 ?e1 ?s1 WHERE {
	?id1 wdt:P2244 ?o1 ;
		wdt:P2233 ?l1 ;
		wdt:P2213 ?d1 ;
		wdt:P2248 ?e1 ;
		wdt:P4501 ?s1 .
}
SIMILARITY JOIN ON (?o1 ?l1 ?d1 ?e1 ?s1) (?o2 ?l2 ?d2 ?e2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?o2 ?l2 ?d2 ?e2 ?s2 WHERE {
	?id2 wdt:P2244 ?o2 ;
		wdt:P2233 ?l2 ;
		wdt:P2213 ?d2 ;
		wdt:P2248 ?e2 ;
		wdt:P4501 ?s2 .
}
