PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?n1 ?e1 ?q1 WHERE {
	?id1 wdt:P1457 ?n1 ;
		wdt:P2045 ?e1 ;
		wdt:P2243 ?q1 .
}
SIMILARITY JOIN ON (?n1 ?e1 ?q1) (?n2 ?e2 ?q2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?n2 ?e2 ?q2 WHERE {
	?id2 wdt:P1457 ?n2 ;
		wdt:P2045 ?e2 ;
		wdt:P2243 ?q2 .
}
