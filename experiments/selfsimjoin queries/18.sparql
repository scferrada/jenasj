PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?r1 ?i1 ?o1 ?t1 ?p1 WHERE {
	?id1 wdt:P1457 ?q1 ;
		wdt:P4501 ?r1 ;
		wdt:P2244 ?i1 ;
		wdt:P2243 ?o1 ;
		wdt:P2045 ?t1 ;
		wdt:P2325 ?p1 .
}
SIMILARITY JOIN ON (?q1 ?r1 ?i1 ?o1 ?t1 ?p1) (?q2 ?r2 ?i2 ?o2 ?t2 ?p2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?r2 ?i2 ?o2 ?t2 ?p2 WHERE {
	?id2 wdt:P1457 ?q2 ;
		wdt:P4501 ?r2 ;
		wdt:P2244 ?i2 ;
		wdt:P2243 ?o2 ;
		wdt:P2045 ?t2 ;
		wdt:P2325 ?p2 .
}
