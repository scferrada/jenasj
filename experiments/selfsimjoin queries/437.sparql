PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?b1 ?d1 ?i1 ?h1 ?w1 WHERE {
	?id1 wdt:P2146 ?x1 ;
		wdt:P1457 ?b1 ;
		wdt:P2244 ?d1 ;
		wdt:P2243 ?i1 ;
		wdt:P1096 ?h1 ;
		wdt:P2325 ?w1 .
}
SIMILARITY JOIN ON (?x1 ?b1 ?d1 ?i1 ?h1 ?w1) (?x2 ?b2 ?d2 ?i2 ?h2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?b2 ?d2 ?i2 ?h2 ?w2 WHERE {
	?id2 wdt:P2146 ?x2 ;
		wdt:P1457 ?b2 ;
		wdt:P2244 ?d2 ;
		wdt:P2243 ?i2 ;
		wdt:P1096 ?h2 ;
		wdt:P2325 ?w2 .
}
