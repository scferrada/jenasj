PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?z1 ?e1 ?a1 ?l1 WHERE {
	?id1 wdt:P2146 ?t1 ;
		wdt:P2244 ?z1 ;
		wdt:P1457 ?e1 ;
		wdt:P2248 ?a1 ;
		wdt:P2045 ?l1 .
}
SIMILARITY JOIN ON (?t1 ?z1 ?e1 ?a1 ?l1) (?t2 ?z2 ?e2 ?a2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?z2 ?e2 ?a2 ?l2 WHERE {
	?id2 wdt:P2146 ?t2 ;
		wdt:P2244 ?z2 ;
		wdt:P1457 ?e2 ;
		wdt:P2248 ?a2 ;
		wdt:P2045 ?l2 .
}
