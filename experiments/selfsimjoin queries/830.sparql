PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?b1 ?s1 ?t1 ?z1 WHERE {
	?id1 wdt:P2213 ?p1 ;
		wdt:P2045 ?b1 ;
		wdt:P1096 ?s1 ;
		wdt:P2248 ?t1 ;
		wdt:P4501 ?z1 .
}
SIMILARITY JOIN ON (?p1 ?b1 ?s1 ?t1 ?z1) (?p2 ?b2 ?s2 ?t2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?b2 ?s2 ?t2 ?z2 WHERE {
	?id2 wdt:P2213 ?p2 ;
		wdt:P2045 ?b2 ;
		wdt:P1096 ?s2 ;
		wdt:P2248 ?t2 ;
		wdt:P4501 ?z2 .
}
