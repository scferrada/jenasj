PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?e1 ?d1 ?g1 ?a1 ?o1 WHERE {
	?id1 wdt:P2248 ?l1 ;
		wdt:P2244 ?e1 ;
		wdt:P2243 ?d1 ;
		wdt:P2213 ?g1 ;
		wdt:P1096 ?a1 ;
		wdt:P2233 ?o1 .
}
SIMILARITY JOIN ON (?l1 ?e1 ?d1 ?g1 ?a1 ?o1) (?l2 ?e2 ?d2 ?g2 ?a2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?e2 ?d2 ?g2 ?a2 ?o2 WHERE {
	?id2 wdt:P2248 ?l2 ;
		wdt:P2244 ?e2 ;
		wdt:P2243 ?d2 ;
		wdt:P2213 ?g2 ;
		wdt:P1096 ?a2 ;
		wdt:P2233 ?o2 .
}
