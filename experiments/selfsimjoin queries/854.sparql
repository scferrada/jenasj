PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?v1 ?b1 ?g1 ?i1 WHERE {
	?id1 wdt:P2213 ?s1 ;
		wdt:P1457 ?v1 ;
		wdt:P1096 ?b1 ;
		wdt:P2233 ?g1 ;
		wdt:P2243 ?i1 .
}
SIMILARITY JOIN ON (?s1 ?v1 ?b1 ?g1 ?i1) (?s2 ?v2 ?b2 ?g2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?v2 ?b2 ?g2 ?i2 WHERE {
	?id2 wdt:P2213 ?s2 ;
		wdt:P1457 ?v2 ;
		wdt:P1096 ?b2 ;
		wdt:P2233 ?g2 ;
		wdt:P2243 ?i2 .
}
