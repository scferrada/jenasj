PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?m1 ?u1 ?z1 ?x1 ?j1 WHERE {
	?id1 wdt:P1457 ?s1 ;
		wdt:P2248 ?m1 ;
		wdt:P2244 ?u1 ;
		wdt:P2243 ?z1 ;
		wdt:P2213 ?x1 ;
		wdt:P2045 ?j1 .
}
SIMILARITY JOIN ON (?s1 ?m1 ?u1 ?z1 ?x1 ?j1) (?s2 ?m2 ?u2 ?z2 ?x2 ?j2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?m2 ?u2 ?z2 ?x2 ?j2 WHERE {
	?id2 wdt:P1457 ?s2 ;
		wdt:P2248 ?m2 ;
		wdt:P2244 ?u2 ;
		wdt:P2243 ?z2 ;
		wdt:P2213 ?x2 ;
		wdt:P2045 ?j2 .
}
