PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?w1 ?j1 ?i1 ?f1 WHERE {
	?id1 wdt:P2146 ?w1 ;
		wdt:P2244 ?j1 ;
		wdt:P2045 ?i1 ;
		wdt:P2213 ?f1 .
}
SIMILARITY JOIN ON (?w1 ?j1 ?i1 ?f1) (?w2 ?j2 ?i2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?w2 ?j2 ?i2 ?f2 WHERE {
	?id2 wdt:P2146 ?w2 ;
		wdt:P2244 ?j2 ;
		wdt:P2045 ?i2 ;
		wdt:P2213 ?f2 .
}
