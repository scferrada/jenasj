PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?c1 ?k1 ?q1 ?l1 ?e1 WHERE {
	?id1 wdt:P2244 ?c1 ;
		wdt:P1457 ?k1 ;
		wdt:P1096 ?q1 ;
		wdt:P2045 ?l1 ;
		wdt:P2243 ?e1 .
}
SIMILARITY JOIN ON (?c1 ?k1 ?q1 ?l1 ?e1) (?c2 ?k2 ?q2 ?l2 ?e2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?c2 ?k2 ?q2 ?l2 ?e2 WHERE {
	?id2 wdt:P2244 ?c2 ;
		wdt:P1457 ?k2 ;
		wdt:P1096 ?q2 ;
		wdt:P2045 ?l2 ;
		wdt:P2243 ?e2 .
}
