PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?t1 ?g1 WHERE {
	?id1 wdt:P2146 ?p1 ;
		wdt:P2213 ?t1 ;
		wdt:P2325 ?g1 .
}
SIMILARITY JOIN ON (?p1 ?t1 ?g1) (?p2 ?t2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?t2 ?g2 WHERE {
	?id2 wdt:P2146 ?p2 ;
		wdt:P2213 ?t2 ;
		wdt:P2325 ?g2 .
}
