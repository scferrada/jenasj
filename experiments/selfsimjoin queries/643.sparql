PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?p1 ?h1 WHERE {
	?id1 wdt:P2154 ?x1 ;
		wdt:P1123 ?p1 ;
		wdt:P2160 ?h1 .
}
SIMILARITY JOIN ON (?x1 ?p1 ?h1) (?x2 ?p2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?p2 ?h2 WHERE {
	?id2 wdt:P2154 ?x2 ;
		wdt:P1123 ?p2 ;
		wdt:P2160 ?h2 .
}
