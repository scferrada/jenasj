PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?y1 ?q1 ?g1 ?o1 WHERE {
	?id1 wdt:P2146 ?t1 ;
		wdt:P2213 ?y1 ;
		wdt:P1096 ?q1 ;
		wdt:P2248 ?g1 ;
		wdt:P4501 ?o1 .
}
SIMILARITY JOIN ON (?t1 ?y1 ?q1 ?g1 ?o1) (?t2 ?y2 ?q2 ?g2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?y2 ?q2 ?g2 ?o2 WHERE {
	?id2 wdt:P2146 ?t2 ;
		wdt:P2213 ?y2 ;
		wdt:P1096 ?q2 ;
		wdt:P2248 ?g2 ;
		wdt:P4501 ?o2 .
}
