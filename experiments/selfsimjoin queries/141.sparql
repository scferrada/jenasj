PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?t1 ?z1 ?l1 ?a1 WHERE {
	?id1 wdt:P2146 ?j1 ;
		wdt:P1457 ?t1 ;
		wdt:P1096 ?z1 ;
		wdt:P2233 ?l1 ;
		wdt:P2045 ?a1 .
}
SIMILARITY JOIN ON (?j1 ?t1 ?z1 ?l1 ?a1) (?j2 ?t2 ?z2 ?l2 ?a2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?t2 ?z2 ?l2 ?a2 WHERE {
	?id2 wdt:P2146 ?j2 ;
		wdt:P1457 ?t2 ;
		wdt:P1096 ?z2 ;
		wdt:P2233 ?l2 ;
		wdt:P2045 ?a2 .
}
