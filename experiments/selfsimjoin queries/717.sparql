PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?g1 ?h1 ?n1 ?b1 ?j1 WHERE {
	?id1 wdt:P2146 ?g1 ;
		wdt:P2045 ?h1 ;
		wdt:P2325 ?n1 ;
		wdt:P2248 ?b1 ;
		wdt:P4501 ?j1 .
}
SIMILARITY JOIN ON (?g1 ?h1 ?n1 ?b1 ?j1) (?g2 ?h2 ?n2 ?b2 ?j2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?g2 ?h2 ?n2 ?b2 ?j2 WHERE {
	?id2 wdt:P2146 ?g2 ;
		wdt:P2045 ?h2 ;
		wdt:P2325 ?n2 ;
		wdt:P2248 ?b2 ;
		wdt:P4501 ?j2 .
}
