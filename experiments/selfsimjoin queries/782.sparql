PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?a1 ?b1 ?f1 WHERE {
	?id1 wdt:P2244 ?a1 ;
		wdt:P2233 ?b1 ;
		wdt:P2325 ?f1 .
}
SIMILARITY JOIN ON (?a1 ?b1 ?f1) (?a2 ?b2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?a2 ?b2 ?f2 WHERE {
	?id2 wdt:P2244 ?a2 ;
		wdt:P2233 ?b2 ;
		wdt:P2325 ?f2 .
}
