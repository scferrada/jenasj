PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?o1 ?a1 ?m1 ?d1 ?i1 WHERE {
	?id1 wdt:P2213 ?o1 ;
		wdt:P2045 ?a1 ;
		wdt:P4501 ?m1 ;
		wdt:P2243 ?d1 ;
		wdt:P2325 ?i1 .
}
SIMILARITY JOIN ON (?o1 ?a1 ?m1 ?d1 ?i1) (?o2 ?a2 ?m2 ?d2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?o2 ?a2 ?m2 ?d2 ?i2 WHERE {
	?id2 wdt:P2213 ?o2 ;
		wdt:P2045 ?a2 ;
		wdt:P4501 ?m2 ;
		wdt:P2243 ?d2 ;
		wdt:P2325 ?i2 .
}
