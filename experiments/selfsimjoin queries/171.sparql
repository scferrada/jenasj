PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?u1 ?p1 ?d1 ?y1 WHERE {
	?id1 wdt:P2146 ?u1 ;
		wdt:P1457 ?p1 ;
		wdt:P1096 ?d1 ;
		wdt:P4501 ?y1 .
}
SIMILARITY JOIN ON (?u1 ?p1 ?d1 ?y1) (?u2 ?p2 ?d2 ?y2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?u2 ?p2 ?d2 ?y2 WHERE {
	?id2 wdt:P2146 ?u2 ;
		wdt:P1457 ?p2 ;
		wdt:P1096 ?d2 ;
		wdt:P4501 ?y2 .
}
