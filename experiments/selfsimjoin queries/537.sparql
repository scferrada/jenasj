PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?f1 ?b1 ?d1 ?p1 ?r1 WHERE {
	?id1 wdt:P2146 ?f1 ;
		wdt:P1457 ?b1 ;
		wdt:P2233 ?d1 ;
		wdt:P2243 ?p1 ;
		wdt:P2325 ?r1 .
}
SIMILARITY JOIN ON (?f1 ?b1 ?d1 ?p1 ?r1) (?f2 ?b2 ?d2 ?p2 ?r2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?f2 ?b2 ?d2 ?p2 ?r2 WHERE {
	?id2 wdt:P2146 ?f2 ;
		wdt:P1457 ?b2 ;
		wdt:P2233 ?d2 ;
		wdt:P2243 ?p2 ;
		wdt:P2325 ?r2 .
}
