PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?n1 ?h1 ?r1 ?d1 ?z1 WHERE {
	?id1 wdt:P2213 ?n1 ;
		wdt:P1457 ?h1 ;
		wdt:P4501 ?r1 ;
		wdt:P2243 ?d1 ;
		wdt:P2248 ?z1 .
}
SIMILARITY JOIN ON (?n1 ?h1 ?r1 ?d1 ?z1) (?n2 ?h2 ?r2 ?d2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?n2 ?h2 ?r2 ?d2 ?z2 WHERE {
	?id2 wdt:P2213 ?n2 ;
		wdt:P1457 ?h2 ;
		wdt:P4501 ?r2 ;
		wdt:P2243 ?d2 ;
		wdt:P2248 ?z2 .
}
