PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?i1 ?f1 ?p1 WHERE {
	?id1 wdt:P2213 ?d1 ;
		wdt:P1457 ?i1 ;
		wdt:P2045 ?f1 ;
		wdt:P2325 ?p1 .
}
SIMILARITY JOIN ON (?d1 ?i1 ?f1 ?p1) (?d2 ?i2 ?f2 ?p2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?i2 ?f2 ?p2 WHERE {
	?id2 wdt:P2213 ?d2 ;
		wdt:P1457 ?i2 ;
		wdt:P2045 ?f2 ;
		wdt:P2325 ?p2 .
}
