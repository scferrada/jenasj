PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?f1 ?z1 ?b1 ?d1 WHERE {
	?id1 wdt:P2213 ?r1 ;
		wdt:P2233 ?f1 ;
		wdt:P2248 ?z1 ;
		wdt:P2243 ?b1 ;
		wdt:P4501 ?d1 .
}
SIMILARITY JOIN ON (?r1 ?f1 ?z1 ?b1 ?d1) (?r2 ?f2 ?z2 ?b2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?f2 ?z2 ?b2 ?d2 WHERE {
	?id2 wdt:P2213 ?r2 ;
		wdt:P2233 ?f2 ;
		wdt:P2248 ?z2 ;
		wdt:P2243 ?b2 ;
		wdt:P4501 ?d2 .
}
