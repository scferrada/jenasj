PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?n1 ?f1 ?i1 WHERE {
	?id1 wdt:P2244 ?r1 ;
		wdt:P2233 ?n1 ;
		wdt:P2248 ?f1 ;
		wdt:P4501 ?i1 .
}
SIMILARITY JOIN ON (?r1 ?n1 ?f1 ?i1) (?r2 ?n2 ?f2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?n2 ?f2 ?i2 WHERE {
	?id2 wdt:P2244 ?r2 ;
		wdt:P2233 ?n2 ;
		wdt:P2248 ?f2 ;
		wdt:P4501 ?i2 .
}
