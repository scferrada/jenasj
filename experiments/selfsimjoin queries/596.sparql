PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?w1 ?e1 ?p1 ?c1 ?o1 WHERE {
	?id1 wdt:P2244 ?w1 ;
		wdt:P2045 ?e1 ;
		wdt:P2233 ?p1 ;
		wdt:P2243 ?c1 ;
		wdt:P2213 ?o1 .
}
SIMILARITY JOIN ON (?w1 ?e1 ?p1 ?c1 ?o1) (?w2 ?e2 ?p2 ?c2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?w2 ?e2 ?p2 ?c2 ?o2 WHERE {
	?id2 wdt:P2244 ?w2 ;
		wdt:P2045 ?e2 ;
		wdt:P2233 ?p2 ;
		wdt:P2243 ?c2 ;
		wdt:P2213 ?o2 .
}
