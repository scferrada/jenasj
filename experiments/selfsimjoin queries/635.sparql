PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?k1 ?s1 WHERE {
	?id1 wdt:P2213 ?v1 ;
		wdt:P1457 ?k1 ;
		wdt:P4501 ?s1 .
}
SIMILARITY JOIN ON (?v1 ?k1 ?s1) (?v2 ?k2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?k2 ?s2 WHERE {
	?id2 wdt:P2213 ?v2 ;
		wdt:P1457 ?k2 ;
		wdt:P4501 ?s2 .
}
