PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?w1 ?c1 ?k1 ?l1 WHERE {
	?id1 wdt:P2146 ?w1 ;
		wdt:P2213 ?c1 ;
		wdt:P2045 ?k1 ;
		wdt:P2248 ?l1 .
}
SIMILARITY JOIN ON (?w1 ?c1 ?k1 ?l1) (?w2 ?c2 ?k2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?w2 ?c2 ?k2 ?l2 WHERE {
	?id2 wdt:P2146 ?w2 ;
		wdt:P2213 ?c2 ;
		wdt:P2045 ?k2 ;
		wdt:P2248 ?l2 .
}
