PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?z1 ?t1 ?e1 ?j1 ?m1 ?v1 WHERE {
	?id1 wdt:P2146 ?z1 ;
		wdt:P1457 ?t1 ;
		wdt:P2248 ?e1 ;
		wdt:P2244 ?j1 ;
		wdt:P2213 ?m1 ;
		wdt:P2233 ?v1 .
}
SIMILARITY JOIN ON (?z1 ?t1 ?e1 ?j1 ?m1 ?v1) (?z2 ?t2 ?e2 ?j2 ?m2 ?v2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?z2 ?t2 ?e2 ?j2 ?m2 ?v2 WHERE {
	?id2 wdt:P2146 ?z2 ;
		wdt:P1457 ?t2 ;
		wdt:P2248 ?e2 ;
		wdt:P2244 ?j2 ;
		wdt:P2213 ?m2 ;
		wdt:P2233 ?v2 .
}
