PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?e1 ?p1 ?w1 WHERE {
	?id1 wdt:P2244 ?q1 ;
		wdt:P1457 ?e1 ;
		wdt:P4501 ?p1 ;
		wdt:P2045 ?w1 .
}
SIMILARITY JOIN ON (?q1 ?e1 ?p1 ?w1) (?q2 ?e2 ?p2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?e2 ?p2 ?w2 WHERE {
	?id2 wdt:P2244 ?q2 ;
		wdt:P1457 ?e2 ;
		wdt:P4501 ?p2 ;
		wdt:P2045 ?w2 .
}
