PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?w1 ?p1 ?g1 ?n1 WHERE {
	?id1 wdt:P2146 ?w1 ;
		wdt:P2213 ?p1 ;
		wdt:P1457 ?g1 ;
		wdt:P4501 ?n1 .
}
SIMILARITY JOIN ON (?w1 ?p1 ?g1 ?n1) (?w2 ?p2 ?g2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?w2 ?p2 ?g2 ?n2 WHERE {
	?id2 wdt:P2146 ?w2 ;
		wdt:P2213 ?p2 ;
		wdt:P1457 ?g2 ;
		wdt:P4501 ?n2 .
}
