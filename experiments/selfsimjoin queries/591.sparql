PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?r1 ?g1 ?q1 ?h1 WHERE {
	?id1 wdt:P2244 ?x1 ;
		wdt:P2045 ?r1 ;
		wdt:P2213 ?g1 ;
		wdt:P4501 ?q1 ;
		wdt:P2325 ?h1 .
}
SIMILARITY JOIN ON (?x1 ?r1 ?g1 ?q1 ?h1) (?x2 ?r2 ?g2 ?q2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?r2 ?g2 ?q2 ?h2 WHERE {
	?id2 wdt:P2244 ?x2 ;
		wdt:P2045 ?r2 ;
		wdt:P2213 ?g2 ;
		wdt:P4501 ?q2 ;
		wdt:P2325 ?h2 .
}
