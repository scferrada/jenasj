PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?o1 ?e1 ?l1 WHERE {
	?id1 wdt:P2244 ?s1 ;
		wdt:P2248 ?o1 ;
		wdt:P2243 ?e1 ;
		wdt:P2213 ?l1 .
}
SIMILARITY JOIN ON (?s1 ?o1 ?e1 ?l1) (?s2 ?o2 ?e2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?o2 ?e2 ?l2 WHERE {
	?id2 wdt:P2244 ?s2 ;
		wdt:P2248 ?o2 ;
		wdt:P2243 ?e2 ;
		wdt:P2213 ?l2 .
}
