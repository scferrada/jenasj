PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?u1 ?c1 ?v1 ?z1 ?o1 ?s1 WHERE {
	?id1 wdt:P2248 ?u1 ;
		wdt:P2243 ?c1 ;
		wdt:P2213 ?v1 ;
		wdt:P2325 ?z1 ;
		wdt:P2233 ?o1 ;
		wdt:P4501 ?s1 .
}
SIMILARITY JOIN ON (?u1 ?c1 ?v1 ?z1 ?o1 ?s1) (?u2 ?c2 ?v2 ?z2 ?o2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?u2 ?c2 ?v2 ?z2 ?o2 ?s2 WHERE {
	?id2 wdt:P2248 ?u2 ;
		wdt:P2243 ?c2 ;
		wdt:P2213 ?v2 ;
		wdt:P2325 ?z2 ;
		wdt:P2233 ?o2 ;
		wdt:P4501 ?s2 .
}
