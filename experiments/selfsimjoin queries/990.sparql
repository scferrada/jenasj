PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?c1 ?t1 ?i1 ?o1 ?e1 WHERE {
	?id1 wdt:P2213 ?c1 ;
		wdt:P1457 ?t1 ;
		wdt:P1096 ?i1 ;
		wdt:P4501 ?o1 ;
		wdt:P2243 ?e1 .
}
SIMILARITY JOIN ON (?c1 ?t1 ?i1 ?o1 ?e1) (?c2 ?t2 ?i2 ?o2 ?e2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?c2 ?t2 ?i2 ?o2 ?e2 WHERE {
	?id2 wdt:P2213 ?c2 ;
		wdt:P1457 ?t2 ;
		wdt:P1096 ?i2 ;
		wdt:P4501 ?o2 ;
		wdt:P2243 ?e2 .
}
