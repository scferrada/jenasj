PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?p1 ?t1 ?r1 ?n1 WHERE {
	?id1 wdt:P2146 ?i1 ;
		wdt:P4501 ?p1 ;
		wdt:P1096 ?t1 ;
		wdt:P2248 ?r1 ;
		wdt:P2325 ?n1 .
}
SIMILARITY JOIN ON (?i1 ?p1 ?t1 ?r1 ?n1) (?i2 ?p2 ?t2 ?r2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?p2 ?t2 ?r2 ?n2 WHERE {
	?id2 wdt:P2146 ?i2 ;
		wdt:P4501 ?p2 ;
		wdt:P1096 ?t2 ;
		wdt:P2248 ?r2 ;
		wdt:P2325 ?n2 .
}
