PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?r1 ?b1 WHERE {
	?id1 wdt:P1457 ?m1 ;
		wdt:P1096 ?r1 ;
		wdt:P4501 ?b1 .
}
SIMILARITY JOIN ON (?m1 ?r1 ?b1) (?m2 ?r2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?r2 ?b2 WHERE {
	?id2 wdt:P1457 ?m2 ;
		wdt:P1096 ?r2 ;
		wdt:P4501 ?b2 .
}
