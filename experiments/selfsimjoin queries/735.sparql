PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?z1 ?c1 ?j1 ?t1 WHERE {
	?id1 wdt:P2146 ?e1 ;
		wdt:P2244 ?z1 ;
		wdt:P4501 ?c1 ;
		wdt:P2243 ?j1 ;
		wdt:P2213 ?t1 .
}
SIMILARITY JOIN ON (?e1 ?z1 ?c1 ?j1 ?t1) (?e2 ?z2 ?c2 ?j2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?z2 ?c2 ?j2 ?t2 WHERE {
	?id2 wdt:P2146 ?e2 ;
		wdt:P2244 ?z2 ;
		wdt:P4501 ?c2 ;
		wdt:P2243 ?j2 ;
		wdt:P2213 ?t2 .
}
