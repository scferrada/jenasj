PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?f1 ?m1 ?c1 ?b1 ?u1 WHERE {
	?id1 wdt:P2213 ?f1 ;
		wdt:P1457 ?m1 ;
		wdt:P1096 ?c1 ;
		wdt:P2248 ?b1 ;
		wdt:P2325 ?u1 .
}
SIMILARITY JOIN ON (?f1 ?m1 ?c1 ?b1 ?u1) (?f2 ?m2 ?c2 ?b2 ?u2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?f2 ?m2 ?c2 ?b2 ?u2 WHERE {
	?id2 wdt:P2213 ?f2 ;
		wdt:P1457 ?m2 ;
		wdt:P1096 ?c2 ;
		wdt:P2248 ?b2 ;
		wdt:P2325 ?u2 .
}
