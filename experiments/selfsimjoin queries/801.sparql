PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?k1 ?u1 ?o1 ?w1 ?n1 WHERE {
	?id1 wdt:P2244 ?i1 ;
		wdt:P2243 ?k1 ;
		wdt:P2213 ?u1 ;
		wdt:P2045 ?o1 ;
		wdt:P2325 ?w1 ;
		wdt:P2233 ?n1 .
}
SIMILARITY JOIN ON (?i1 ?k1 ?u1 ?o1 ?w1 ?n1) (?i2 ?k2 ?u2 ?o2 ?w2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?k2 ?u2 ?o2 ?w2 ?n2 WHERE {
	?id2 wdt:P2244 ?i2 ;
		wdt:P2243 ?k2 ;
		wdt:P2213 ?u2 ;
		wdt:P2045 ?o2 ;
		wdt:P2325 ?w2 ;
		wdt:P2233 ?n2 .
}
