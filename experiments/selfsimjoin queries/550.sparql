PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?k1 ?y1 ?n1 WHERE {
	?id1 wdt:P2146 ?e1 ;
		wdt:P2244 ?k1 ;
		wdt:P2045 ?y1 ;
		wdt:P2248 ?n1 .
}
SIMILARITY JOIN ON (?e1 ?k1 ?y1 ?n1) (?e2 ?k2 ?y2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?k2 ?y2 ?n2 WHERE {
	?id2 wdt:P2146 ?e2 ;
		wdt:P2244 ?k2 ;
		wdt:P2045 ?y2 ;
		wdt:P2248 ?n2 .
}
