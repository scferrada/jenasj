PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?z1 ?i1 ?j1 WHERE {
	?id1 wdt:P2244 ?h1 ;
		wdt:P2213 ?z1 ;
		wdt:P2243 ?i1 ;
		wdt:P2325 ?j1 .
}
SIMILARITY JOIN ON (?h1 ?z1 ?i1 ?j1) (?h2 ?z2 ?i2 ?j2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?z2 ?i2 ?j2 WHERE {
	?id2 wdt:P2244 ?h2 ;
		wdt:P2213 ?z2 ;
		wdt:P2243 ?i2 ;
		wdt:P2325 ?j2 .
}
