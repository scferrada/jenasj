PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?z1 ?q1 ?i1 ?a1 ?j1 ?y1 WHERE {
	?id1 wdt:P2146 ?z1 ;
		wdt:P1457 ?q1 ;
		wdt:P2248 ?i1 ;
		wdt:P2244 ?a1 ;
		wdt:P2045 ?j1 ;
		wdt:P2233 ?y1 .
}
SIMILARITY JOIN ON (?z1 ?q1 ?i1 ?a1 ?j1 ?y1) (?z2 ?q2 ?i2 ?a2 ?j2 ?y2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?z2 ?q2 ?i2 ?a2 ?j2 ?y2 WHERE {
	?id2 wdt:P2146 ?z2 ;
		wdt:P1457 ?q2 ;
		wdt:P2248 ?i2 ;
		wdt:P2244 ?a2 ;
		wdt:P2045 ?j2 ;
		wdt:P2233 ?y2 .
}
