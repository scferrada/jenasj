PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?d1 ?a1 ?u1 WHERE {
	?id1 wdt:P1457 ?h1 ;
		wdt:P2325 ?d1 ;
		wdt:P4501 ?a1 ;
		wdt:P2045 ?u1 .
}
SIMILARITY JOIN ON (?h1 ?d1 ?a1 ?u1) (?h2 ?d2 ?a2 ?u2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?d2 ?a2 ?u2 WHERE {
	?id2 wdt:P1457 ?h2 ;
		wdt:P2325 ?d2 ;
		wdt:P4501 ?a2 ;
		wdt:P2045 ?u2 .
}
