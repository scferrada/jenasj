PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?n1 ?o1 ?z1 ?e1 ?u1 WHERE {
	?id1 wdt:P1457 ?y1 ;
		wdt:P4501 ?n1 ;
		wdt:P2244 ?o1 ;
		wdt:P2045 ?z1 ;
		wdt:P1096 ?e1 ;
		wdt:P2325 ?u1 .
}
SIMILARITY JOIN ON (?y1 ?n1 ?o1 ?z1 ?e1 ?u1) (?y2 ?n2 ?o2 ?z2 ?e2 ?u2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?n2 ?o2 ?z2 ?e2 ?u2 WHERE {
	?id2 wdt:P1457 ?y2 ;
		wdt:P4501 ?n2 ;
		wdt:P2244 ?o2 ;
		wdt:P2045 ?z2 ;
		wdt:P1096 ?e2 ;
		wdt:P2325 ?u2 .
}
