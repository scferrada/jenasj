PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?k1 ?m1 ?f1 ?e1 WHERE {
	?id1 wdt:P2146 ?v1 ;
		wdt:P2244 ?k1 ;
		wdt:P1457 ?m1 ;
		wdt:P2248 ?f1 ;
		wdt:P2243 ?e1 .
}
SIMILARITY JOIN ON (?v1 ?k1 ?m1 ?f1 ?e1) (?v2 ?k2 ?m2 ?f2 ?e2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?k2 ?m2 ?f2 ?e2 WHERE {
	?id2 wdt:P2146 ?v2 ;
		wdt:P2244 ?k2 ;
		wdt:P1457 ?m2 ;
		wdt:P2248 ?f2 ;
		wdt:P2243 ?e2 .
}
