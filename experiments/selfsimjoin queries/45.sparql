PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?m1 ?a1 WHERE {
	?id1 wdt:P2213 ?i1 ;
		wdt:P2248 ?m1 ;
		wdt:P4501 ?a1 .
}
SIMILARITY JOIN ON (?i1 ?m1 ?a1) (?i2 ?m2 ?a2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?m2 ?a2 WHERE {
	?id2 wdt:P2213 ?i2 ;
		wdt:P2248 ?m2 ;
		wdt:P4501 ?a2 .
}
