PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?z1 ?s1 ?e1 ?l1 WHERE {
	?id1 wdt:P2248 ?m1 ;
		wdt:P1457 ?z1 ;
		wdt:P2045 ?s1 ;
		wdt:P2243 ?e1 ;
		wdt:P2325 ?l1 .
}
SIMILARITY JOIN ON (?m1 ?z1 ?s1 ?e1 ?l1) (?m2 ?z2 ?s2 ?e2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?z2 ?s2 ?e2 ?l2 WHERE {
	?id2 wdt:P2248 ?m2 ;
		wdt:P1457 ?z2 ;
		wdt:P2045 ?s2 ;
		wdt:P2243 ?e2 ;
		wdt:P2325 ?l2 .
}
