PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?s1 ?o1 ?k1 WHERE {
	?id1 wdt:P1086 ?v1 ;
		wdt:P2154 ?s1 ;
		wdt:P1123 ?o1 ;
		wdt:P1122 ?k1 .
}
SIMILARITY JOIN ON (?v1 ?s1 ?o1 ?k1) (?v2 ?s2 ?o2 ?k2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?s2 ?o2 ?k2 WHERE {
	?id2 wdt:P1086 ?v2 ;
		wdt:P2154 ?s2 ;
		wdt:P1123 ?o2 ;
		wdt:P1122 ?k2 .
}
