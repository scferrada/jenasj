PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?h1 ?o1 ?d1 WHERE {
	?id1 wdt:P2244 ?t1 ;
		wdt:P1457 ?h1 ;
		wdt:P1096 ?o1 ;
		wdt:P2233 ?d1 .
}
SIMILARITY JOIN ON (?t1 ?h1 ?o1 ?d1) (?t2 ?h2 ?o2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?h2 ?o2 ?d2 WHERE {
	?id2 wdt:P2244 ?t2 ;
		wdt:P1457 ?h2 ;
		wdt:P1096 ?o2 ;
		wdt:P2233 ?d2 .
}
