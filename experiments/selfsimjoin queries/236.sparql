PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?g1 ?k1 ?w1 ?l1 ?v1 WHERE {
	?id1 wdt:P2146 ?g1 ;
		wdt:P2233 ?k1 ;
		wdt:P4501 ?w1 ;
		wdt:P2243 ?l1 ;
		wdt:P2325 ?v1 .
}
SIMILARITY JOIN ON (?g1 ?k1 ?w1 ?l1 ?v1) (?g2 ?k2 ?w2 ?l2 ?v2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?g2 ?k2 ?w2 ?l2 ?v2 WHERE {
	?id2 wdt:P2146 ?g2 ;
		wdt:P2233 ?k2 ;
		wdt:P4501 ?w2 ;
		wdt:P2243 ?l2 ;
		wdt:P2325 ?v2 .
}
