PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?n1 ?l1 ?j1 ?i1 WHERE {
	?id1 wdt:P2213 ?y1 ;
		wdt:P1096 ?n1 ;
		wdt:P2233 ?l1 ;
		wdt:P2248 ?j1 ;
		wdt:P4501 ?i1 .
}
SIMILARITY JOIN ON (?y1 ?n1 ?l1 ?j1 ?i1) (?y2 ?n2 ?l2 ?j2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?n2 ?l2 ?j2 ?i2 WHERE {
	?id2 wdt:P2213 ?y2 ;
		wdt:P1096 ?n2 ;
		wdt:P2233 ?l2 ;
		wdt:P2248 ?j2 ;
		wdt:P4501 ?i2 .
}
