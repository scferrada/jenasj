PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?o1 ?l1 ?n1 WHERE {
	?id1 wdt:P2045 ?j1 ;
		wdt:P1096 ?o1 ;
		wdt:P2248 ?l1 ;
		wdt:P2243 ?n1 .
}
SIMILARITY JOIN ON (?j1 ?o1 ?l1 ?n1) (?j2 ?o2 ?l2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?o2 ?l2 ?n2 WHERE {
	?id2 wdt:P2045 ?j2 ;
		wdt:P1096 ?o2 ;
		wdt:P2248 ?l2 ;
		wdt:P2243 ?n2 .
}
