PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?f1 ?k1 ?o1 ?y1 WHERE {
	?id1 wdt:P2146 ?q1 ;
		wdt:P2045 ?f1 ;
		wdt:P2233 ?k1 ;
		wdt:P4501 ?o1 ;
		wdt:P2325 ?y1 .
}
SIMILARITY JOIN ON (?q1 ?f1 ?k1 ?o1 ?y1) (?q2 ?f2 ?k2 ?o2 ?y2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?f2 ?k2 ?o2 ?y2 WHERE {
	?id2 wdt:P2146 ?q2 ;
		wdt:P2045 ?f2 ;
		wdt:P2233 ?k2 ;
		wdt:P4501 ?o2 ;
		wdt:P2325 ?y2 .
}
