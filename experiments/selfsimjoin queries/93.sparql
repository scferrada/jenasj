PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?c1 ?b1 ?f1 ?h1 ?a1 WHERE {
	?id1 wdt:P2146 ?c1 ;
		wdt:P2213 ?b1 ;
		wdt:P1457 ?f1 ;
		wdt:P4501 ?h1 ;
		wdt:P2325 ?a1 .
}
SIMILARITY JOIN ON (?c1 ?b1 ?f1 ?h1 ?a1) (?c2 ?b2 ?f2 ?h2 ?a2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?c2 ?b2 ?f2 ?h2 ?a2 WHERE {
	?id2 wdt:P2146 ?c2 ;
		wdt:P2213 ?b2 ;
		wdt:P1457 ?f2 ;
		wdt:P4501 ?h2 ;
		wdt:P2325 ?a2 .
}
