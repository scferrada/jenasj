PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?s1 ?f1 ?l1 ?e1 WHERE {
	?id1 wdt:P2146 ?i1 ;
		wdt:P1096 ?s1 ;
		wdt:P2233 ?f1 ;
		wdt:P4501 ?l1 ;
		wdt:P2325 ?e1 .
}
SIMILARITY JOIN ON (?i1 ?s1 ?f1 ?l1 ?e1) (?i2 ?s2 ?f2 ?l2 ?e2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?s2 ?f2 ?l2 ?e2 WHERE {
	?id2 wdt:P2146 ?i2 ;
		wdt:P1096 ?s2 ;
		wdt:P2233 ?f2 ;
		wdt:P4501 ?l2 ;
		wdt:P2325 ?e2 .
}
