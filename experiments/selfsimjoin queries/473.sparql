PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?u1 ?a1 ?p1 ?c1 WHERE {
	?id1 wdt:P2325 ?u1 ;
		wdt:P2233 ?a1 ;
		wdt:P2248 ?p1 ;
		wdt:P4501 ?c1 .
}
SIMILARITY JOIN ON (?u1 ?a1 ?p1 ?c1) (?u2 ?a2 ?p2 ?c2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?u2 ?a2 ?p2 ?c2 WHERE {
	?id2 wdt:P2325 ?u2 ;
		wdt:P2233 ?a2 ;
		wdt:P2248 ?p2 ;
		wdt:P4501 ?c2 .
}
