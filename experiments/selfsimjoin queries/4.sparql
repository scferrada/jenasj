PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?g1 ?k1 ?t1 ?r1 ?i1 WHERE {
	?id1 wdt:P2146 ?g1 ;
		wdt:P2244 ?k1 ;
		wdt:P1457 ?t1 ;
		wdt:P4501 ?r1 ;
		wdt:P2248 ?i1 .
}
SIMILARITY JOIN ON (?g1 ?k1 ?t1 ?r1 ?i1) (?g2 ?k2 ?t2 ?r2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?g2 ?k2 ?t2 ?r2 ?i2 WHERE {
	?id2 wdt:P2146 ?g2 ;
		wdt:P2244 ?k2 ;
		wdt:P1457 ?t2 ;
		wdt:P4501 ?r2 ;
		wdt:P2248 ?i2 .
}
