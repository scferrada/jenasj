PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?t1 ?n1 ?b1 ?r1 WHERE {
	?id1 wdt:P2244 ?m1 ;
		wdt:P1457 ?t1 ;
		wdt:P2213 ?n1 ;
		wdt:P4501 ?b1 ;
		wdt:P2045 ?r1 .
}
SIMILARITY JOIN ON (?m1 ?t1 ?n1 ?b1 ?r1) (?m2 ?t2 ?n2 ?b2 ?r2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?t2 ?n2 ?b2 ?r2 WHERE {
	?id2 wdt:P2244 ?m2 ;
		wdt:P1457 ?t2 ;
		wdt:P2213 ?n2 ;
		wdt:P4501 ?b2 ;
		wdt:P2045 ?r2 .
}
