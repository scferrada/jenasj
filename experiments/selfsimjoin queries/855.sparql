PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?u1 ?h1 ?q1 WHERE {
	?id1 wdt:P2154 ?u1 ;
		wdt:P1123 ?h1 ;
		wdt:P2114 ?q1 .
}
SIMILARITY JOIN ON (?u1 ?h1 ?q1) (?u2 ?h2 ?q2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?u2 ?h2 ?q2 WHERE {
	?id2 wdt:P2154 ?u2 ;
		wdt:P1123 ?h2 ;
		wdt:P2114 ?q2 .
}
