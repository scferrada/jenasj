PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?w1 ?r1 ?h1 ?z1 WHERE {
	?id1 wdt:P2146 ?l1 ;
		wdt:P1457 ?w1 ;
		wdt:P1096 ?r1 ;
		wdt:P2248 ?h1 ;
		wdt:P2243 ?z1 .
}
SIMILARITY JOIN ON (?l1 ?w1 ?r1 ?h1 ?z1) (?l2 ?w2 ?r2 ?h2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?w2 ?r2 ?h2 ?z2 WHERE {
	?id2 wdt:P2146 ?l2 ;
		wdt:P1457 ?w2 ;
		wdt:P1096 ?r2 ;
		wdt:P2248 ?h2 ;
		wdt:P2243 ?z2 .
}
