PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?p1 ?z1 WHERE {
	?id1 wdt:P2146 ?r1 ;
		wdt:P2213 ?p1 ;
		wdt:P2248 ?z1 .
}
SIMILARITY JOIN ON (?r1 ?p1 ?z1) (?r2 ?p2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?p2 ?z2 WHERE {
	?id2 wdt:P2146 ?r2 ;
		wdt:P2213 ?p2 ;
		wdt:P2248 ?z2 .
}
