PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?i1 ?o1 ?v1 ?a1 ?r1 WHERE {
	?id1 wdt:P4501 ?d1 ;
		wdt:P2244 ?i1 ;
		wdt:P2243 ?o1 ;
		wdt:P2045 ?v1 ;
		wdt:P1096 ?a1 ;
		wdt:P2248 ?r1 .
}
SIMILARITY JOIN ON (?d1 ?i1 ?o1 ?v1 ?a1 ?r1) (?d2 ?i2 ?o2 ?v2 ?a2 ?r2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?i2 ?o2 ?v2 ?a2 ?r2 WHERE {
	?id2 wdt:P4501 ?d2 ;
		wdt:P2244 ?i2 ;
		wdt:P2243 ?o2 ;
		wdt:P2045 ?v2 ;
		wdt:P1096 ?a2 ;
		wdt:P2248 ?r2 .
}
