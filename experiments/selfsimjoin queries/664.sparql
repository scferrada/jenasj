PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?z1 ?v1 WHERE {
	?id1 wdt:P2213 ?q1 ;
		wdt:P2248 ?z1 ;
		wdt:P2243 ?v1 .
}
SIMILARITY JOIN ON (?q1 ?z1 ?v1) (?q2 ?z2 ?v2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?z2 ?v2 WHERE {
	?id2 wdt:P2213 ?q2 ;
		wdt:P2248 ?z2 ;
		wdt:P2243 ?v2 .
}
