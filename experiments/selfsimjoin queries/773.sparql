PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?h1 ?a1 WHERE {
	?id1 wdt:P1123 ?p1 ;
		wdt:P1148 ?h1 ;
		wdt:P2114 ?a1 .
}
SIMILARITY JOIN ON (?p1 ?h1 ?a1) (?p2 ?h2 ?a2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?h2 ?a2 WHERE {
	?id2 wdt:P1123 ?p2 ;
		wdt:P1148 ?h2 ;
		wdt:P2114 ?a2 .
}
