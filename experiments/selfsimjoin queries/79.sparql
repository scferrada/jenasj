PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?b1 ?s1 ?i1 ?v1 ?c1 WHERE {
	?id1 wdt:P2146 ?b1 ;
		wdt:P2213 ?s1 ;
		wdt:P1457 ?i1 ;
		wdt:P2233 ?v1 ;
		wdt:P2325 ?c1 .
}
SIMILARITY JOIN ON (?b1 ?s1 ?i1 ?v1 ?c1) (?b2 ?s2 ?i2 ?v2 ?c2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?b2 ?s2 ?i2 ?v2 ?c2 WHERE {
	?id2 wdt:P2146 ?b2 ;
		wdt:P2213 ?s2 ;
		wdt:P1457 ?i2 ;
		wdt:P2233 ?v2 ;
		wdt:P2325 ?c2 .
}
