PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?y1 ?u1 ?i1 ?k1 WHERE {
	?id1 wdt:P2244 ?v1 ;
		wdt:P2325 ?y1 ;
		wdt:P2233 ?u1 ;
		wdt:P2248 ?i1 ;
		wdt:P4501 ?k1 .
}
SIMILARITY JOIN ON (?v1 ?y1 ?u1 ?i1 ?k1) (?v2 ?y2 ?u2 ?i2 ?k2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?y2 ?u2 ?i2 ?k2 WHERE {
	?id2 wdt:P2244 ?v2 ;
		wdt:P2325 ?y2 ;
		wdt:P2233 ?u2 ;
		wdt:P2248 ?i2 ;
		wdt:P4501 ?k2 .
}
