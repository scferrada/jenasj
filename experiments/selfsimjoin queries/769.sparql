PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?z1 ?h1 ?u1 WHERE {
	?id1 wdt:P2213 ?s1 ;
		wdt:P1457 ?z1 ;
		wdt:P4501 ?h1 ;
		wdt:P2325 ?u1 .
}
SIMILARITY JOIN ON (?s1 ?z1 ?h1 ?u1) (?s2 ?z2 ?h2 ?u2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?z2 ?h2 ?u2 WHERE {
	?id2 wdt:P2213 ?s2 ;
		wdt:P1457 ?z2 ;
		wdt:P4501 ?h2 ;
		wdt:P2325 ?u2 .
}
