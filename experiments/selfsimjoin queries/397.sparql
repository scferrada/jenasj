PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?o1 ?l1 ?y1 ?g1 WHERE {
	?id1 wdt:P2213 ?o1 ;
		wdt:P1457 ?l1 ;
		wdt:P2243 ?y1 ;
		wdt:P2325 ?g1 .
}
SIMILARITY JOIN ON (?o1 ?l1 ?y1 ?g1) (?o2 ?l2 ?y2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?o2 ?l2 ?y2 ?g2 WHERE {
	?id2 wdt:P2213 ?o2 ;
		wdt:P1457 ?l2 ;
		wdt:P2243 ?y2 ;
		wdt:P2325 ?g2 .
}
