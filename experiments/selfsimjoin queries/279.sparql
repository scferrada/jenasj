PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?d1 ?u1 ?t1 ?e1 WHERE {
	?id1 wdt:P2146 ?i1 ;
		wdt:P2233 ?d1 ;
		wdt:P2248 ?u1 ;
		wdt:P2243 ?t1 ;
		wdt:P2325 ?e1 .
}
SIMILARITY JOIN ON (?i1 ?d1 ?u1 ?t1 ?e1) (?i2 ?d2 ?u2 ?t2 ?e2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?d2 ?u2 ?t2 ?e2 WHERE {
	?id2 wdt:P2146 ?i2 ;
		wdt:P2233 ?d2 ;
		wdt:P2248 ?u2 ;
		wdt:P2243 ?t2 ;
		wdt:P2325 ?e2 .
}
