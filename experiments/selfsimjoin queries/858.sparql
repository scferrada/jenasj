PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?l1 ?d1 ?z1 ?j1 WHERE {
	?id1 wdt:P2045 ?v1 ;
		wdt:P1096 ?l1 ;
		wdt:P2233 ?d1 ;
		wdt:P4501 ?z1 ;
		wdt:P2243 ?j1 .
}
SIMILARITY JOIN ON (?v1 ?l1 ?d1 ?z1 ?j1) (?v2 ?l2 ?d2 ?z2 ?j2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?l2 ?d2 ?z2 ?j2 WHERE {
	?id2 wdt:P2045 ?v2 ;
		wdt:P1096 ?l2 ;
		wdt:P2233 ?d2 ;
		wdt:P4501 ?z2 ;
		wdt:P2243 ?j2 .
}
