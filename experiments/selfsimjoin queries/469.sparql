PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?j1 ?b1 ?n1 ?d1 ?l1 WHERE {
	?id1 wdt:P1457 ?h1 ;
		wdt:P2248 ?j1 ;
		wdt:P2244 ?b1 ;
		wdt:P2213 ?n1 ;
		wdt:P2325 ?d1 ;
		wdt:P4501 ?l1 .
}
SIMILARITY JOIN ON (?h1 ?j1 ?b1 ?n1 ?d1 ?l1) (?h2 ?j2 ?b2 ?n2 ?d2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?j2 ?b2 ?n2 ?d2 ?l2 WHERE {
	?id2 wdt:P1457 ?h2 ;
		wdt:P2248 ?j2 ;
		wdt:P2244 ?b2 ;
		wdt:P2213 ?n2 ;
		wdt:P2325 ?d2 ;
		wdt:P4501 ?l2 .
}
