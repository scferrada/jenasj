PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?u1 ?v1 ?h1 ?w1 ?p1 WHERE {
	?id1 wdt:P2213 ?u1 ;
		wdt:P1457 ?v1 ;
		wdt:P2233 ?h1 ;
		wdt:P2248 ?w1 ;
		wdt:P2325 ?p1 .
}
SIMILARITY JOIN ON (?u1 ?v1 ?h1 ?w1 ?p1) (?u2 ?v2 ?h2 ?w2 ?p2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?u2 ?v2 ?h2 ?w2 ?p2 WHERE {
	?id2 wdt:P2213 ?u2 ;
		wdt:P1457 ?v2 ;
		wdt:P2233 ?h2 ;
		wdt:P2248 ?w2 ;
		wdt:P2325 ?p2 .
}
