PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?x1 ?n1 WHERE {
	?id1 wdt:P1457 ?m1 ;
		wdt:P1096 ?x1 ;
		wdt:P2233 ?n1 .
}
SIMILARITY JOIN ON (?m1 ?x1 ?n1) (?m2 ?x2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?x2 ?n2 WHERE {
	?id2 wdt:P1457 ?m2 ;
		wdt:P1096 ?x2 ;
		wdt:P2233 ?n2 .
}
