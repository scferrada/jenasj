PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?o1 ?t1 ?i1 ?v1 ?q1 ?b1 WHERE {
	?id1 wdt:P4501 ?o1 ;
		wdt:P2243 ?t1 ;
		wdt:P2213 ?i1 ;
		wdt:P1096 ?v1 ;
		wdt:P2325 ?q1 ;
		wdt:P2233 ?b1 .
}
SIMILARITY JOIN ON (?o1 ?t1 ?i1 ?v1 ?q1 ?b1) (?o2 ?t2 ?i2 ?v2 ?q2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?o2 ?t2 ?i2 ?v2 ?q2 ?b2 WHERE {
	?id2 wdt:P4501 ?o2 ;
		wdt:P2243 ?t2 ;
		wdt:P2213 ?i2 ;
		wdt:P1096 ?v2 ;
		wdt:P2325 ?q2 ;
		wdt:P2233 ?b2 .
}
