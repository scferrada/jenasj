PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?k1 ?h1 ?o1 ?g1 WHERE {
	?id1 wdt:P2146 ?d1 ;
		wdt:P2213 ?k1 ;
		wdt:P1457 ?h1 ;
		wdt:P1096 ?o1 ;
		wdt:P4501 ?g1 .
}
SIMILARITY JOIN ON (?d1 ?k1 ?h1 ?o1 ?g1) (?d2 ?k2 ?h2 ?o2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?k2 ?h2 ?o2 ?g2 WHERE {
	?id2 wdt:P2146 ?d2 ;
		wdt:P2213 ?k2 ;
		wdt:P1457 ?h2 ;
		wdt:P1096 ?o2 ;
		wdt:P4501 ?g2 .
}
