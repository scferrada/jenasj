PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?j1 ?n1 WHERE {
	?id1 wdt:P2154 ?l1 ;
		wdt:P1122 ?j1 ;
		wdt:P2160 ?n1 .
}
SIMILARITY JOIN ON (?l1 ?j1 ?n1) (?l2 ?j2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?j2 ?n2 WHERE {
	?id2 wdt:P2154 ?l2 ;
		wdt:P1122 ?j2 ;
		wdt:P2160 ?n2 .
}
