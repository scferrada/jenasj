PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?o1 ?a1 ?h1 ?v1 ?d1 WHERE {
	?id1 wdt:P2213 ?o1 ;
		wdt:P1096 ?a1 ;
		wdt:P2248 ?h1 ;
		wdt:P2243 ?v1 ;
		wdt:P2325 ?d1 .
}
SIMILARITY JOIN ON (?o1 ?a1 ?h1 ?v1 ?d1) (?o2 ?a2 ?h2 ?v2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?o2 ?a2 ?h2 ?v2 ?d2 WHERE {
	?id2 wdt:P2213 ?o2 ;
		wdt:P1096 ?a2 ;
		wdt:P2248 ?h2 ;
		wdt:P2243 ?v2 ;
		wdt:P2325 ?d2 .
}
