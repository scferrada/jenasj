PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?u1 ?z1 ?v1 ?h1 ?n1 WHERE {
	?id1 wdt:P1457 ?e1 ;
		wdt:P2248 ?u1 ;
		wdt:P2045 ?z1 ;
		wdt:P1096 ?v1 ;
		wdt:P2233 ?h1 ;
		wdt:P4501 ?n1 .
}
SIMILARITY JOIN ON (?e1 ?u1 ?z1 ?v1 ?h1 ?n1) (?e2 ?u2 ?z2 ?v2 ?h2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?u2 ?z2 ?v2 ?h2 ?n2 WHERE {
	?id2 wdt:P1457 ?e2 ;
		wdt:P2248 ?u2 ;
		wdt:P2045 ?z2 ;
		wdt:P1096 ?v2 ;
		wdt:P2233 ?h2 ;
		wdt:P4501 ?n2 .
}
