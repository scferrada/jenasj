PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?l1 ?t1 ?y1 WHERE {
	?id1 wdt:P2146 ?m1 ;
		wdt:P2213 ?l1 ;
		wdt:P2248 ?t1 ;
		wdt:P2243 ?y1 .
}
SIMILARITY JOIN ON (?m1 ?l1 ?t1 ?y1) (?m2 ?l2 ?t2 ?y2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?l2 ?t2 ?y2 WHERE {
	?id2 wdt:P2146 ?m2 ;
		wdt:P2213 ?l2 ;
		wdt:P2248 ?t2 ;
		wdt:P2243 ?y2 .
}
