PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?a1 ?z1 ?x1 ?y1 ?u1 WHERE {
	?id1 wdt:P2146 ?s1 ;
		wdt:P4501 ?a1 ;
		wdt:P2213 ?z1 ;
		wdt:P1096 ?x1 ;
		wdt:P2233 ?y1 ;
		wdt:P2248 ?u1 .
}
SIMILARITY JOIN ON (?s1 ?a1 ?z1 ?x1 ?y1 ?u1) (?s2 ?a2 ?z2 ?x2 ?y2 ?u2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?a2 ?z2 ?x2 ?y2 ?u2 WHERE {
	?id2 wdt:P2146 ?s2 ;
		wdt:P4501 ?a2 ;
		wdt:P2213 ?z2 ;
		wdt:P1096 ?x2 ;
		wdt:P2233 ?y2 ;
		wdt:P2248 ?u2 .
}
