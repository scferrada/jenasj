PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?g1 ?x1 ?c1 ?d1 ?l1 ?k1 WHERE {
	?id1 wdt:P2146 ?g1 ;
		wdt:P1457 ?x1 ;
		wdt:P4501 ?c1 ;
		wdt:P2243 ?d1 ;
		wdt:P2213 ?l1 ;
		wdt:P2233 ?k1 .
}
SIMILARITY JOIN ON (?g1 ?x1 ?c1 ?d1 ?l1 ?k1) (?g2 ?x2 ?c2 ?d2 ?l2 ?k2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?g2 ?x2 ?c2 ?d2 ?l2 ?k2 WHERE {
	?id2 wdt:P2146 ?g2 ;
		wdt:P1457 ?x2 ;
		wdt:P4501 ?c2 ;
		wdt:P2243 ?d2 ;
		wdt:P2213 ?l2 ;
		wdt:P2233 ?k2 .
}
