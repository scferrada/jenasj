PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?b1 ?z1 ?d1 ?h1 ?y1 ?e1 WHERE {
	?id1 wdt:P2146 ?b1 ;
		wdt:P1457 ?z1 ;
		wdt:P2248 ?d1 ;
		wdt:P2243 ?h1 ;
		wdt:P2045 ?y1 ;
		wdt:P1096 ?e1 .
}
SIMILARITY JOIN ON (?b1 ?z1 ?d1 ?h1 ?y1 ?e1) (?b2 ?z2 ?d2 ?h2 ?y2 ?e2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?b2 ?z2 ?d2 ?h2 ?y2 ?e2 WHERE {
	?id2 wdt:P2146 ?b2 ;
		wdt:P1457 ?z2 ;
		wdt:P2248 ?d2 ;
		wdt:P2243 ?h2 ;
		wdt:P2045 ?y2 ;
		wdt:P1096 ?e2 .
}
