PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?l1 ?y1 ?a1 ?n1 WHERE {
	?id1 wdt:P2244 ?v1 ;
		wdt:P2045 ?l1 ;
		wdt:P2233 ?y1 ;
		wdt:P2243 ?a1 ;
		wdt:P2325 ?n1 .
}
SIMILARITY JOIN ON (?v1 ?l1 ?y1 ?a1 ?n1) (?v2 ?l2 ?y2 ?a2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?l2 ?y2 ?a2 ?n2 WHERE {
	?id2 wdt:P2244 ?v2 ;
		wdt:P2045 ?l2 ;
		wdt:P2233 ?y2 ;
		wdt:P2243 ?a2 ;
		wdt:P2325 ?n2 .
}
