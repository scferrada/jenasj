PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?h1 ?i1 ?n1 ?m1 ?a1 WHERE {
	?id1 wdt:P2248 ?q1 ;
		wdt:P2243 ?h1 ;
		wdt:P2213 ?i1 ;
		wdt:P2045 ?n1 ;
		wdt:P2325 ?m1 ;
		wdt:P4501 ?a1 .
}
SIMILARITY JOIN ON (?q1 ?h1 ?i1 ?n1 ?m1 ?a1) (?q2 ?h2 ?i2 ?n2 ?m2 ?a2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?h2 ?i2 ?n2 ?m2 ?a2 WHERE {
	?id2 wdt:P2248 ?q2 ;
		wdt:P2243 ?h2 ;
		wdt:P2213 ?i2 ;
		wdt:P2045 ?n2 ;
		wdt:P2325 ?m2 ;
		wdt:P4501 ?a2 .
}
