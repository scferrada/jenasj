PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?v1 ?x1 WHERE {
	?id1 wdt:P2213 ?q1 ;
		wdt:P1096 ?v1 ;
		wdt:P2325 ?x1 .
}
SIMILARITY JOIN ON (?q1 ?v1 ?x1) (?q2 ?v2 ?x2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?v2 ?x2 WHERE {
	?id2 wdt:P2213 ?q2 ;
		wdt:P1096 ?v2 ;
		wdt:P2325 ?x2 .
}
