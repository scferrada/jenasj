PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?k1 ?t1 ?p1 ?v1 ?b1 WHERE {
	?id1 wdt:P1457 ?e1 ;
		wdt:P4501 ?k1 ;
		wdt:P2244 ?t1 ;
		wdt:P2213 ?p1 ;
		wdt:P1096 ?v1 ;
		wdt:P2233 ?b1 .
}
SIMILARITY JOIN ON (?e1 ?k1 ?t1 ?p1 ?v1 ?b1) (?e2 ?k2 ?t2 ?p2 ?v2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?k2 ?t2 ?p2 ?v2 ?b2 WHERE {
	?id2 wdt:P1457 ?e2 ;
		wdt:P4501 ?k2 ;
		wdt:P2244 ?t2 ;
		wdt:P2213 ?p2 ;
		wdt:P1096 ?v2 ;
		wdt:P2233 ?b2 .
}
