PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?g1 ?h1 ?r1 ?f1 WHERE {
	?id1 wdt:P2244 ?g1 ;
		wdt:P2045 ?h1 ;
		wdt:P1096 ?r1 ;
		wdt:P2233 ?f1 .
}
SIMILARITY JOIN ON (?g1 ?h1 ?r1 ?f1) (?g2 ?h2 ?r2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?g2 ?h2 ?r2 ?f2 WHERE {
	?id2 wdt:P2244 ?g2 ;
		wdt:P2045 ?h2 ;
		wdt:P1096 ?r2 ;
		wdt:P2233 ?f2 .
}
