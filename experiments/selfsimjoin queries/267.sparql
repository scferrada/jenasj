PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?z1 ?l1 WHERE {
	?id1 wdt:P1457 ?q1 ;
		wdt:P4501 ?z1 ;
		wdt:P2325 ?l1 .
}
SIMILARITY JOIN ON (?q1 ?z1 ?l1) (?q2 ?z2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?z2 ?l2 WHERE {
	?id2 wdt:P1457 ?q2 ;
		wdt:P4501 ?z2 ;
		wdt:P2325 ?l2 .
}
