PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?u1 ?w1 ?e1 ?y1 ?t1 WHERE {
	?id1 wdt:P2248 ?r1 ;
		wdt:P2244 ?u1 ;
		wdt:P1096 ?w1 ;
		wdt:P2325 ?e1 ;
		wdt:P2233 ?y1 ;
		wdt:P4501 ?t1 .
}
SIMILARITY JOIN ON (?r1 ?u1 ?w1 ?e1 ?y1 ?t1) (?r2 ?u2 ?w2 ?e2 ?y2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?u2 ?w2 ?e2 ?y2 ?t2 WHERE {
	?id2 wdt:P2248 ?r2 ;
		wdt:P2244 ?u2 ;
		wdt:P1096 ?w2 ;
		wdt:P2325 ?e2 ;
		wdt:P2233 ?y2 ;
		wdt:P4501 ?t2 .
}
