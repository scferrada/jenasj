PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?k1 ?u1 ?d1 WHERE {
	?id1 wdt:P2146 ?k1 ;
		wdt:P2045 ?u1 ;
		wdt:P2325 ?d1 .
}
SIMILARITY JOIN ON (?k1 ?u1 ?d1) (?k2 ?u2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?k2 ?u2 ?d2 WHERE {
	?id2 wdt:P2146 ?k2 ;
		wdt:P2045 ?u2 ;
		wdt:P2325 ?d2 .
}
