PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?y1 ?x1 ?b1 WHERE {
	?id1 wdt:P2045 ?l1 ;
		wdt:P1096 ?y1 ;
		wdt:P2233 ?x1 ;
		wdt:P4501 ?b1 .
}
SIMILARITY JOIN ON (?l1 ?y1 ?x1 ?b1) (?l2 ?y2 ?x2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?y2 ?x2 ?b2 WHERE {
	?id2 wdt:P2045 ?l2 ;
		wdt:P1096 ?y2 ;
		wdt:P2233 ?x2 ;
		wdt:P4501 ?b2 .
}
