PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?v1 ?b1 ?q1 ?w1 WHERE {
	?id1 wdt:P4501 ?j1 ;
		wdt:P2045 ?v1 ;
		wdt:P1096 ?b1 ;
		wdt:P2248 ?q1 ;
		wdt:P2325 ?w1 .
}
SIMILARITY JOIN ON (?j1 ?v1 ?b1 ?q1 ?w1) (?j2 ?v2 ?b2 ?q2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?v2 ?b2 ?q2 ?w2 WHERE {
	?id2 wdt:P4501 ?j2 ;
		wdt:P2045 ?v2 ;
		wdt:P1096 ?b2 ;
		wdt:P2248 ?q2 ;
		wdt:P2325 ?w2 .
}
