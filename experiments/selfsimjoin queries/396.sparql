PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?k1 ?b1 ?u1 ?g1 ?a1 WHERE {
	?id1 wdt:P2146 ?e1 ;
		wdt:P1457 ?k1 ;
		wdt:P2248 ?b1 ;
		wdt:P2244 ?u1 ;
		wdt:P2045 ?g1 ;
		wdt:P4501 ?a1 .
}
SIMILARITY JOIN ON (?e1 ?k1 ?b1 ?u1 ?g1 ?a1) (?e2 ?k2 ?b2 ?u2 ?g2 ?a2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?k2 ?b2 ?u2 ?g2 ?a2 WHERE {
	?id2 wdt:P2146 ?e2 ;
		wdt:P1457 ?k2 ;
		wdt:P2248 ?b2 ;
		wdt:P2244 ?u2 ;
		wdt:P2045 ?g2 ;
		wdt:P4501 ?a2 .
}
