PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?o1 ?t1 ?d1 ?a1 ?g1 WHERE {
	?id1 wdt:P2146 ?s1 ;
		wdt:P4501 ?o1 ;
		wdt:P2244 ?t1 ;
		wdt:P2213 ?d1 ;
		wdt:P1096 ?a1 ;
		wdt:P2325 ?g1 .
}
SIMILARITY JOIN ON (?s1 ?o1 ?t1 ?d1 ?a1 ?g1) (?s2 ?o2 ?t2 ?d2 ?a2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?o2 ?t2 ?d2 ?a2 ?g2 WHERE {
	?id2 wdt:P2146 ?s2 ;
		wdt:P4501 ?o2 ;
		wdt:P2244 ?t2 ;
		wdt:P2213 ?d2 ;
		wdt:P1096 ?a2 ;
		wdt:P2325 ?g2 .
}
