PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?q1 ?x1 ?h1 WHERE {
	?id1 wdt:P2146 ?j1 ;
		wdt:P2233 ?q1 ;
		wdt:P2248 ?x1 ;
		wdt:P2243 ?h1 .
}
SIMILARITY JOIN ON (?j1 ?q1 ?x1 ?h1) (?j2 ?q2 ?x2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?q2 ?x2 ?h2 WHERE {
	?id2 wdt:P2146 ?j2 ;
		wdt:P2233 ?q2 ;
		wdt:P2248 ?x2 ;
		wdt:P2243 ?h2 .
}
