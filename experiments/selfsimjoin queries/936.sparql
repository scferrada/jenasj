PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?t1 ?l1 ?u1 ?v1 WHERE {
	?id1 wdt:P2146 ?y1 ;
		wdt:P2045 ?t1 ;
		wdt:P2233 ?l1 ;
		wdt:P2248 ?u1 ;
		wdt:P4501 ?v1 .
}
SIMILARITY JOIN ON (?y1 ?t1 ?l1 ?u1 ?v1) (?y2 ?t2 ?l2 ?u2 ?v2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?t2 ?l2 ?u2 ?v2 WHERE {
	?id2 wdt:P2146 ?y2 ;
		wdt:P2045 ?t2 ;
		wdt:P2233 ?l2 ;
		wdt:P2248 ?u2 ;
		wdt:P4501 ?v2 .
}
