PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?m1 ?e1 ?h1 WHERE {
	?id1 wdt:P2213 ?m1 ;
		wdt:P2233 ?e1 ;
		wdt:P2248 ?h1 .
}
SIMILARITY JOIN ON (?m1 ?e1 ?h1) (?m2 ?e2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?m2 ?e2 ?h2 WHERE {
	?id2 wdt:P2213 ?m2 ;
		wdt:P2233 ?e2 ;
		wdt:P2248 ?h2 .
}
