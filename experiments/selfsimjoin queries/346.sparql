PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?c1 ?v1 ?a1 ?i1 WHERE {
	?id1 wdt:P2146 ?c1 ;
		wdt:P2244 ?v1 ;
		wdt:P1096 ?a1 ;
		wdt:P2248 ?i1 .
}
SIMILARITY JOIN ON (?c1 ?v1 ?a1 ?i1) (?c2 ?v2 ?a2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?c2 ?v2 ?a2 ?i2 WHERE {
	?id2 wdt:P2146 ?c2 ;
		wdt:P2244 ?v2 ;
		wdt:P1096 ?a2 ;
		wdt:P2248 ?i2 .
}
