PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?a1 ?d1 ?f1 ?z1 ?x1 WHERE {
	?id1 wdt:P2244 ?a1 ;
		wdt:P2233 ?d1 ;
		wdt:P2248 ?f1 ;
		wdt:P2243 ?z1 ;
		wdt:P2325 ?x1 .
}
SIMILARITY JOIN ON (?a1 ?d1 ?f1 ?z1 ?x1) (?a2 ?d2 ?f2 ?z2 ?x2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?a2 ?d2 ?f2 ?z2 ?x2 WHERE {
	?id2 wdt:P2244 ?a2 ;
		wdt:P2233 ?d2 ;
		wdt:P2248 ?f2 ;
		wdt:P2243 ?z2 ;
		wdt:P2325 ?x2 .
}
