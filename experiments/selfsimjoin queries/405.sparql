PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?x1 ?j1 ?u1 ?q1 WHERE {
	?id1 wdt:P2244 ?s1 ;
		wdt:P1096 ?x1 ;
		wdt:P2233 ?j1 ;
		wdt:P2243 ?u1 ;
		wdt:P2213 ?q1 .
}
SIMILARITY JOIN ON (?s1 ?x1 ?j1 ?u1 ?q1) (?s2 ?x2 ?j2 ?u2 ?q2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?x2 ?j2 ?u2 ?q2 WHERE {
	?id2 wdt:P2244 ?s2 ;
		wdt:P1096 ?x2 ;
		wdt:P2233 ?j2 ;
		wdt:P2243 ?u2 ;
		wdt:P2213 ?q2 .
}
