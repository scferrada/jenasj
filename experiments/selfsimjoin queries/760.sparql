PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?b1 ?o1 ?d1 ?v1 ?x1 WHERE {
	?id1 wdt:P2146 ?b1 ;
		wdt:P2244 ?o1 ;
		wdt:P1096 ?d1 ;
		wdt:P4501 ?v1 ;
		wdt:P2213 ?x1 .
}
SIMILARITY JOIN ON (?b1 ?o1 ?d1 ?v1 ?x1) (?b2 ?o2 ?d2 ?v2 ?x2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?b2 ?o2 ?d2 ?v2 ?x2 WHERE {
	?id2 wdt:P2146 ?b2 ;
		wdt:P2244 ?o2 ;
		wdt:P1096 ?d2 ;
		wdt:P4501 ?v2 ;
		wdt:P2213 ?x2 .
}
