PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?k1 ?t1 ?z1 ?p1 ?s1 ?n1 WHERE {
	?id1 wdt:P2146 ?k1 ;
		wdt:P1457 ?t1 ;
		wdt:P4501 ?z1 ;
		wdt:P2243 ?p1 ;
		wdt:P2213 ?s1 ;
		wdt:P2045 ?n1 .
}
SIMILARITY JOIN ON (?k1 ?t1 ?z1 ?p1 ?s1 ?n1) (?k2 ?t2 ?z2 ?p2 ?s2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?k2 ?t2 ?z2 ?p2 ?s2 ?n2 WHERE {
	?id2 wdt:P2146 ?k2 ;
		wdt:P1457 ?t2 ;
		wdt:P4501 ?z2 ;
		wdt:P2243 ?p2 ;
		wdt:P2213 ?s2 ;
		wdt:P2045 ?n2 .
}
