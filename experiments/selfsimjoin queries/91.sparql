PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?t1 ?o1 WHERE {
	?id1 wdt:P2244 ?y1 ;
		wdt:P2233 ?t1 ;
		wdt:P2248 ?o1 .
}
SIMILARITY JOIN ON (?y1 ?t1 ?o1) (?y2 ?t2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?t2 ?o2 WHERE {
	?id2 wdt:P2244 ?y2 ;
		wdt:P2233 ?t2 ;
		wdt:P2248 ?o2 .
}
