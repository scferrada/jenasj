PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?k1 ?n1 ?m1 ?i1 ?a1 ?c1 WHERE {
	?id1 wdt:P2248 ?k1 ;
		wdt:P2244 ?n1 ;
		wdt:P2213 ?m1 ;
		wdt:P2325 ?i1 ;
		wdt:P2233 ?a1 ;
		wdt:P4501 ?c1 .
}
SIMILARITY JOIN ON (?k1 ?n1 ?m1 ?i1 ?a1 ?c1) (?k2 ?n2 ?m2 ?i2 ?a2 ?c2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?k2 ?n2 ?m2 ?i2 ?a2 ?c2 WHERE {
	?id2 wdt:P2248 ?k2 ;
		wdt:P2244 ?n2 ;
		wdt:P2213 ?m2 ;
		wdt:P2325 ?i2 ;
		wdt:P2233 ?a2 ;
		wdt:P4501 ?c2 .
}
