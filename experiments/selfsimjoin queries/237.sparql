PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?b1 ?q1 ?v1 ?i1 WHERE {
	?id1 wdt:P2244 ?l1 ;
		wdt:P1096 ?b1 ;
		wdt:P2248 ?q1 ;
		wdt:P2243 ?v1 ;
		wdt:P2213 ?i1 .
}
SIMILARITY JOIN ON (?l1 ?b1 ?q1 ?v1 ?i1) (?l2 ?b2 ?q2 ?v2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?b2 ?q2 ?v2 ?i2 WHERE {
	?id2 wdt:P2244 ?l2 ;
		wdt:P1096 ?b2 ;
		wdt:P2248 ?q2 ;
		wdt:P2243 ?v2 ;
		wdt:P2213 ?i2 .
}
