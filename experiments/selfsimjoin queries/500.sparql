PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?w1 ?x1 ?d1 ?m1 WHERE {
	?id1 wdt:P2146 ?l1 ;
		wdt:P2045 ?w1 ;
		wdt:P2233 ?x1 ;
		wdt:P2243 ?d1 ;
		wdt:P2325 ?m1 .
}
SIMILARITY JOIN ON (?l1 ?w1 ?x1 ?d1 ?m1) (?l2 ?w2 ?x2 ?d2 ?m2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?w2 ?x2 ?d2 ?m2 WHERE {
	?id2 wdt:P2146 ?l2 ;
		wdt:P2045 ?w2 ;
		wdt:P2233 ?x2 ;
		wdt:P2243 ?d2 ;
		wdt:P2325 ?m2 .
}
