PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?c1 ?s1 ?u1 ?n1 WHERE {
	?id1 wdt:P2045 ?y1 ;
		wdt:P2233 ?c1 ;
		wdt:P2248 ?s1 ;
		wdt:P2243 ?u1 ;
		wdt:P2325 ?n1 .
}
SIMILARITY JOIN ON (?y1 ?c1 ?s1 ?u1 ?n1) (?y2 ?c2 ?s2 ?u2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?c2 ?s2 ?u2 ?n2 WHERE {
	?id2 wdt:P2045 ?y2 ;
		wdt:P2233 ?c2 ;
		wdt:P2248 ?s2 ;
		wdt:P2243 ?u2 ;
		wdt:P2325 ?n2 .
}
