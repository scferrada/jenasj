PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?k1 ?s1 ?o1 WHERE {
	?id1 wdt:P2244 ?l1 ;
		wdt:P1096 ?k1 ;
		wdt:P4501 ?s1 ;
		wdt:P2243 ?o1 .
}
SIMILARITY JOIN ON (?l1 ?k1 ?s1 ?o1) (?l2 ?k2 ?s2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?k2 ?s2 ?o2 WHERE {
	?id2 wdt:P2244 ?l2 ;
		wdt:P1096 ?k2 ;
		wdt:P4501 ?s2 ;
		wdt:P2243 ?o2 .
}
