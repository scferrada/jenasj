PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?t1 ?x1 ?z1 ?a1 ?g1 WHERE {
	?id1 wdt:P2146 ?i1 ;
		wdt:P2248 ?t1 ;
		wdt:P2244 ?x1 ;
		wdt:P1096 ?z1 ;
		wdt:P2325 ?a1 ;
		wdt:P2233 ?g1 .
}
SIMILARITY JOIN ON (?i1 ?t1 ?x1 ?z1 ?a1 ?g1) (?i2 ?t2 ?x2 ?z2 ?a2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?t2 ?x2 ?z2 ?a2 ?g2 WHERE {
	?id2 wdt:P2146 ?i2 ;
		wdt:P2248 ?t2 ;
		wdt:P2244 ?x2 ;
		wdt:P1096 ?z2 ;
		wdt:P2325 ?a2 ;
		wdt:P2233 ?g2 .
}
