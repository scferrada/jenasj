PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?z1 ?a1 ?k1 ?b1 ?n1 ?q1 WHERE {
	?id1 wdt:P2146 ?z1 ;
		wdt:P4501 ?a1 ;
		wdt:P2213 ?k1 ;
		wdt:P2325 ?b1 ;
		wdt:P2233 ?n1 ;
		wdt:P2248 ?q1 .
}
SIMILARITY JOIN ON (?z1 ?a1 ?k1 ?b1 ?n1 ?q1) (?z2 ?a2 ?k2 ?b2 ?n2 ?q2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?z2 ?a2 ?k2 ?b2 ?n2 ?q2 WHERE {
	?id2 wdt:P2146 ?z2 ;
		wdt:P4501 ?a2 ;
		wdt:P2213 ?k2 ;
		wdt:P2325 ?b2 ;
		wdt:P2233 ?n2 ;
		wdt:P2248 ?q2 .
}
