PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?d1 ?b1 ?t1 ?f1 ?r1 WHERE {
	?id1 wdt:P2146 ?e1 ;
		wdt:P1457 ?d1 ;
		wdt:P4501 ?b1 ;
		wdt:P2213 ?t1 ;
		wdt:P2045 ?f1 ;
		wdt:P2325 ?r1 .
}
SIMILARITY JOIN ON (?e1 ?d1 ?b1 ?t1 ?f1 ?r1) (?e2 ?d2 ?b2 ?t2 ?f2 ?r2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?d2 ?b2 ?t2 ?f2 ?r2 WHERE {
	?id2 wdt:P2146 ?e2 ;
		wdt:P1457 ?d2 ;
		wdt:P4501 ?b2 ;
		wdt:P2213 ?t2 ;
		wdt:P2045 ?f2 ;
		wdt:P2325 ?r2 .
}
