PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?n1 ?q1 ?j1 ?m1 ?i1 WHERE {
	?id1 wdt:P2146 ?h1 ;
		wdt:P2244 ?n1 ;
		wdt:P2213 ?q1 ;
		wdt:P2045 ?j1 ;
		wdt:P1096 ?m1 ;
		wdt:P2325 ?i1 .
}
SIMILARITY JOIN ON (?h1 ?n1 ?q1 ?j1 ?m1 ?i1) (?h2 ?n2 ?q2 ?j2 ?m2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?n2 ?q2 ?j2 ?m2 ?i2 WHERE {
	?id2 wdt:P2146 ?h2 ;
		wdt:P2244 ?n2 ;
		wdt:P2213 ?q2 ;
		wdt:P2045 ?j2 ;
		wdt:P1096 ?m2 ;
		wdt:P2325 ?i2 .
}
