PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?u1 ?v1 ?g1 WHERE {
	?id1 wdt:P2146 ?j1 ;
		wdt:P2213 ?u1 ;
		wdt:P1457 ?v1 ;
		wdt:P2325 ?g1 .
}
SIMILARITY JOIN ON (?j1 ?u1 ?v1 ?g1) (?j2 ?u2 ?v2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?u2 ?v2 ?g2 WHERE {
	?id2 wdt:P2146 ?j2 ;
		wdt:P2213 ?u2 ;
		wdt:P1457 ?v2 ;
		wdt:P2325 ?g2 .
}
