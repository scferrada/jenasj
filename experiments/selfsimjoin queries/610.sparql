PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?o1 ?n1 ?y1 ?h1 ?s1 WHERE {
	?id1 wdt:P2146 ?q1 ;
		wdt:P2248 ?o1 ;
		wdt:P2244 ?n1 ;
		wdt:P2243 ?y1 ;
		wdt:P2213 ?h1 ;
		wdt:P2045 ?s1 .
}
SIMILARITY JOIN ON (?q1 ?o1 ?n1 ?y1 ?h1 ?s1) (?q2 ?o2 ?n2 ?y2 ?h2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?o2 ?n2 ?y2 ?h2 ?s2 WHERE {
	?id2 wdt:P2146 ?q2 ;
		wdt:P2248 ?o2 ;
		wdt:P2244 ?n2 ;
		wdt:P2243 ?y2 ;
		wdt:P2213 ?h2 ;
		wdt:P2045 ?s2 .
}
