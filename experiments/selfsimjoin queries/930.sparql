PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?s1 ?l1 WHERE {
	?id1 wdt:P6343 ?t1 ;
		wdt:P6344 ?s1 ;
		wdt:P1539 ?l1 .
}
SIMILARITY JOIN ON (?t1 ?s1 ?l1) (?t2 ?s2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?s2 ?l2 WHERE {
	?id2 wdt:P6343 ?t2 ;
		wdt:P6344 ?s2 ;
		wdt:P1539 ?l2 .
}
