PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?i1 ?p1 ?d1 ?u1 WHERE {
	?id1 wdt:P2154 ?v1 ;
		wdt:P1123 ?i1 ;
		wdt:P1122 ?p1 ;
		wdt:P2160 ?d1 ;
		wdt:P2114 ?u1 .
}
SIMILARITY JOIN ON (?v1 ?i1 ?p1 ?d1 ?u1) (?v2 ?i2 ?p2 ?d2 ?u2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?i2 ?p2 ?d2 ?u2 WHERE {
	?id2 wdt:P2154 ?v2 ;
		wdt:P1123 ?i2 ;
		wdt:P1122 ?p2 ;
		wdt:P2160 ?d2 ;
		wdt:P2114 ?u2 .
}
