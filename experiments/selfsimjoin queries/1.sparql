PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?n1 ?f1 ?y1 WHERE {
	?id1 wdt:P2045 ?x1 ;
		wdt:P1096 ?n1 ;
		wdt:P2248 ?f1 ;
		wdt:P4501 ?y1 .
}
SIMILARITY JOIN ON (?x1 ?n1 ?f1 ?y1) (?x2 ?n2 ?f2 ?y2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?n2 ?f2 ?y2 WHERE {
	?id2 wdt:P2045 ?x2 ;
		wdt:P1096 ?n2 ;
		wdt:P2248 ?f2 ;
		wdt:P4501 ?y2 .
}
