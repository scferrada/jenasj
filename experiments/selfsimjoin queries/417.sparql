PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?f1 ?v1 ?c1 ?t1 ?s1 WHERE {
	?id1 wdt:P2146 ?f1 ;
		wdt:P2213 ?v1 ;
		wdt:P1457 ?c1 ;
		wdt:P2233 ?t1 ;
		wdt:P4501 ?s1 .
}
SIMILARITY JOIN ON (?f1 ?v1 ?c1 ?t1 ?s1) (?f2 ?v2 ?c2 ?t2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?f2 ?v2 ?c2 ?t2 ?s2 WHERE {
	?id2 wdt:P2146 ?f2 ;
		wdt:P2213 ?v2 ;
		wdt:P1457 ?c2 ;
		wdt:P2233 ?t2 ;
		wdt:P4501 ?s2 .
}
