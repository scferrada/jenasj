PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?a1 ?o1 WHERE {
	?id1 wdt:P2154 ?q1 ;
		wdt:P1148 ?a1 ;
		wdt:P1122 ?o1 .
}
SIMILARITY JOIN ON (?q1 ?a1 ?o1) (?q2 ?a2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?a2 ?o2 WHERE {
	?id2 wdt:P2154 ?q2 ;
		wdt:P1148 ?a2 ;
		wdt:P1122 ?o2 .
}
