PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?g1 ?l1 ?x1 ?u1 WHERE {
	?id1 wdt:P2146 ?g1 ;
		wdt:P2045 ?l1 ;
		wdt:P1096 ?x1 ;
		wdt:P2233 ?u1 .
}
SIMILARITY JOIN ON (?g1 ?l1 ?x1 ?u1) (?g2 ?l2 ?x2 ?u2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?g2 ?l2 ?x2 ?u2 WHERE {
	?id2 wdt:P2146 ?g2 ;
		wdt:P2045 ?l2 ;
		wdt:P1096 ?x2 ;
		wdt:P2233 ?u2 .
}
