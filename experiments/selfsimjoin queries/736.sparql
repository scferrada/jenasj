PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?l1 ?n1 ?y1 ?r1 ?w1 WHERE {
	?id1 wdt:P2146 ?e1 ;
		wdt:P1457 ?l1 ;
		wdt:P2243 ?n1 ;
		wdt:P1096 ?y1 ;
		wdt:P2325 ?r1 ;
		wdt:P2233 ?w1 .
}
SIMILARITY JOIN ON (?e1 ?l1 ?n1 ?y1 ?r1 ?w1) (?e2 ?l2 ?n2 ?y2 ?r2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?l2 ?n2 ?y2 ?r2 ?w2 WHERE {
	?id2 wdt:P2146 ?e2 ;
		wdt:P1457 ?l2 ;
		wdt:P2243 ?n2 ;
		wdt:P1096 ?y2 ;
		wdt:P2325 ?r2 ;
		wdt:P2233 ?w2 .
}
