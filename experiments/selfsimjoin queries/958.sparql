PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?j1 ?f1 ?d1 WHERE {
	?id1 wdt:P2244 ?h1 ;
		wdt:P1457 ?j1 ;
		wdt:P4501 ?f1 ;
		wdt:P2248 ?d1 .
}
SIMILARITY JOIN ON (?h1 ?j1 ?f1 ?d1) (?h2 ?j2 ?f2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?j2 ?f2 ?d2 WHERE {
	?id2 wdt:P2244 ?h2 ;
		wdt:P1457 ?j2 ;
		wdt:P4501 ?f2 ;
		wdt:P2248 ?d2 .
}
