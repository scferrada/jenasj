PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?i1 ?d1 ?g1 WHERE {
	?id1 wdt:P2146 ?j1 ;
		wdt:P2244 ?i1 ;
		wdt:P2233 ?d1 ;
		wdt:P2325 ?g1 .
}
SIMILARITY JOIN ON (?j1 ?i1 ?d1 ?g1) (?j2 ?i2 ?d2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?i2 ?d2 ?g2 WHERE {
	?id2 wdt:P2146 ?j2 ;
		wdt:P2244 ?i2 ;
		wdt:P2233 ?d2 ;
		wdt:P2325 ?g2 .
}
