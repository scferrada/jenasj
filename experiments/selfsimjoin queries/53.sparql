PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?h1 ?j1 ?z1 WHERE {
	?id1 wdt:P2146 ?y1 ;
		wdt:P2213 ?h1 ;
		wdt:P2233 ?j1 ;
		wdt:P2325 ?z1 .
}
SIMILARITY JOIN ON (?y1 ?h1 ?j1 ?z1) (?y2 ?h2 ?j2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?h2 ?j2 ?z2 WHERE {
	?id2 wdt:P2146 ?y2 ;
		wdt:P2213 ?h2 ;
		wdt:P2233 ?j2 ;
		wdt:P2325 ?z2 .
}
