PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?m1 ?x1 ?b1 WHERE {
	?id1 wdt:P1096 ?h1 ;
		wdt:P4501 ?m1 ;
		wdt:P2243 ?x1 ;
		wdt:P2325 ?b1 .
}
SIMILARITY JOIN ON (?h1 ?m1 ?x1 ?b1) (?h2 ?m2 ?x2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?m2 ?x2 ?b2 WHERE {
	?id2 wdt:P1096 ?h2 ;
		wdt:P4501 ?m2 ;
		wdt:P2243 ?x2 ;
		wdt:P2325 ?b2 .
}
