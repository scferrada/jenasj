PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?b1 ?t1 ?v1 ?o1 ?s1 WHERE {
	?id1 wdt:P4501 ?j1 ;
		wdt:P2244 ?b1 ;
		wdt:P2213 ?t1 ;
		wdt:P2045 ?v1 ;
		wdt:P1096 ?o1 ;
		wdt:P2233 ?s1 .
}
SIMILARITY JOIN ON (?j1 ?b1 ?t1 ?v1 ?o1 ?s1) (?j2 ?b2 ?t2 ?v2 ?o2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?b2 ?t2 ?v2 ?o2 ?s2 WHERE {
	?id2 wdt:P4501 ?j2 ;
		wdt:P2244 ?b2 ;
		wdt:P2213 ?t2 ;
		wdt:P2045 ?v2 ;
		wdt:P1096 ?o2 ;
		wdt:P2233 ?s2 .
}
