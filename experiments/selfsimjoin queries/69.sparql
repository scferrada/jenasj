PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?b1 ?p1 ?d1 ?q1 ?m1 WHERE {
	?id1 wdt:P2244 ?b1 ;
		wdt:P2045 ?p1 ;
		wdt:P2233 ?d1 ;
		wdt:P4501 ?q1 ;
		wdt:P2213 ?m1 .
}
SIMILARITY JOIN ON (?b1 ?p1 ?d1 ?q1 ?m1) (?b2 ?p2 ?d2 ?q2 ?m2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?b2 ?p2 ?d2 ?q2 ?m2 WHERE {
	?id2 wdt:P2244 ?b2 ;
		wdt:P2045 ?p2 ;
		wdt:P2233 ?d2 ;
		wdt:P4501 ?q2 ;
		wdt:P2213 ?m2 .
}
