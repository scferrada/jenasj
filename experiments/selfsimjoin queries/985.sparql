PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?a1 ?e1 ?y1 ?g1 ?p1 ?j1 WHERE {
	?id1 wdt:P1457 ?a1 ;
		wdt:P4501 ?e1 ;
		wdt:P2244 ?y1 ;
		wdt:P2243 ?g1 ;
		wdt:P1096 ?p1 ;
		wdt:P2233 ?j1 .
}
SIMILARITY JOIN ON (?a1 ?e1 ?y1 ?g1 ?p1 ?j1) (?a2 ?e2 ?y2 ?g2 ?p2 ?j2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?a2 ?e2 ?y2 ?g2 ?p2 ?j2 WHERE {
	?id2 wdt:P1457 ?a2 ;
		wdt:P4501 ?e2 ;
		wdt:P2244 ?y2 ;
		wdt:P2243 ?g2 ;
		wdt:P1096 ?p2 ;
		wdt:P2233 ?j2 .
}
