PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?c1 ?n1 ?a1 ?m1 ?x1 ?l1 WHERE {
	?id1 wdt:P2248 ?c1 ;
		wdt:P2213 ?n1 ;
		wdt:P2045 ?a1 ;
		wdt:P1096 ?m1 ;
		wdt:P2325 ?x1 ;
		wdt:P4501 ?l1 .
}
SIMILARITY JOIN ON (?c1 ?n1 ?a1 ?m1 ?x1 ?l1) (?c2 ?n2 ?a2 ?m2 ?x2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?c2 ?n2 ?a2 ?m2 ?x2 ?l2 WHERE {
	?id2 wdt:P2248 ?c2 ;
		wdt:P2213 ?n2 ;
		wdt:P2045 ?a2 ;
		wdt:P1096 ?m2 ;
		wdt:P2325 ?x2 ;
		wdt:P4501 ?l2 .
}
