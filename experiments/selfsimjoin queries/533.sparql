PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?y1 ?n1 ?f1 ?a1 ?w1 WHERE {
	?id1 wdt:P4501 ?i1 ;
		wdt:P2243 ?y1 ;
		wdt:P2213 ?n1 ;
		wdt:P1096 ?f1 ;
		wdt:P2233 ?a1 ;
		wdt:P2248 ?w1 .
}
SIMILARITY JOIN ON (?i1 ?y1 ?n1 ?f1 ?a1 ?w1) (?i2 ?y2 ?n2 ?f2 ?a2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?y2 ?n2 ?f2 ?a2 ?w2 WHERE {
	?id2 wdt:P4501 ?i2 ;
		wdt:P2243 ?y2 ;
		wdt:P2213 ?n2 ;
		wdt:P1096 ?f2 ;
		wdt:P2233 ?a2 ;
		wdt:P2248 ?w2 .
}
