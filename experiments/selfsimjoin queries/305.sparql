PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?s1 ?d1 ?w1 ?v1 ?g1 WHERE {
	?id1 wdt:P2146 ?e1 ;
		wdt:P2244 ?s1 ;
		wdt:P2243 ?d1 ;
		wdt:P2213 ?w1 ;
		wdt:P2325 ?v1 ;
		wdt:P2233 ?g1 .
}
SIMILARITY JOIN ON (?e1 ?s1 ?d1 ?w1 ?v1 ?g1) (?e2 ?s2 ?d2 ?w2 ?v2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?s2 ?d2 ?w2 ?v2 ?g2 WHERE {
	?id2 wdt:P2146 ?e2 ;
		wdt:P2244 ?s2 ;
		wdt:P2243 ?d2 ;
		wdt:P2213 ?w2 ;
		wdt:P2325 ?v2 ;
		wdt:P2233 ?g2 .
}
