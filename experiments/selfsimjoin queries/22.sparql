PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?r1 ?m1 ?t1 ?p1 WHERE {
	?id1 wdt:P2146 ?y1 ;
		wdt:P2213 ?r1 ;
		wdt:P2045 ?m1 ;
		wdt:P1096 ?t1 ;
		wdt:P2325 ?p1 .
}
SIMILARITY JOIN ON (?y1 ?r1 ?m1 ?t1 ?p1) (?y2 ?r2 ?m2 ?t2 ?p2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?r2 ?m2 ?t2 ?p2 WHERE {
	?id2 wdt:P2146 ?y2 ;
		wdt:P2213 ?r2 ;
		wdt:P2045 ?m2 ;
		wdt:P1096 ?t2 ;
		wdt:P2325 ?p2 .
}
