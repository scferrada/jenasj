PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?x1 ?i1 WHERE {
	?id1 wdt:P2213 ?p1 ;
		wdt:P2233 ?x1 ;
		wdt:P4501 ?i1 .
}
SIMILARITY JOIN ON (?p1 ?x1 ?i1) (?p2 ?x2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?x2 ?i2 WHERE {
	?id2 wdt:P2213 ?p2 ;
		wdt:P2233 ?x2 ;
		wdt:P4501 ?i2 .
}
