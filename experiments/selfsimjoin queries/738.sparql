PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?a1 ?e1 ?l1 ?k1 ?g1 ?z1 WHERE {
	?id1 wdt:P1457 ?a1 ;
		wdt:P2244 ?e1 ;
		wdt:P2213 ?l1 ;
		wdt:P1096 ?k1 ;
		wdt:P2325 ?g1 ;
		wdt:P2233 ?z1 .
}
SIMILARITY JOIN ON (?a1 ?e1 ?l1 ?k1 ?g1 ?z1) (?a2 ?e2 ?l2 ?k2 ?g2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?a2 ?e2 ?l2 ?k2 ?g2 ?z2 WHERE {
	?id2 wdt:P1457 ?a2 ;
		wdt:P2244 ?e2 ;
		wdt:P2213 ?l2 ;
		wdt:P1096 ?k2 ;
		wdt:P2325 ?g2 ;
		wdt:P2233 ?z2 .
}
