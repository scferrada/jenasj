PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?a1 ?u1 ?t1 ?p1 ?g1 WHERE {
	?id1 wdt:P1457 ?a1 ;
		wdt:P1096 ?u1 ;
		wdt:P2045 ?t1 ;
		wdt:P2243 ?p1 ;
		wdt:P4501 ?g1 .
}
SIMILARITY JOIN ON (?a1 ?u1 ?t1 ?p1 ?g1) (?a2 ?u2 ?t2 ?p2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?a2 ?u2 ?t2 ?p2 ?g2 WHERE {
	?id2 wdt:P1457 ?a2 ;
		wdt:P1096 ?u2 ;
		wdt:P2045 ?t2 ;
		wdt:P2243 ?p2 ;
		wdt:P4501 ?g2 .
}
