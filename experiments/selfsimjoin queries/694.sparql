PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?e1 ?y1 ?d1 ?p1 ?k1 WHERE {
	?id1 wdt:P1457 ?t1 ;
		wdt:P2248 ?e1 ;
		wdt:P2244 ?y1 ;
		wdt:P2045 ?d1 ;
		wdt:P1096 ?p1 ;
		wdt:P2325 ?k1 .
}
SIMILARITY JOIN ON (?t1 ?e1 ?y1 ?d1 ?p1 ?k1) (?t2 ?e2 ?y2 ?d2 ?p2 ?k2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?e2 ?y2 ?d2 ?p2 ?k2 WHERE {
	?id2 wdt:P1457 ?t2 ;
		wdt:P2248 ?e2 ;
		wdt:P2244 ?y2 ;
		wdt:P2045 ?d2 ;
		wdt:P1096 ?p2 ;
		wdt:P2325 ?k2 .
}
