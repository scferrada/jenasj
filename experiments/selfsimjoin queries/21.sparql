PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?n1 ?h1 ?v1 ?x1 ?b1 WHERE {
	?id1 wdt:P2146 ?j1 ;
		wdt:P4501 ?n1 ;
		wdt:P2244 ?h1 ;
		wdt:P1096 ?v1 ;
		wdt:P2325 ?x1 ;
		wdt:P2233 ?b1 .
}
SIMILARITY JOIN ON (?j1 ?n1 ?h1 ?v1 ?x1 ?b1) (?j2 ?n2 ?h2 ?v2 ?x2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?n2 ?h2 ?v2 ?x2 ?b2 WHERE {
	?id2 wdt:P2146 ?j2 ;
		wdt:P4501 ?n2 ;
		wdt:P2244 ?h2 ;
		wdt:P1096 ?v2 ;
		wdt:P2325 ?x2 ;
		wdt:P2233 ?b2 .
}
