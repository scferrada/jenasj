PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?q1 ?w1 ?v1 WHERE {
	?id1 wdt:P2146 ?j1 ;
		wdt:P1096 ?q1 ;
		wdt:P2233 ?w1 ;
		wdt:P2243 ?v1 .
}
SIMILARITY JOIN ON (?j1 ?q1 ?w1 ?v1) (?j2 ?q2 ?w2 ?v2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?q2 ?w2 ?v2 WHERE {
	?id2 wdt:P2146 ?j2 ;
		wdt:P1096 ?q2 ;
		wdt:P2233 ?w2 ;
		wdt:P2243 ?v2 .
}
