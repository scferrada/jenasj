PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?w1 ?q1 ?c1 WHERE {
	?id1 wdt:P2213 ?w1 ;
		wdt:P1096 ?q1 ;
		wdt:P2243 ?c1 .
}
SIMILARITY JOIN ON (?w1 ?q1 ?c1) (?w2 ?q2 ?c2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?w2 ?q2 ?c2 WHERE {
	?id2 wdt:P2213 ?w2 ;
		wdt:P1096 ?q2 ;
		wdt:P2243 ?c2 .
}
