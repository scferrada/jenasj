PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?n1 ?v1 ?d1 ?p1 ?s1 WHERE {
	?id1 wdt:P2146 ?j1 ;
		wdt:P2244 ?n1 ;
		wdt:P2213 ?v1 ;
		wdt:P1096 ?d1 ;
		wdt:P2325 ?p1 ;
		wdt:P2233 ?s1 .
}
SIMILARITY JOIN ON (?j1 ?n1 ?v1 ?d1 ?p1 ?s1) (?j2 ?n2 ?v2 ?d2 ?p2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?n2 ?v2 ?d2 ?p2 ?s2 WHERE {
	?id2 wdt:P2146 ?j2 ;
		wdt:P2244 ?n2 ;
		wdt:P2213 ?v2 ;
		wdt:P1096 ?d2 ;
		wdt:P2325 ?p2 ;
		wdt:P2233 ?s2 .
}
