PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?a1 ?m1 ?z1 ?b1 ?f1 ?d1 WHERE {
	?id1 wdt:P1457 ?a1 ;
		wdt:P2248 ?m1 ;
		wdt:P2244 ?z1 ;
		wdt:P2045 ?b1 ;
		wdt:P2325 ?f1 ;
		wdt:P2233 ?d1 .
}
SIMILARITY JOIN ON (?a1 ?m1 ?z1 ?b1 ?f1 ?d1) (?a2 ?m2 ?z2 ?b2 ?f2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?a2 ?m2 ?z2 ?b2 ?f2 ?d2 WHERE {
	?id2 wdt:P1457 ?a2 ;
		wdt:P2248 ?m2 ;
		wdt:P2244 ?z2 ;
		wdt:P2045 ?b2 ;
		wdt:P2325 ?f2 ;
		wdt:P2233 ?d2 .
}
