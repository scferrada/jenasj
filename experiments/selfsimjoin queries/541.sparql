PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?u1 ?k1 ?r1 ?w1 ?m1 WHERE {
	?id1 wdt:P2244 ?u1 ;
		wdt:P1457 ?k1 ;
		wdt:P4501 ?r1 ;
		wdt:P2243 ?w1 ;
		wdt:P2325 ?m1 .
}
SIMILARITY JOIN ON (?u1 ?k1 ?r1 ?w1 ?m1) (?u2 ?k2 ?r2 ?w2 ?m2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?u2 ?k2 ?r2 ?w2 ?m2 WHERE {
	?id2 wdt:P2244 ?u2 ;
		wdt:P1457 ?k2 ;
		wdt:P4501 ?r2 ;
		wdt:P2243 ?w2 ;
		wdt:P2325 ?m2 .
}
