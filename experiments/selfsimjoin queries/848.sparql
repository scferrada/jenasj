PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?n1 ?i1 ?r1 ?m1 ?s1 WHERE {
	?id1 wdt:P1457 ?n1 ;
		wdt:P1096 ?i1 ;
		wdt:P2233 ?r1 ;
		wdt:P4501 ?m1 ;
		wdt:P2243 ?s1 .
}
SIMILARITY JOIN ON (?n1 ?i1 ?r1 ?m1 ?s1) (?n2 ?i2 ?r2 ?m2 ?s2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?n2 ?i2 ?r2 ?m2 ?s2 WHERE {
	?id2 wdt:P1457 ?n2 ;
		wdt:P1096 ?i2 ;
		wdt:P2233 ?r2 ;
		wdt:P4501 ?m2 ;
		wdt:P2243 ?s2 .
}
