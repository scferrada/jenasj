PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?q1 ?m1 ?g1 WHERE {
	?id1 wdt:P2146 ?x1 ;
		wdt:P2213 ?q1 ;
		wdt:P2248 ?m1 ;
		wdt:P2325 ?g1 .
}
SIMILARITY JOIN ON (?x1 ?q1 ?m1 ?g1) (?x2 ?q2 ?m2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?q2 ?m2 ?g2 WHERE {
	?id2 wdt:P2146 ?x2 ;
		wdt:P2213 ?q2 ;
		wdt:P2248 ?m2 ;
		wdt:P2325 ?g2 .
}
