PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?u1 ?m1 ?v1 ?w1 ?p1 WHERE {
	?id1 wdt:P1457 ?r1 ;
		wdt:P2244 ?u1 ;
		wdt:P2243 ?m1 ;
		wdt:P2045 ?v1 ;
		wdt:P1096 ?w1 ;
		wdt:P2325 ?p1 .
}
SIMILARITY JOIN ON (?r1 ?u1 ?m1 ?v1 ?w1 ?p1) (?r2 ?u2 ?m2 ?v2 ?w2 ?p2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?u2 ?m2 ?v2 ?w2 ?p2 WHERE {
	?id2 wdt:P1457 ?r2 ;
		wdt:P2244 ?u2 ;
		wdt:P2243 ?m2 ;
		wdt:P2045 ?v2 ;
		wdt:P1096 ?w2 ;
		wdt:P2325 ?p2 .
}
