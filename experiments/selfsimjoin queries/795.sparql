PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?o1 ?d1 ?u1 ?r1 ?l1 ?i1 WHERE {
	?id1 wdt:P2248 ?o1 ;
		wdt:P2244 ?d1 ;
		wdt:P2213 ?u1 ;
		wdt:P2045 ?r1 ;
		wdt:P1096 ?l1 ;
		wdt:P4501 ?i1 .
}
SIMILARITY JOIN ON (?o1 ?d1 ?u1 ?r1 ?l1 ?i1) (?o2 ?d2 ?u2 ?r2 ?l2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?o2 ?d2 ?u2 ?r2 ?l2 ?i2 WHERE {
	?id2 wdt:P2248 ?o2 ;
		wdt:P2244 ?d2 ;
		wdt:P2213 ?u2 ;
		wdt:P2045 ?r2 ;
		wdt:P1096 ?l2 ;
		wdt:P4501 ?i2 .
}
