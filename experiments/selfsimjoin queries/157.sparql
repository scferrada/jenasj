PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?f1 ?n1 ?q1 ?w1 WHERE {
	?id1 wdt:P2146 ?d1 ;
		wdt:P2244 ?f1 ;
		wdt:P2045 ?n1 ;
		wdt:P4501 ?q1 ;
		wdt:P2325 ?w1 .
}
SIMILARITY JOIN ON (?d1 ?f1 ?n1 ?q1 ?w1) (?d2 ?f2 ?n2 ?q2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?f2 ?n2 ?q2 ?w2 WHERE {
	?id2 wdt:P2146 ?d2 ;
		wdt:P2244 ?f2 ;
		wdt:P2045 ?n2 ;
		wdt:P4501 ?q2 ;
		wdt:P2325 ?w2 .
}
