PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?o1 ?y1 ?b1 ?s1 ?h1 WHERE {
	?id1 wdt:P2146 ?o1 ;
		wdt:P2244 ?y1 ;
		wdt:P1457 ?b1 ;
		wdt:P4501 ?s1 ;
		wdt:P2213 ?h1 .
}
SIMILARITY JOIN ON (?o1 ?y1 ?b1 ?s1 ?h1) (?o2 ?y2 ?b2 ?s2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?o2 ?y2 ?b2 ?s2 ?h2 WHERE {
	?id2 wdt:P2146 ?o2 ;
		wdt:P2244 ?y2 ;
		wdt:P1457 ?b2 ;
		wdt:P4501 ?s2 ;
		wdt:P2213 ?h2 .
}
