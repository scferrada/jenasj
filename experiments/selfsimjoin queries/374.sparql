PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?z1 ?c1 ?m1 ?a1 ?h1 WHERE {
	?id1 wdt:P2244 ?z1 ;
		wdt:P2045 ?c1 ;
		wdt:P1096 ?m1 ;
		wdt:P2248 ?a1 ;
		wdt:P2325 ?h1 .
}
SIMILARITY JOIN ON (?z1 ?c1 ?m1 ?a1 ?h1) (?z2 ?c2 ?m2 ?a2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?z2 ?c2 ?m2 ?a2 ?h2 WHERE {
	?id2 wdt:P2244 ?z2 ;
		wdt:P2045 ?c2 ;
		wdt:P1096 ?m2 ;
		wdt:P2248 ?a2 ;
		wdt:P2325 ?h2 .
}
