PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?y1 ?i1 ?t1 ?v1 ?z1 WHERE {
	?id1 wdt:P2146 ?r1 ;
		wdt:P4501 ?y1 ;
		wdt:P2244 ?i1 ;
		wdt:P2243 ?t1 ;
		wdt:P2213 ?v1 ;
		wdt:P1096 ?z1 .
}
SIMILARITY JOIN ON (?r1 ?y1 ?i1 ?t1 ?v1 ?z1) (?r2 ?y2 ?i2 ?t2 ?v2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?y2 ?i2 ?t2 ?v2 ?z2 WHERE {
	?id2 wdt:P2146 ?r2 ;
		wdt:P4501 ?y2 ;
		wdt:P2244 ?i2 ;
		wdt:P2243 ?t2 ;
		wdt:P2213 ?v2 ;
		wdt:P1096 ?z2 .
}
