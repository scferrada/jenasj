PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?l1 ?m1 ?w1 WHERE {
	?id1 wdt:P2213 ?l1 ;
		wdt:P1096 ?m1 ;
		wdt:P2248 ?w1 .
}
SIMILARITY JOIN ON (?l1 ?m1 ?w1) (?l2 ?m2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?l2 ?m2 ?w2 WHERE {
	?id2 wdt:P2213 ?l2 ;
		wdt:P1096 ?m2 ;
		wdt:P2248 ?w2 .
}
