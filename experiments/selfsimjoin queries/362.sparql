PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?o1 ?b1 ?f1 ?n1 WHERE {
	?id1 wdt:P2244 ?x1 ;
		wdt:P1457 ?o1 ;
		wdt:P1096 ?b1 ;
		wdt:P2248 ?f1 ;
		wdt:P2325 ?n1 .
}
SIMILARITY JOIN ON (?x1 ?o1 ?b1 ?f1 ?n1) (?x2 ?o2 ?b2 ?f2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?o2 ?b2 ?f2 ?n2 WHERE {
	?id2 wdt:P2244 ?x2 ;
		wdt:P1457 ?o2 ;
		wdt:P1096 ?b2 ;
		wdt:P2248 ?f2 ;
		wdt:P2325 ?n2 .
}
