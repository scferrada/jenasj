PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?a1 ?m1 ?d1 ?z1 WHERE {
	?id1 wdt:P2244 ?a1 ;
		wdt:P2233 ?m1 ;
		wdt:P2213 ?d1 ;
		wdt:P2325 ?z1 .
}
SIMILARITY JOIN ON (?a1 ?m1 ?d1 ?z1) (?a2 ?m2 ?d2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?a2 ?m2 ?d2 ?z2 WHERE {
	?id2 wdt:P2244 ?a2 ;
		wdt:P2233 ?m2 ;
		wdt:P2213 ?d2 ;
		wdt:P2325 ?z2 .
}
