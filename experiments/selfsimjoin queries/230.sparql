PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?u1 ?p1 ?f1 ?r1 ?t1 WHERE {
	?id1 wdt:P2248 ?i1 ;
		wdt:P2244 ?u1 ;
		wdt:P2243 ?p1 ;
		wdt:P2213 ?f1 ;
		wdt:P1096 ?r1 ;
		wdt:P4501 ?t1 .
}
SIMILARITY JOIN ON (?i1 ?u1 ?p1 ?f1 ?r1 ?t1) (?i2 ?u2 ?p2 ?f2 ?r2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?u2 ?p2 ?f2 ?r2 ?t2 WHERE {
	?id2 wdt:P2248 ?i2 ;
		wdt:P2244 ?u2 ;
		wdt:P2243 ?p2 ;
		wdt:P2213 ?f2 ;
		wdt:P1096 ?r2 ;
		wdt:P4501 ?t2 .
}
