PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?j1 ?c1 ?z1 ?x1 WHERE {
	?id1 wdt:P2146 ?p1 ;
		wdt:P2213 ?j1 ;
		wdt:P4501 ?c1 ;
		wdt:P2243 ?z1 ;
		wdt:P2325 ?x1 .
}
SIMILARITY JOIN ON (?p1 ?j1 ?c1 ?z1 ?x1) (?p2 ?j2 ?c2 ?z2 ?x2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?j2 ?c2 ?z2 ?x2 WHERE {
	?id2 wdt:P2146 ?p2 ;
		wdt:P2213 ?j2 ;
		wdt:P4501 ?c2 ;
		wdt:P2243 ?z2 ;
		wdt:P2325 ?x2 .
}
