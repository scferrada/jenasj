PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?r1 ?f1 ?m1 ?x1 ?p1 WHERE {
	?id1 wdt:P2146 ?s1 ;
		wdt:P1457 ?r1 ;
		wdt:P2248 ?f1 ;
		wdt:P2244 ?m1 ;
		wdt:P1096 ?x1 ;
		wdt:P4501 ?p1 .
}
SIMILARITY JOIN ON (?s1 ?r1 ?f1 ?m1 ?x1 ?p1) (?s2 ?r2 ?f2 ?m2 ?x2 ?p2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?r2 ?f2 ?m2 ?x2 ?p2 WHERE {
	?id2 wdt:P2146 ?s2 ;
		wdt:P1457 ?r2 ;
		wdt:P2248 ?f2 ;
		wdt:P2244 ?m2 ;
		wdt:P1096 ?x2 ;
		wdt:P4501 ?p2 .
}
