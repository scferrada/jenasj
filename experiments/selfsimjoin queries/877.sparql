PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?z1 ?s1 ?n1 ?l1 WHERE {
	?id1 wdt:P2213 ?z1 ;
		wdt:P2045 ?s1 ;
		wdt:P2233 ?n1 ;
		wdt:P4501 ?l1 .
}
SIMILARITY JOIN ON (?z1 ?s1 ?n1 ?l1) (?z2 ?s2 ?n2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?z2 ?s2 ?n2 ?l2 WHERE {
	?id2 wdt:P2213 ?z2 ;
		wdt:P2045 ?s2 ;
		wdt:P2233 ?n2 ;
		wdt:P4501 ?l2 .
}
