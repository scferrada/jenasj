PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?a1 ?z1 ?t1 WHERE {
	?id1 wdt:P2213 ?a1 ;
		wdt:P1457 ?z1 ;
		wdt:P2243 ?t1 .
}
SIMILARITY JOIN ON (?a1 ?z1 ?t1) (?a2 ?z2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?a2 ?z2 ?t2 WHERE {
	?id2 wdt:P2213 ?a2 ;
		wdt:P1457 ?z2 ;
		wdt:P2243 ?t2 .
}
