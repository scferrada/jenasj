PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?c1 ?o1 ?t1 ?u1 ?n1 WHERE {
	?id1 wdt:P2146 ?c1 ;
		wdt:P2244 ?o1 ;
		wdt:P2045 ?t1 ;
		wdt:P1096 ?u1 ;
		wdt:P2325 ?n1 .
}
SIMILARITY JOIN ON (?c1 ?o1 ?t1 ?u1 ?n1) (?c2 ?o2 ?t2 ?u2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?c2 ?o2 ?t2 ?u2 ?n2 WHERE {
	?id2 wdt:P2146 ?c2 ;
		wdt:P2244 ?o2 ;
		wdt:P2045 ?t2 ;
		wdt:P1096 ?u2 ;
		wdt:P2325 ?n2 .
}
