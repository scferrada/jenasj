PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?u1 ?i1 ?x1 ?k1 ?y1 ?c1 WHERE {
	?id1 wdt:P1457 ?u1 ;
		wdt:P2248 ?i1 ;
		wdt:P2244 ?x1 ;
		wdt:P2243 ?k1 ;
		wdt:P2325 ?y1 ;
		wdt:P2233 ?c1 .
}
SIMILARITY JOIN ON (?u1 ?i1 ?x1 ?k1 ?y1 ?c1) (?u2 ?i2 ?x2 ?k2 ?y2 ?c2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?u2 ?i2 ?x2 ?k2 ?y2 ?c2 WHERE {
	?id2 wdt:P1457 ?u2 ;
		wdt:P2248 ?i2 ;
		wdt:P2244 ?x2 ;
		wdt:P2243 ?k2 ;
		wdt:P2325 ?y2 ;
		wdt:P2233 ?c2 .
}
