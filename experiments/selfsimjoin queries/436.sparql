PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?c1 ?n1 ?i1 ?f1 WHERE {
	?id1 wdt:P2244 ?j1 ;
		wdt:P2325 ?c1 ;
		wdt:P2248 ?n1 ;
		wdt:P2243 ?i1 ;
		wdt:P4501 ?f1 .
}
SIMILARITY JOIN ON (?j1 ?c1 ?n1 ?i1 ?f1) (?j2 ?c2 ?n2 ?i2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?c2 ?n2 ?i2 ?f2 WHERE {
	?id2 wdt:P2244 ?j2 ;
		wdt:P2325 ?c2 ;
		wdt:P2248 ?n2 ;
		wdt:P2243 ?i2 ;
		wdt:P4501 ?f2 .
}
