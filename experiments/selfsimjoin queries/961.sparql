PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?h1 ?m1 ?i1 ?o1 WHERE {
	?id1 wdt:P2146 ?x1 ;
		wdt:P1457 ?h1 ;
		wdt:P2233 ?m1 ;
		wdt:P2248 ?i1 ;
		wdt:P2243 ?o1 .
}
SIMILARITY JOIN ON (?x1 ?h1 ?m1 ?i1 ?o1) (?x2 ?h2 ?m2 ?i2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?h2 ?m2 ?i2 ?o2 WHERE {
	?id2 wdt:P2146 ?x2 ;
		wdt:P1457 ?h2 ;
		wdt:P2233 ?m2 ;
		wdt:P2248 ?i2 ;
		wdt:P2243 ?o2 .
}
