PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?f1 ?j1 ?d1 ?p1 ?m1 ?w1 WHERE {
	?id1 wdt:P2248 ?f1 ;
		wdt:P2244 ?j1 ;
		wdt:P2243 ?d1 ;
		wdt:P2325 ?p1 ;
		wdt:P2233 ?m1 ;
		wdt:P4501 ?w1 .
}
SIMILARITY JOIN ON (?f1 ?j1 ?d1 ?p1 ?m1 ?w1) (?f2 ?j2 ?d2 ?p2 ?m2 ?w2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?f2 ?j2 ?d2 ?p2 ?m2 ?w2 WHERE {
	?id2 wdt:P2248 ?f2 ;
		wdt:P2244 ?j2 ;
		wdt:P2243 ?d2 ;
		wdt:P2325 ?p2 ;
		wdt:P2233 ?m2 ;
		wdt:P4501 ?w2 .
}
