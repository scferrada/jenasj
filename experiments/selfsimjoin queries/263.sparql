PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?b1 ?e1 ?w1 ?v1 ?r1 ?d1 WHERE {
	?id1 wdt:P2154 ?b1 ;
		wdt:P1122 ?e1 ;
		wdt:P2160 ?w1 ;
		wdt:P2114 ?v1 ;
		wdt:P1086 ?r1 ;
		wdt:P1148 ?d1 .
}
SIMILARITY JOIN ON (?b1 ?e1 ?w1 ?v1 ?r1 ?d1) (?b2 ?e2 ?w2 ?v2 ?r2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?b2 ?e2 ?w2 ?v2 ?r2 ?d2 WHERE {
	?id2 wdt:P2154 ?b2 ;
		wdt:P1122 ?e2 ;
		wdt:P2160 ?w2 ;
		wdt:P2114 ?v2 ;
		wdt:P1086 ?r2 ;
		wdt:P1148 ?d2 .
}
