PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?b1 ?o1 ?g1 ?y1 ?w1 ?h1 WHERE {
	?id1 wdt:P2146 ?b1 ;
		wdt:P4501 ?o1 ;
		wdt:P2213 ?g1 ;
		wdt:P2045 ?y1 ;
		wdt:P1096 ?w1 ;
		wdt:P2233 ?h1 .
}
SIMILARITY JOIN ON (?b1 ?o1 ?g1 ?y1 ?w1 ?h1) (?b2 ?o2 ?g2 ?y2 ?w2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?b2 ?o2 ?g2 ?y2 ?w2 ?h2 WHERE {
	?id2 wdt:P2146 ?b2 ;
		wdt:P4501 ?o2 ;
		wdt:P2213 ?g2 ;
		wdt:P2045 ?y2 ;
		wdt:P1096 ?w2 ;
		wdt:P2233 ?h2 .
}
