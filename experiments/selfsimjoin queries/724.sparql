PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?g1 ?r1 ?l1 ?o1 ?n1 WHERE {
	?id1 wdt:P2146 ?g1 ;
		wdt:P2244 ?r1 ;
		wdt:P1457 ?l1 ;
		wdt:P4501 ?o1 ;
		wdt:P2325 ?n1 .
}
SIMILARITY JOIN ON (?g1 ?r1 ?l1 ?o1 ?n1) (?g2 ?r2 ?l2 ?o2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?g2 ?r2 ?l2 ?o2 ?n2 WHERE {
	?id2 wdt:P2146 ?g2 ;
		wdt:P2244 ?r2 ;
		wdt:P1457 ?l2 ;
		wdt:P4501 ?o2 ;
		wdt:P2325 ?n2 .
}
