PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?n1 ?i1 ?u1 ?m1 ?b1 WHERE {
	?id1 wdt:P1457 ?y1 ;
		wdt:P2248 ?n1 ;
		wdt:P2244 ?i1 ;
		wdt:P2243 ?u1 ;
		wdt:P2213 ?m1 ;
		wdt:P2325 ?b1 .
}
SIMILARITY JOIN ON (?y1 ?n1 ?i1 ?u1 ?m1 ?b1) (?y2 ?n2 ?i2 ?u2 ?m2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?n2 ?i2 ?u2 ?m2 ?b2 WHERE {
	?id2 wdt:P1457 ?y2 ;
		wdt:P2248 ?n2 ;
		wdt:P2244 ?i2 ;
		wdt:P2243 ?u2 ;
		wdt:P2213 ?m2 ;
		wdt:P2325 ?b2 .
}
