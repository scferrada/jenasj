PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?c1 ?y1 ?o1 WHERE {
	?id1 wdt:P2146 ?c1 ;
		wdt:P2233 ?y1 ;
		wdt:P2248 ?o1 .
}
SIMILARITY JOIN ON (?c1 ?y1 ?o1) (?c2 ?y2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?c2 ?y2 ?o2 WHERE {
	?id2 wdt:P2146 ?c2 ;
		wdt:P2233 ?y2 ;
		wdt:P2248 ?o2 .
}
