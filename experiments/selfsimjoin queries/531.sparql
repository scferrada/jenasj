PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?f1 ?q1 ?y1 ?u1 WHERE {
	?id1 wdt:P2213 ?f1 ;
		wdt:P2045 ?q1 ;
		wdt:P2248 ?y1 ;
		wdt:P4501 ?u1 .
}
SIMILARITY JOIN ON (?f1 ?q1 ?y1 ?u1) (?f2 ?q2 ?y2 ?u2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?f2 ?q2 ?y2 ?u2 WHERE {
	?id2 wdt:P2213 ?f2 ;
		wdt:P2045 ?q2 ;
		wdt:P2248 ?y2 ;
		wdt:P4501 ?u2 .
}
