PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?h1 ?c1 ?g1 ?o1 WHERE {
	?id1 wdt:P2146 ?j1 ;
		wdt:P1096 ?h1 ;
		wdt:P4501 ?c1 ;
		wdt:P2243 ?g1 ;
		wdt:P2325 ?o1 .
}
SIMILARITY JOIN ON (?j1 ?h1 ?c1 ?g1 ?o1) (?j2 ?h2 ?c2 ?g2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?h2 ?c2 ?g2 ?o2 WHERE {
	?id2 wdt:P2146 ?j2 ;
		wdt:P1096 ?h2 ;
		wdt:P4501 ?c2 ;
		wdt:P2243 ?g2 ;
		wdt:P2325 ?o2 .
}
