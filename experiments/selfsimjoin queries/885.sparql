PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?i1 ?l1 ?m1 ?s1 ?e1 ?v1 WHERE {
	?id1 wdt:P1457 ?i1 ;
		wdt:P2244 ?l1 ;
		wdt:P2045 ?m1 ;
		wdt:P1096 ?s1 ;
		wdt:P2325 ?e1 ;
		wdt:P2233 ?v1 .
}
SIMILARITY JOIN ON (?i1 ?l1 ?m1 ?s1 ?e1 ?v1) (?i2 ?l2 ?m2 ?s2 ?e2 ?v2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?i2 ?l2 ?m2 ?s2 ?e2 ?v2 WHERE {
	?id2 wdt:P1457 ?i2 ;
		wdt:P2244 ?l2 ;
		wdt:P2045 ?m2 ;
		wdt:P1096 ?s2 ;
		wdt:P2325 ?e2 ;
		wdt:P2233 ?v2 .
}
