PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?q1 ?j1 ?w1 ?v1 ?z1 WHERE {
	?id1 wdt:P2146 ?d1 ;
		wdt:P4501 ?q1 ;
		wdt:P2244 ?j1 ;
		wdt:P2243 ?w1 ;
		wdt:P2213 ?v1 ;
		wdt:P2325 ?z1 .
}
SIMILARITY JOIN ON (?d1 ?q1 ?j1 ?w1 ?v1 ?z1) (?d2 ?q2 ?j2 ?w2 ?v2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?q2 ?j2 ?w2 ?v2 ?z2 WHERE {
	?id2 wdt:P2146 ?d2 ;
		wdt:P4501 ?q2 ;
		wdt:P2244 ?j2 ;
		wdt:P2243 ?w2 ;
		wdt:P2213 ?v2 ;
		wdt:P2325 ?z2 .
}
