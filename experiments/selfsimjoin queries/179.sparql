PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?h1 ?q1 ?j1 ?z1 WHERE {
	?id1 wdt:P2244 ?v1 ;
		wdt:P1457 ?h1 ;
		wdt:P1096 ?q1 ;
		wdt:P4501 ?j1 ;
		wdt:P2045 ?z1 .
}
SIMILARITY JOIN ON (?v1 ?h1 ?q1 ?j1 ?z1) (?v2 ?h2 ?q2 ?j2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?h2 ?q2 ?j2 ?z2 WHERE {
	?id2 wdt:P2244 ?v2 ;
		wdt:P1457 ?h2 ;
		wdt:P1096 ?q2 ;
		wdt:P4501 ?j2 ;
		wdt:P2045 ?z2 .
}
