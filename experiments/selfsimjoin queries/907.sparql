PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?u1 ?p1 ?z1 ?t1 ?h1 WHERE {
	?id1 wdt:P1457 ?e1 ;
		wdt:P4501 ?u1 ;
		wdt:P2244 ?p1 ;
		wdt:P2243 ?z1 ;
		wdt:P1096 ?t1 ;
		wdt:P2325 ?h1 .
}
SIMILARITY JOIN ON (?e1 ?u1 ?p1 ?z1 ?t1 ?h1) (?e2 ?u2 ?p2 ?z2 ?t2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?u2 ?p2 ?z2 ?t2 ?h2 WHERE {
	?id2 wdt:P1457 ?e2 ;
		wdt:P4501 ?u2 ;
		wdt:P2244 ?p2 ;
		wdt:P2243 ?z2 ;
		wdt:P1096 ?t2 ;
		wdt:P2325 ?h2 .
}
