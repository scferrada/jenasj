PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?u1 ?w1 ?q1 ?m1 ?p1 WHERE {
	?id1 wdt:P2045 ?u1 ;
		wdt:P1096 ?w1 ;
		wdt:P2233 ?q1 ;
		wdt:P2248 ?m1 ;
		wdt:P2243 ?p1 .
}
SIMILARITY JOIN ON (?u1 ?w1 ?q1 ?m1 ?p1) (?u2 ?w2 ?q2 ?m2 ?p2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?u2 ?w2 ?q2 ?m2 ?p2 WHERE {
	?id2 wdt:P2045 ?u2 ;
		wdt:P1096 ?w2 ;
		wdt:P2233 ?q2 ;
		wdt:P2248 ?m2 ;
		wdt:P2243 ?p2 .
}
