PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?w1 ?e1 ?j1 ?u1 WHERE {
	?id1 wdt:P2213 ?w1 ;
		wdt:P1457 ?e1 ;
		wdt:P2233 ?j1 ;
		wdt:P2248 ?u1 .
}
SIMILARITY JOIN ON (?w1 ?e1 ?j1 ?u1) (?w2 ?e2 ?j2 ?u2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?w2 ?e2 ?j2 ?u2 WHERE {
	?id2 wdt:P2213 ?w2 ;
		wdt:P1457 ?e2 ;
		wdt:P2233 ?j2 ;
		wdt:P2248 ?u2 .
}
