PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?n1 ?d1 ?i1 ?b1 WHERE {
	?id1 wdt:P2213 ?n1 ;
		wdt:P1096 ?d1 ;
		wdt:P2233 ?i1 ;
		wdt:P4501 ?b1 .
}
SIMILARITY JOIN ON (?n1 ?d1 ?i1 ?b1) (?n2 ?d2 ?i2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?n2 ?d2 ?i2 ?b2 WHERE {
	?id2 wdt:P2213 ?n2 ;
		wdt:P1096 ?d2 ;
		wdt:P2233 ?i2 ;
		wdt:P4501 ?b2 .
}
