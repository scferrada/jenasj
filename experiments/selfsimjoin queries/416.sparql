PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?b1 ?v1 ?f1 ?x1 ?n1 WHERE {
	?id1 wdt:P2248 ?b1 ;
		wdt:P2146 ?v1 ;
		wdt:P1457 ?f1 ;
		wdt:P4501 ?x1 ;
		wdt:P2045 ?n1 .
}
SIMILARITY JOIN ON (?b1 ?v1 ?f1 ?x1 ?n1) (?b2 ?v2 ?f2 ?x2 ?n2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?b2 ?v2 ?f2 ?x2 ?n2 WHERE {
	?id2 wdt:P2248 ?b2 ;
		wdt:P2146 ?v2 ;
		wdt:P1457 ?f2 ;
		wdt:P4501 ?x2 ;
		wdt:P2045 ?n2 .
}
