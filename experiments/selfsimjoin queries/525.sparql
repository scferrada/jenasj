PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?v1 ?t1 ?q1 WHERE {
	?id1 wdt:P1457 ?d1 ;
		wdt:P1096 ?v1 ;
		wdt:P2243 ?t1 ;
		wdt:P2325 ?q1 .
}
SIMILARITY JOIN ON (?d1 ?v1 ?t1 ?q1) (?d2 ?v2 ?t2 ?q2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?v2 ?t2 ?q2 WHERE {
	?id2 wdt:P1457 ?d2 ;
		wdt:P1096 ?v2 ;
		wdt:P2243 ?t2 ;
		wdt:P2325 ?q2 .
}
