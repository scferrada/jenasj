PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?n1 ?s1 ?v1 ?z1 ?j1 WHERE {
	?id1 wdt:P2146 ?n1 ;
		wdt:P1096 ?s1 ;
		wdt:P2248 ?v1 ;
		wdt:P2243 ?z1 ;
		wdt:P2325 ?j1 .
}
SIMILARITY JOIN ON (?n1 ?s1 ?v1 ?z1 ?j1) (?n2 ?s2 ?v2 ?z2 ?j2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?n2 ?s2 ?v2 ?z2 ?j2 WHERE {
	?id2 wdt:P2146 ?n2 ;
		wdt:P1096 ?s2 ;
		wdt:P2248 ?v2 ;
		wdt:P2243 ?z2 ;
		wdt:P2325 ?j2 .
}
