PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?o1 ?u1 ?r1 ?z1 WHERE {
	?id1 wdt:P2146 ?h1 ;
		wdt:P1096 ?o1 ;
		wdt:P2233 ?u1 ;
		wdt:P2243 ?r1 ;
		wdt:P2325 ?z1 .
}
SIMILARITY JOIN ON (?h1 ?o1 ?u1 ?r1 ?z1) (?h2 ?o2 ?u2 ?r2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?o2 ?u2 ?r2 ?z2 WHERE {
	?id2 wdt:P2146 ?h2 ;
		wdt:P1096 ?o2 ;
		wdt:P2233 ?u2 ;
		wdt:P2243 ?r2 ;
		wdt:P2325 ?z2 .
}
