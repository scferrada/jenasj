PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?a1 ?k1 ?g1 ?j1 ?z1 WHERE {
	?id1 wdt:P4501 ?t1 ;
		wdt:P2244 ?a1 ;
		wdt:P2213 ?k1 ;
		wdt:P2045 ?g1 ;
		wdt:P1096 ?j1 ;
		wdt:P2325 ?z1 .
}
SIMILARITY JOIN ON (?t1 ?a1 ?k1 ?g1 ?j1 ?z1) (?t2 ?a2 ?k2 ?g2 ?j2 ?z2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?a2 ?k2 ?g2 ?j2 ?z2 WHERE {
	?id2 wdt:P4501 ?t2 ;
		wdt:P2244 ?a2 ;
		wdt:P2213 ?k2 ;
		wdt:P2045 ?g2 ;
		wdt:P1096 ?j2 ;
		wdt:P2325 ?z2 .
}
