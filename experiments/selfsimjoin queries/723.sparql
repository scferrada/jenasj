PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?w1 ?k1 ?g1 ?e1 ?z1 ?d1 WHERE {
	?id1 wdt:P2248 ?w1 ;
		wdt:P2244 ?k1 ;
		wdt:P2243 ?g1 ;
		wdt:P2045 ?e1 ;
		wdt:P1096 ?z1 ;
		wdt:P2325 ?d1 .
}
SIMILARITY JOIN ON (?w1 ?k1 ?g1 ?e1 ?z1 ?d1) (?w2 ?k2 ?g2 ?e2 ?z2 ?d2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?w2 ?k2 ?g2 ?e2 ?z2 ?d2 WHERE {
	?id2 wdt:P2248 ?w2 ;
		wdt:P2244 ?k2 ;
		wdt:P2243 ?g2 ;
		wdt:P2045 ?e2 ;
		wdt:P1096 ?z2 ;
		wdt:P2325 ?d2 .
}
