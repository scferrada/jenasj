PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?u1 ?n1 ?c1 ?l1 WHERE {
	?id1 wdt:P2244 ?u1 ;
		wdt:P1096 ?n1 ;
		wdt:P2233 ?c1 ;
		wdt:P2213 ?l1 .
}
SIMILARITY JOIN ON (?u1 ?n1 ?c1 ?l1) (?u2 ?n2 ?c2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?u2 ?n2 ?c2 ?l2 WHERE {
	?id2 wdt:P2244 ?u2 ;
		wdt:P1096 ?n2 ;
		wdt:P2233 ?c2 ;
		wdt:P2213 ?l2 .
}
