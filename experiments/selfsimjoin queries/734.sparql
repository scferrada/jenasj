PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?z1 ?a1 ?n1 ?u1 ?c1 WHERE {
	?id1 wdt:P2213 ?z1 ;
		wdt:P2045 ?a1 ;
		wdt:P1096 ?n1 ;
		wdt:P2233 ?u1 ;
		wdt:P2248 ?c1 .
}
SIMILARITY JOIN ON (?z1 ?a1 ?n1 ?u1 ?c1) (?z2 ?a2 ?n2 ?u2 ?c2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?z2 ?a2 ?n2 ?u2 ?c2 WHERE {
	?id2 wdt:P2213 ?z2 ;
		wdt:P2045 ?a2 ;
		wdt:P1096 ?n2 ;
		wdt:P2233 ?u2 ;
		wdt:P2248 ?c2 .
}
