PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?e1 ?p1 ?k1 WHERE {
	?id1 wdt:P2045 ?d1 ;
		wdt:P2248 ?e1 ;
		wdt:P2243 ?p1 ;
		wdt:P4501 ?k1 .
}
SIMILARITY JOIN ON (?d1 ?e1 ?p1 ?k1) (?d2 ?e2 ?p2 ?k2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?e2 ?p2 ?k2 WHERE {
	?id2 wdt:P2045 ?d2 ;
		wdt:P2248 ?e2 ;
		wdt:P2243 ?p2 ;
		wdt:P4501 ?k2 .
}
