PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?k1 ?v1 ?n1 ?w1 ?m1 WHERE {
	?id1 wdt:P2244 ?k1 ;
		wdt:P1457 ?v1 ;
		wdt:P2233 ?n1 ;
		wdt:P4501 ?w1 ;
		wdt:P2045 ?m1 .
}
SIMILARITY JOIN ON (?k1 ?v1 ?n1 ?w1 ?m1) (?k2 ?v2 ?n2 ?w2 ?m2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?k2 ?v2 ?n2 ?w2 ?m2 WHERE {
	?id2 wdt:P2244 ?k2 ;
		wdt:P1457 ?v2 ;
		wdt:P2233 ?n2 ;
		wdt:P4501 ?w2 ;
		wdt:P2045 ?m2 .
}
