PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?w1 ?p1 ?t1 ?h1 ?c1 ?x1 WHERE {
	?id1 wdt:P1457 ?w1 ;
		wdt:P2243 ?p1 ;
		wdt:P2045 ?t1 ;
		wdt:P1096 ?h1 ;
		wdt:P2325 ?c1 ;
		wdt:P2233 ?x1 .
}
SIMILARITY JOIN ON (?w1 ?p1 ?t1 ?h1 ?c1 ?x1) (?w2 ?p2 ?t2 ?h2 ?c2 ?x2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?w2 ?p2 ?t2 ?h2 ?c2 ?x2 WHERE {
	?id2 wdt:P1457 ?w2 ;
		wdt:P2243 ?p2 ;
		wdt:P2045 ?t2 ;
		wdt:P1096 ?h2 ;
		wdt:P2325 ?c2 ;
		wdt:P2233 ?x2 .
}
