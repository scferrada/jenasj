PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?a1 ?l1 ?y1 ?m1 ?g1 WHERE {
	?id1 wdt:P2146 ?d1 ;
		wdt:P4501 ?a1 ;
		wdt:P2045 ?l1 ;
		wdt:P1096 ?y1 ;
		wdt:P2325 ?m1 ;
		wdt:P2233 ?g1 .
}
SIMILARITY JOIN ON (?d1 ?a1 ?l1 ?y1 ?m1 ?g1) (?d2 ?a2 ?l2 ?y2 ?m2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?a2 ?l2 ?y2 ?m2 ?g2 WHERE {
	?id2 wdt:P2146 ?d2 ;
		wdt:P4501 ?a2 ;
		wdt:P2045 ?l2 ;
		wdt:P1096 ?y2 ;
		wdt:P2325 ?m2 ;
		wdt:P2233 ?g2 .
}
