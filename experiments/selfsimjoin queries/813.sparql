PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?p1 ?h1 ?j1 ?f1 ?g1 WHERE {
	?id1 wdt:P1457 ?y1 ;
		wdt:P4501 ?p1 ;
		wdt:P2244 ?h1 ;
		wdt:P2243 ?j1 ;
		wdt:P2325 ?f1 ;
		wdt:P2233 ?g1 .
}
SIMILARITY JOIN ON (?y1 ?p1 ?h1 ?j1 ?f1 ?g1) (?y2 ?p2 ?h2 ?j2 ?f2 ?g2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?p2 ?h2 ?j2 ?f2 ?g2 WHERE {
	?id2 wdt:P1457 ?y2 ;
		wdt:P4501 ?p2 ;
		wdt:P2244 ?h2 ;
		wdt:P2243 ?j2 ;
		wdt:P2325 ?f2 ;
		wdt:P2233 ?g2 .
}
