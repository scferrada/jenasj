PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?c1 ?h1 ?t1 WHERE {
	?id1 wdt:P2213 ?p1 ;
		wdt:P1457 ?c1 ;
		wdt:P2233 ?h1 ;
		wdt:P2045 ?t1 .
}
SIMILARITY JOIN ON (?p1 ?c1 ?h1 ?t1) (?p2 ?c2 ?h2 ?t2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?c2 ?h2 ?t2 WHERE {
	?id2 wdt:P2213 ?p2 ;
		wdt:P1457 ?c2 ;
		wdt:P2233 ?h2 ;
		wdt:P2045 ?t2 .
}
