PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?s1 ?n1 ?i1 WHERE {
	?id1 wdt:P2213 ?y1 ;
		wdt:P2045 ?s1 ;
		wdt:P1096 ?n1 ;
		wdt:P2243 ?i1 .
}
SIMILARITY JOIN ON (?y1 ?s1 ?n1 ?i1) (?y2 ?s2 ?n2 ?i2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?s2 ?n2 ?i2 WHERE {
	?id2 wdt:P2213 ?y2 ;
		wdt:P2045 ?s2 ;
		wdt:P1096 ?n2 ;
		wdt:P2243 ?i2 .
}
