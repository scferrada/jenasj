PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?x1 ?z1 ?s1 ?e1 ?h1 ?v1 WHERE {
	?id1 wdt:P1457 ?x1 ;
		wdt:P2248 ?z1 ;
		wdt:P2243 ?s1 ;
		wdt:P2045 ?e1 ;
		wdt:P1096 ?h1 ;
		wdt:P4501 ?v1 .
}
SIMILARITY JOIN ON (?x1 ?z1 ?s1 ?e1 ?h1 ?v1) (?x2 ?z2 ?s2 ?e2 ?h2 ?v2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?x2 ?z2 ?s2 ?e2 ?h2 ?v2 WHERE {
	?id2 wdt:P1457 ?x2 ;
		wdt:P2248 ?z2 ;
		wdt:P2243 ?s2 ;
		wdt:P2045 ?e2 ;
		wdt:P1096 ?h2 ;
		wdt:P4501 ?v2 .
}
