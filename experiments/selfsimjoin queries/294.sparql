PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?t1 ?h1 ?d1 ?j1 ?v1 ?y1 WHERE {
	?id1 wdt:P2248 ?t1 ;
		wdt:P2213 ?h1 ;
		wdt:P1096 ?d1 ;
		wdt:P2325 ?j1 ;
		wdt:P2233 ?v1 ;
		wdt:P4501 ?y1 .
}
SIMILARITY JOIN ON (?t1 ?h1 ?d1 ?j1 ?v1 ?y1) (?t2 ?h2 ?d2 ?j2 ?v2 ?y2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?t2 ?h2 ?d2 ?j2 ?v2 ?y2 WHERE {
	?id2 wdt:P2248 ?t2 ;
		wdt:P2213 ?h2 ;
		wdt:P1096 ?d2 ;
		wdt:P2325 ?j2 ;
		wdt:P2233 ?v2 ;
		wdt:P4501 ?y2 .
}
