PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?q1 ?p1 ?n1 ?c1 ?x1 WHERE {
	?id1 wdt:P2146 ?q1 ;
		wdt:P2244 ?p1 ;
		wdt:P2213 ?n1 ;
		wdt:P4501 ?c1 ;
		wdt:P2325 ?x1 .
}
SIMILARITY JOIN ON (?q1 ?p1 ?n1 ?c1 ?x1) (?q2 ?p2 ?n2 ?c2 ?x2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?q2 ?p2 ?n2 ?c2 ?x2 WHERE {
	?id2 wdt:P2146 ?q2 ;
		wdt:P2244 ?p2 ;
		wdt:P2213 ?n2 ;
		wdt:P4501 ?c2 ;
		wdt:P2325 ?x2 .
}
