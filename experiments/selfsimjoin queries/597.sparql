PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?e1 ?u1 ?g1 ?j1 WHERE {
	?id1 wdt:P2244 ?p1 ;
		wdt:P1096 ?e1 ;
		wdt:P2233 ?u1 ;
		wdt:P2248 ?g1 ;
		wdt:P2243 ?j1 .
}
SIMILARITY JOIN ON (?p1 ?e1 ?u1 ?g1 ?j1) (?p2 ?e2 ?u2 ?g2 ?j2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?e2 ?u2 ?g2 ?j2 WHERE {
	?id2 wdt:P2244 ?p2 ;
		wdt:P1096 ?e2 ;
		wdt:P2233 ?u2 ;
		wdt:P2248 ?g2 ;
		wdt:P2243 ?j2 .
}
