PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?i1 ?b1 WHERE {
	?id1 wdt:P1086 ?h1 ;
		wdt:P1148 ?i1 ;
		wdt:P2160 ?b1 .
}
SIMILARITY JOIN ON (?h1 ?i1 ?b1) (?h2 ?i2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?i2 ?b2 WHERE {
	?id2 wdt:P1086 ?h2 ;
		wdt:P1148 ?i2 ;
		wdt:P2160 ?b2 .
}
