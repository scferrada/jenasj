PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?z1 ?j1 ?q1 ?s1 ?o1 WHERE {
	?id1 wdt:P2213 ?z1 ;
		wdt:P1457 ?j1 ;
		wdt:P2325 ?q1 ;
		wdt:P4501 ?s1 ;
		wdt:P2248 ?o1 .
}
SIMILARITY JOIN ON (?z1 ?j1 ?q1 ?s1 ?o1) (?z2 ?j2 ?q2 ?s2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?z2 ?j2 ?q2 ?s2 ?o2 WHERE {
	?id2 wdt:P2213 ?z2 ;
		wdt:P1457 ?j2 ;
		wdt:P2325 ?q2 ;
		wdt:P4501 ?s2 ;
		wdt:P2248 ?o2 .
}
