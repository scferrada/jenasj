PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?n1 ?d1 ?y1 ?l1 ?b1 WHERE {
	?id1 wdt:P2146 ?n1 ;
		wdt:P1457 ?d1 ;
		wdt:P2233 ?y1 ;
		wdt:P4501 ?l1 ;
		wdt:P2325 ?b1 .
}
SIMILARITY JOIN ON (?n1 ?d1 ?y1 ?l1 ?b1) (?n2 ?d2 ?y2 ?l2 ?b2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?n2 ?d2 ?y2 ?l2 ?b2 WHERE {
	?id2 wdt:P2146 ?n2 ;
		wdt:P1457 ?d2 ;
		wdt:P2233 ?y2 ;
		wdt:P4501 ?l2 ;
		wdt:P2325 ?b2 .
}
