PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?v1 ?d1 ?h1 ?z1 ?t1 ?f1 WHERE {
	?id1 wdt:P2146 ?v1 ;
		wdt:P2248 ?d1 ;
		wdt:P2244 ?h1 ;
		wdt:P2045 ?z1 ;
		wdt:P1096 ?t1 ;
		wdt:P2325 ?f1 .
}
SIMILARITY JOIN ON (?v1 ?d1 ?h1 ?z1 ?t1 ?f1) (?v2 ?d2 ?h2 ?z2 ?t2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?v2 ?d2 ?h2 ?z2 ?t2 ?f2 WHERE {
	?id2 wdt:P2146 ?v2 ;
		wdt:P2248 ?d2 ;
		wdt:P2244 ?h2 ;
		wdt:P2045 ?z2 ;
		wdt:P1096 ?t2 ;
		wdt:P2325 ?f2 .
}
