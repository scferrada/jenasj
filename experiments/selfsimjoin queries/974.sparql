PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?j1 ?p1 ?z1 ?w1 ?m1 WHERE {
	?id1 wdt:P2146 ?j1 ;
		wdt:P2045 ?p1 ;
		wdt:P1096 ?z1 ;
		wdt:P2248 ?w1 ;
		wdt:P4501 ?m1 .
}
SIMILARITY JOIN ON (?j1 ?p1 ?z1 ?w1 ?m1) (?j2 ?p2 ?z2 ?w2 ?m2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?j2 ?p2 ?z2 ?w2 ?m2 WHERE {
	?id2 wdt:P2146 ?j2 ;
		wdt:P2045 ?p2 ;
		wdt:P1096 ?z2 ;
		wdt:P2248 ?w2 ;
		wdt:P4501 ?m2 .
}
