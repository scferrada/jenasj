PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?r1 ?h1 ?b1 ?f1 WHERE {
	?id1 wdt:P2146 ?r1 ;
		wdt:P2233 ?h1 ;
		wdt:P4501 ?b1 ;
		wdt:P2325 ?f1 .
}
SIMILARITY JOIN ON (?r1 ?h1 ?b1 ?f1) (?r2 ?h2 ?b2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?r2 ?h2 ?b2 ?f2 WHERE {
	?id2 wdt:P2146 ?r2 ;
		wdt:P2233 ?h2 ;
		wdt:P4501 ?b2 ;
		wdt:P2325 ?f2 .
}
