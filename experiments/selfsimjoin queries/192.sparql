PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?k1 ?c1 ?n1 ?l1 WHERE {
	?id1 wdt:P2244 ?y1 ;
		wdt:P1096 ?k1 ;
		wdt:P4501 ?c1 ;
		wdt:P2243 ?n1 ;
		wdt:P2325 ?l1 .
}
SIMILARITY JOIN ON (?y1 ?k1 ?c1 ?n1 ?l1) (?y2 ?k2 ?c2 ?n2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?k2 ?c2 ?n2 ?l2 WHERE {
	?id2 wdt:P2244 ?y2 ;
		wdt:P1096 ?k2 ;
		wdt:P4501 ?c2 ;
		wdt:P2243 ?n2 ;
		wdt:P2325 ?l2 .
}
