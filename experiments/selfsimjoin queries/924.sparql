PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?c1 ?i1 ?q1 ?o1 WHERE {
	?id1 wdt:P1148 ?c1 ;
		wdt:P2160 ?i1 ;
		wdt:P2114 ?q1 ;
		wdt:P1122 ?o1 .
}
SIMILARITY JOIN ON (?c1 ?i1 ?q1 ?o1) (?c2 ?i2 ?q2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?c2 ?i2 ?q2 ?o2 WHERE {
	?id2 wdt:P1148 ?c2 ;
		wdt:P2160 ?i2 ;
		wdt:P2114 ?q2 ;
		wdt:P1122 ?o2 .
}
