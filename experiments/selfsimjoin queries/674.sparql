PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?k1 ?c1 ?f1 WHERE {
	?id1 wdt:P2244 ?k1 ;
		wdt:P4501 ?c1 ;
		wdt:P2325 ?f1 .
}
SIMILARITY JOIN ON (?k1 ?c1 ?f1) (?k2 ?c2 ?f2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?k2 ?c2 ?f2 WHERE {
	?id2 wdt:P2244 ?k2 ;
		wdt:P4501 ?c2 ;
		wdt:P2325 ?f2 .
}
