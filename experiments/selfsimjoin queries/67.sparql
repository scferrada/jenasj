PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?e1 ?w1 ?h1 WHERE {
	?id1 wdt:P2146 ?e1 ;
		wdt:P2244 ?w1 ;
		wdt:P2213 ?h1 .
}
SIMILARITY JOIN ON (?e1 ?w1 ?h1) (?e2 ?w2 ?h2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?e2 ?w2 ?h2 WHERE {
	?id2 wdt:P2146 ?e2 ;
		wdt:P2244 ?w2 ;
		wdt:P2213 ?h2 .
}
