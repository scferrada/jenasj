PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?h1 ?e1 ?k1 ?p1 ?l1 WHERE {
	?id1 wdt:P2146 ?h1 ;
		wdt:P2244 ?e1 ;
		wdt:P1457 ?k1 ;
		wdt:P2248 ?p1 ;
		wdt:P2213 ?l1 .
}
SIMILARITY JOIN ON (?h1 ?e1 ?k1 ?p1 ?l1) (?h2 ?e2 ?k2 ?p2 ?l2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?h2 ?e2 ?k2 ?p2 ?l2 WHERE {
	?id2 wdt:P2146 ?h2 ;
		wdt:P2244 ?e2 ;
		wdt:P1457 ?k2 ;
		wdt:P2248 ?p2 ;
		wdt:P2213 ?l2 .
}
