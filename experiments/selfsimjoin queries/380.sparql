PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?s1 ?j1 ?r1 ?o1 WHERE {
	?id1 wdt:P2146 ?s1 ;
		wdt:P2045 ?j1 ;
		wdt:P2233 ?r1 ;
		wdt:P2243 ?o1 .
}
SIMILARITY JOIN ON (?s1 ?j1 ?r1 ?o1) (?s2 ?j2 ?r2 ?o2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?s2 ?j2 ?r2 ?o2 WHERE {
	?id2 wdt:P2146 ?s2 ;
		wdt:P2045 ?j2 ;
		wdt:P2233 ?r2 ;
		wdt:P2243 ?o2 .
}
