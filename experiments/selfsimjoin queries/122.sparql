PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?y1 ?a1 ?i1 ?d1 ?v1 WHERE {
	?id1 wdt:P2244 ?y1 ;
		wdt:P1457 ?a1 ;
		wdt:P1096 ?i1 ;
		wdt:P4501 ?d1 ;
		wdt:P2325 ?v1 .
}
SIMILARITY JOIN ON (?y1 ?a1 ?i1 ?d1 ?v1) (?y2 ?a2 ?i2 ?d2 ?v2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?y2 ?a2 ?i2 ?d2 ?v2 WHERE {
	?id2 wdt:P2244 ?y2 ;
		wdt:P1457 ?a2 ;
		wdt:P1096 ?i2 ;
		wdt:P4501 ?d2 ;
		wdt:P2325 ?v2 .
}
