PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?d1 ?v1 ?w1 ?y1 WHERE {
	?id1 wdt:P1096 ?d1 ;
		wdt:P2233 ?v1 ;
		wdt:P2248 ?w1 ;
		wdt:P4501 ?y1 .
}
SIMILARITY JOIN ON (?d1 ?v1 ?w1 ?y1) (?d2 ?v2 ?w2 ?y2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?d2 ?v2 ?w2 ?y2 WHERE {
	?id2 wdt:P1096 ?d2 ;
		wdt:P2233 ?v2 ;
		wdt:P2248 ?w2 ;
		wdt:P4501 ?y2 .
}
