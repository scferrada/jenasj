PREFIX wdt:<http://www.wikidata.org/prop/direct/>
SELECT ?id1 ?p1 ?i1 ?d1 ?y1 ?u1 WHERE {
	?id1 wdt:P2146 ?p1 ;
		wdt:P1457 ?i1 ;
		wdt:P1096 ?d1 ;
		wdt:P2045 ?y1 ;
		wdt:P2325 ?u1 .
}
SIMILARITY JOIN ON (?p1 ?i1 ?d1 ?y1 ?u1) (?p2 ?i2 ?d2 ?y2 ?u2)
WITH DISTANCE manhattan as ?d
TOP 1
SELECT ?id2 ?p2 ?i2 ?d2 ?y2 ?u2 WHERE {
	?id2 wdt:P2146 ?p2 ;
		wdt:P1457 ?i2 ;
		wdt:P1096 ?d2 ;
		wdt:P2045 ?y2 ;
		wdt:P2325 ?u2 .
}
